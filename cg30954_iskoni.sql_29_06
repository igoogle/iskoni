INSERT INTO `tech_features` VALUES (1,212,'Полный контроль и отбраковка поставляемых пиломатериалов включает в себя проверку геометрических выпусков пиломатериала, а также проверку отсутствия грибных поражений, окрасов, гнили и других дефектов',100),(2,213,'Естественная сушка в течение 2-4 недель (в зависимости от времени года)',200),(3,214,'Камерная сушка в мягком режиме с 3-х кратной пропаркой для исключения внутренних напряжений и трещин в пиломатериале',300),(4,215,'Поштучный, а не выборочный контроль влажности древесины на выходе из сушильной камеры. То есть при производстве домокомплекта мы в прямом смысле проверяем каждую заготовку (бревно или доску, в зависимости от того, из чего вы решите строить ваш дом)',400),(5,216,'Использование экологически чистых материалов при производстве клееного бруса',500),(6,217,'Предпочтительное использование «немецкой» гребенки при изготовлении заготовок клееного бруса. Данная технология очень эффективна с точки зрения теплосбережения и пожаростойкости готового строения',600),(7,218,'Антисептическая обработка материала, что гарантирует отсутствие грибка и патогенных организмов',700),(8,219,'45 дней – срок изготовления домокомплекта «под ключ»',800);
