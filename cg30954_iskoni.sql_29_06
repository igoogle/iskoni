INSERT INTO `feature` VALUES (1,'Собственное производство',150,500,'Контроль качества изготовления домокомплекта на всех этапах.'),(2,'Уникальные  технологии',151,200,'Благодаря собственным запатентованным и немецким технологиям наши дома в 3 раза теплее, тише и легче аналогов.'),(3,'Экологичность',152,700,'Девиз нашей компании. При производстве домов из клееного бруса на всех этапах работ мы используем только экологически чистые материалы с сертификатами соответствия.'),(4,'Расширенная гарантия',153,400,'5 лет гарантии. Расширенная гарантия соответствия дома проектной документации на весь срок эксплуатации.'),(5,'Online 24 часа',154,800,'Возможность наблюдать за строительством online 24 часа в сутки.'),(6,'Выгодная цена',149,100,'Стоимость наших домокомплектов из клееного бруса на 30% ниже, чем у конкурентов. Что приводит к 10% экономии стоимости дома.'),(7,'Быстро и качественно',174,300,'Строим дома как для себя за 60 дней под ключ.'),(8,'Проектное бюро',175,600,'Собственное проектное бюро позволит вам воплотить любое желание и создать дом вашей мечты.'),(9,'Подарок на новоселье',176,900,'');
