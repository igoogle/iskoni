INSERT INTO `b_search_content_text` VALUES (105,'626a5d1bd18fcc622fbe4e4e3619a88a','НОВОСТИ\r\n\r\n'),(21,'06cecc23c5cc18e8d1e0166639dc5c25','АВТОРИЗАЦИЯ\r\nВЫ ЗАРЕГИСТРИРОВАНЫ И УСПЕШНО АВТОРИЗОВАЛИСЬ.\rИСПОЛЬЗУЙТЕ АДМИНИСТРАТИВНУЮ ПАНЕЛЬ В ВЕРХНЕЙ ЧАСТИ ЭКРАНА ДЛЯ БЫСТРОГО ДОСТУПА К ФУНКЦИЯМ УПРАВЛЕНИЯ СТРУКТУРОЙ И ИНФОРМАЦИОННЫМ НАПОЛНЕНИЕМ САЙТА. НАБОР КНОПОК ВЕРХНЕЙ ПАНЕЛИ ОТЛИЧАЕТСЯ ДЛЯ РАЗЛИЧНЫХ РАЗДЕЛОВ САЙТА. ТАК ОТДЕЛЬНЫЕ НАБОРЫ ДЕЙСТВИЙ ПРЕДУСМОТРЕНЫ ДЛЯ УПРАВЛЕНИЯ СТАТИЧЕСКИМ СОДЕРЖИМЫМ СТРАНИЦ, ДИНАМИЧЕСКИМИ ПУБЛИКАЦИЯМИ (НОВОСТЯМИ, КАТАЛОГОМ, ФОТОГАЛЕРЕЕЙ) И Т.П.\rВЕРНУТЬСЯ НА ГЛАВНУЮ СТРАНИЦУ\r\n'),(67,'79efec6785774f3a901814de746592f7','УДАЧА ПЛЮС\r\n\r\n'),(55,'5bd236486d7fced78af5183eaa104c06','ВЕЛИЙ\r\n\r\n'),(34,'5bb1e0b89ff2e453fde3f59400919990','КВЕЗАЛЬ\r\n\r\n'),(35,'adfe3194c5d7e88a8bd2e7b4adf91423','ЛОРИ\r\n\r\n'),(36,'31bd4c3153fb2d090f0d7ca30060a83d','АКАЦИЯ\r\n\r\n'),(37,'408504e2ed206dcf0b075b75e97fe52f','МИНДАЛЬ\r\n\r\n'),(62,'ad15e73a96d05962f00450f295ba2bb4','ИНДИВИДУАЛЬНЫЙ ПРОЕКТ ДОМА. РЕСПУБЛИКА ТАТАРСТАН\r\n2 ЭТАЖА, 220 КВ М. ДОПОЛНИТЕЛЬНО ИЗ КЛЕЕНОГО БРУСА БЫЛИ ПОСТРОЕНЫ БАНЯ И ЛЕТНЯЯ КУХНЯ.\r\n'),(40,'157d36310ce29985065c99c7c2518bc7','ЮНКО\r\n\r\n'),(41,'e1d327464c4bb0033609fc953d831083','КЛЕСТ\r\n\r\n'),(42,'6bbe3b9d5b3eaa53811526f5a6408a0a','СНЕГИРЬ\r\n\r\n'),(43,'c7029b9e84c8b92c0ca2c3bdfb83d671','АГАМИ\r\n\r\n'),(44,'fd587e0ee7e6872ee3ab0fd54fc60d85','КОЛИБРИ\r\n\r\n'),(45,'6a04ad5613b223a722465f951e5a6fda','ПОЛИТИКА КОНФИДЕНЦИАЛЬНОСТИ\r\nПОЛИТИКА КОНФИДЕНЦИАЛЬНОСТИ\rLOREM IPSUM DOLOR SIT AMET, CONSECTETUR ADIPISICING ELIT. PARIATUR CULPA, AMET EVENIET MOLLITIA! ALIQUAM DOLOREMQUE IPSA ENIM ULLAM PORRO UT NUMQUAM MAGNAM EA, MAGNI ERROR NESCIUNT CUM MOLESTIAE MINUS ACCUSANTIUM!\r\n'),(46,'c3634fdbcbd21fec0d2f492f9ebda16a','ПОЛИТИКА КОНФИДЕНЦИАЛЬНОСТИ\r\nПОЛИТИКА КОНФИДЕНЦИАЛЬНОСТИ\rПОЛИТИКА ООО «ИСКОНИ» В ОТНОШЕНИИ ОБРАБОТКИ ПЕРСОНАЛЬНЫХ ДАННЫХ \rЮРИДИЧЕСКИЙ АДРЕС ООО «ИСКОНИ»: 107078, Г. МОСКВА, 1ЫЙ БАСМАННЫЙ ПЕРЕУЛОК, 5/20, СТР 2, ОФ. 35.\r1. НАЗНАЧЕНИЕ И ОБЛАСТЬ ДЕЙСТВИЯ ДОКУМЕНТА \r1. «ПОЛИТИКА ООО «ИСКОНИ» (ДАЛЕЕ ПО ТЕКСТУ ТАКЖЕ - ОБЩЕСТВО) В ОТНОШЕНИИ ОБРАБОТКИ ПЕРСОНАЛЬНЫХ ДАННЫХ» (ДАЛЕЕ - ПОЛИТИКА) ОПРЕДЕЛЯЕТ ПОЗИЦИЮ И НАМЕРЕНИЯ ОБЩЕСТВА В ОБЛАСТИ ОБРАБОТКИ И ЗАЩИТЫ ПЕРСОНАЛЬНЫХ ДАННЫХ, С ЦЕЛЬЮ СОБЛЮДЕНИЯ И ЗАЩИТЫ ПРАВ И СВОБОД КАЖДОГО ЧЕЛОВЕКА И, В ОСОБЕННОСТИ, ПРАВА НА НЕПРИКОСНОВЕННОСТЬ ЧАСТНОЙ ЖИЗНИ, ЛИЧНУЮ И СЕМЕЙНУЮ ТАЙНУ, ЗАЩИТУ СВОЕЙ ЧЕСТИ И ДОБРОГО ИМЕНИ. \r2. ПОЛИТИКА НЕУКОСНИТЕЛЬНО ИСПОЛНЯЕТСЯ РУКОВОДИТЕЛЯМИ И РАБОТНИКАМИ ВСЕХ СТРУКТУРНЫХ ПОДРАЗДЕЛЕНИЙ И ФИЛИАЛОВ ООО «ИСКОНИ». \r3. ДЕЙСТВИЕ ПОЛИТИКИ РАСПРОСТРАНЯЕТСЯ НА ВСЕ ПЕРСОНАЛЬНЫЕ ДАННЫЕ СУБЪЕКТОВ, ОБРАБАТЫВАЕМЫЕ В ОБЩЕСТВЕ С ПРИМЕНЕНИЕМ СРЕДСТВ АВТОМАТИЗАЦИИ И БЕЗ ПРИМЕНЕНИЯ ТАКИХ СРЕДСТВ. \r4. К НАСТОЯЩЕЙ ПОЛИТИКЕ ИМЕЕТ ДОСТУП ЛЮБОЙ СУБЪЕКТ ПЕРСОНАЛЬНЫХ ДАННЫХ. \r2. ОПРЕДЕЛЕНИЯ \r1. ПЕРСОНАЛЬНЫЕ ДАННЫЕ - ЛЮБАЯ ИНФОРМАЦИЯ, ОТНОСЯЩАЯСЯ К ПРЯМО ИЛИ КОСВЕННО ОПРЕДЕЛЕННОМУ ИЛИ ОПРЕДЕЛЯЕМОМУ ФИЗИЧЕСКОМУ ЛИЦУ (ГРАЖДАНИНУ). Т.Е. К ТАКОЙ ИНФОРМАЦИИ, В ЧАСТНОСТИ, МОЖНО ОТНЕСТИ: ФИО, ГОД, МЕСЯЦ, ДАТА И МЕСТО РОЖДЕНИЯ, АДРЕС, СВЕДЕНИЯ О СЕМЕЙНОМ, СОЦИАЛЬНОМ, ИМУЩЕСТВЕННОМ ПОЛОЖЕНИИ, СВЕДЕНИЯ ОБ ОБРАЗОВАНИИ, ПРОФЕССИИ, ДОХОДАХ, СВЕДЕНИЯ О СОСТОЯНИИ ЗДОРОВЬЯ, А ТАКЖЕ ДРУГУЮ ИНФОРМАЦИЮ. \r2. ОБРАБОТКА ПЕРСОНАЛЬНЫХ ДАННЫХ - ЛЮБОЕ ДЕЙСТВИЕ (ОПЕРАЦИЯ) ИЛИ СОВОКУПНОСТЬ ДЕЙСТВИЙ (ОПЕРАЦИЙ) С ПЕРСОНАЛЬНЫМИ ДАННЫМ, СОВЕРШАЕМЫХ С ИСПОЛЬЗОВАНИЕМ СРЕДСТВ АВТОМАТИЗАЦИИ ИЛИ БЕЗ ИСПОЛЬЗОВАНИЯ ТАКИХ СРЕДСТВ. К ТАКИМ ДЕЙСТВИЯМ (ОПЕРАЦИЯМ) МОЖНО ОТНЕСТИ: СБОР, ПОЛУЧЕНИЕ, ЗАПИСЬ, СИСТЕМАТИЗАЦИЮ, НАКОПЛЕНИЕ, ХРАНЕНИЕ, УТОЧНЕНИЕ (ОБНОВЛЕНИЕ, ИЗМЕНЕНИЕ), ИЗВЛЕЧЕНИЕ, ИСПОЛЬЗОВАНИЕ, ПЕРЕДАЧУ (РАСПРОСТРАНЕНИЕ, ПРЕДОСТАВЛЕНИЕ, ДОСТУП), ОБЕЗЛИЧИВАНИЕ, БЛОКИРОВАНИЕ, УДАЛЕНИЕ, УНИЧТОЖЕНИЕ ПЕРСОНАЛЬНЫХ ДАННЫХ. \r3. СУБЪЕКТЫ ПЕРСОНАЛЬНЫХ ДАННЫХ ООО «ИСКОНИ» ОБРАБАТЫВАЕТ ПЕРСОНАЛЬНЫЕ ДАННЫЕ СЛЕДУЮЩИХ ЛИЦ: \r• РАБОТНИКОВ ООО «ИСКОНИ»; \r• СУБЪЕКТОВ, С КОТОРЫМИ ЗАКЛЮЧЕНЫ ДОГОВОРЫ ГРАЖДАНСКО-ПРАВОВОГО ХАРАКТЕРА; \r• КАНДИДАТОВ НА ЗАМЕЩЕНИЕ ВАКАНТНЫХ ДОЛЖНОСТЕЙ ООО «ИСКОНИ»; \r• КЛИЕНТОВ ООО «ИСКОНИ»; \r• ЗАРЕГИСТРИРОВАННЫХ ПОЛЬЗОВАТЕЛЕЙ САЙТА ООО «ИСКОНИ»» \r• ПРЕДСТАВИТЕЛЕЙ ЮРИДИЧЕСКИХ ЛИЦ; \r• ПОСТАВЩИКОВ (ИНДИВИДУАЛЬНЫХ ПРЕДПРИНИМАТЕЛЕЙ). \r4. ПРИНЦИПЫ И УСЛОВИЯ ОБРАБОТКИ ПЕРСОНАЛЬНЫХ ДАННЫХ \r1. ПОД БЕЗОПАСНОСТЬЮ ПЕРСОНАЛЬНЫХ ДАННЫХ ООО «ИСКОНИ» ПОНИМАЕТ ЗАЩИЩЕННОСТЬ ПЕРСОНАЛЬНЫХ ДАННЫХ ОТ НЕПРАВОМЕРНОГО ИЛИ СЛУЧАЙНОГО ДОСТУПА К НИМ, УНИЧТОЖЕНИЯ, ИЗМЕНЕНИЯ, БЛОКИРОВАНИЯ, КОПИРОВАНИЯ, ПРЕДОСТАВЛЕНИЯ, РАСПРОСТРАНЕНИЯ ПЕРСОНАЛЬНЫХ ДАННЫХ, А ТАКЖЕ ОТ ИНЫХ НЕПРАВОМЕРНЫХ ДЕЙСТВИЙ В ОТНОШЕНИИ ПЕРСОНАЛЬНЫХ ДАННЫХ И ПРИНИМАЕТ НЕОБХОДИМЫЕ ПРАВОВЫЕ, ОРГАНИЗАЦИОННЫЕ И ТЕХНИЧЕСКИЕ МЕРЫ ДЛЯ ЗАЩИТЫ ПЕРСОНАЛЬНЫХ ДАННЫХ. \r2. ОБРАБОТКА И ОБЕСПЕЧЕНИЕ БЕЗОПАСНОСТИ ПЕРСОНАЛЬНЫХ ДАННЫХ В ООО «ИСКОНИ» ОСУЩЕСТВЛЯЕТСЯ В СООТВЕТСТВИИ С ТРЕБОВАНИЯМИ КОНСТИТУЦИИ РОССИЙСКОЙ ФЕДЕРАЦИИ, ФЕДЕРАЛЬНОГО ЗАКОНА № 152-ФЗ «О ПЕРСОНАЛЬНЫХ ДАННЫХ», ПОДЗАКОННЫХ АКТОВ, ДРУГИХ ОПРЕДЕЛЯЮЩИХ СЛУЧАИ И ОСОБЕННОСТИ ОБРАБОТКИ ПЕРСОНАЛЬНЫХ ДАННЫХ ФЕДЕРАЛЬНЫХ ЗАКОНОВ РОССИЙСКОЙ ФЕДЕРАЦИИ, РУКОВОДЯЩИХ И МЕТОДИЧЕСКИХ ДОКУМЕНТОВ ФСТЭК РОССИИ И ФСБ РОССИИ. \r3. ПРИ ОБРАБОТКЕ ПЕРСОНАЛЬНЫХ ДАННЫХ ООО «ИСКОНИ» ПРИДЕРЖИВАЕТСЯ СЛЕДУЮЩИХ ПРИНЦИПОВ: \r• ЗАКОННОСТИ И СПРАВЕДЛИВОЙ ОСНОВЫ; \r• ОГРАНИЧЕНИЯ ОБРАБОТКИ ПЕРСОНАЛЬНЫХ ДАННЫХ ДОСТИЖЕНИЕМ КОНКРЕТНЫХ, ЗАРАНЕЕ ОПРЕДЕЛЕННЫХ И ЗАКОННЫХ ЦЕЛЕЙ; \r• НЕДОПУЩЕНИЯ ОБРАБОТКИ ПЕРСОНАЛЬНЫХ ДАННЫХ, НЕСОВМЕСТИМОЙ С ЦЕЛЯМИ СБОРА ПЕРСОНАЛЬНЫХ ДАННЫХ; \r• НЕДОПУЩЕНИЯ ОБЪЕДИНЕНИЯ БАЗ ДАННЫХ, СОДЕРЖАЩИХ ПЕРСОНАЛЬНЫЕ ДАННЫЕ, ОБРАБОТКА КОТОРЫХ ОСУЩЕСТВЛЯЕТСЯ В ЦЕЛЯХ, НЕСОВМЕСТИМЫХ МЕЖДУ СОБОЙ; \r• ОБРАБОТКИ ПЕРСОНАЛЬНЫХ ДАННЫХ, КОТОРЫЕ ОТВЕЧАЮТ ЦЕЛЯМ ИХ ОБРАБОТКИ; \r• СООТВЕТСТВИЯ СОДЕРЖАНИЯ. \r4. ОБЩЕСТВО ОБРАБАТЫВАЕТ ПЕРСОНАЛЬНЫЕ ДАННЫЕ ТОЛЬКО ПРИ НАЛИЧИИ ХОТЯ БЫ ОДНОГО ИЗ СЛЕДУЮЩИХ УСЛОВИЙ: \r• ОБРАБОТКА ПЕРСОНАЛЬНЫХ ДАННЫХ ОСУЩЕСТВЛЯЕТСЯ С СОГЛАСИЯ СУБЪЕКТА ПЕРСОНАЛЬНЫХ ДАННЫХ НА ОБРАБОТКУ ЕГО ПЕРСОНАЛЬНЫХ ДАННЫХ; \r• ОБРАБОТКА ПЕРСОНАЛЬНЫХ ДАННЫХ НЕОБХОДИМА ДЛЯ ДОСТИЖЕНИЯ ЦЕЛЕЙ, ПРЕДУСМОТРЕННЫХ ЗАКОНОМ, ДЛЯ ОСУЩЕСТВЛЕНИЯ И ВЫПОЛНЕНИЯ ВОЗЛОЖЕННЫХ ЗАКОНОДАТЕЛЬСТВОМ РОССИЙСКОЙ ФЕДЕРАЦИИ НА ОПЕРАТОРА ФУНКЦИЙ, ПОЛНОМОЧИЙ И ОБЯЗАННОСТЕЙ; \r• ОБРАБОТКА ПЕРСОНАЛЬНЫХ ДАННЫХ НЕОБХОДИМА ДЛЯ ИСПОЛНЕНИЯ ДОГОВОРА, СТОРОНОЙ КОТОРОГО ЛИБО ВЫГОДОПРИОБРЕТАТЕЛЕМ ИЛИ ПОРУЧИТЕЛЕМ ПО КОТОРОМУ ЯВЛЯЕТСЯ СУБЪЕКТ ПЕРСОНАЛЬНЫХ ДАННЫХ, А ТАКЖЕ ДЛЯ ЗАКЛЮЧЕНИЯ ДОГОВОРА ПО ИНИЦИАТИВЕ СУБЪЕКТА ПЕРСОНАЛЬНЫХ ДАННЫХ ИЛИ ДОГОВОРА, ПО КОТОРОМУ СУБЪЕКТ ПЕРСОНАЛЬНЫХ ДАННЫХ БУДЕТ ЯВЛЯТЬСЯ ВЫГОДОПРИОБРЕТАТЕЛЕМ ИЛИ ПОРУЧИТЕЛЕМ; \r• ОБРАБОТКА ПЕРСОНАЛЬНЫХ ДАННЫХ НЕОБХОДИМА ДЛЯ ОСУЩЕСТВЛЕНИЯ ПРАВ И ЗАКОННЫХ ИНТЕРЕСОВ ОБЩЕСТВА ИЛИ ТРЕТЬИХ ЛИЦ ЛИБО ДЛЯ ДОСТИЖЕНИЯ ОБЩЕСТВЕННО ЗНАЧИМЫХ ЦЕЛЕЙ ПРИ УСЛОВИИ, ЧТО ПРИ ЭТОМ НЕ НАРУШАЮТСЯ ПРАВА И СВОБОДЫ СУБЪЕКТА ПЕРСОНАЛЬНЫХ ДАННЫХ; \r• ОСУЩЕСТВЛЯЕТСЯ ОБРАБОТКА ПЕРСОНАЛЬНЫХ ДАННЫХ, ДОСТУП НЕОГРАНИЧЕННОГО КРУГА ЛИЦ К КОТОРЫМ ПРЕДОСТАВЛЕН СУБЪЕКТОМ ПЕРСОНАЛЬНЫХ ДАННЫХ ЛИБО ПО ЕГО ПРОСЬБЕ; \r• ОСУЩЕСТВЛЯЕТСЯ ОБРАБОТКА ПЕРСОНАЛЬНЫХ ДАННЫХ, ПОДЛЕЖАЩИХ ОПУБЛИКОВАНИЮ ИЛИ ОБЯЗАТЕЛЬНОМУ РАСКРЫТИЮ В СООТВЕТСТВИИ С ФЕДЕРАЛЬНЫМ ЗАКОНОМ. \r4. ООО «ИСКОНИ» ВПРАВЕ ПОРУЧИТЬ ОБРАБОТКУ ПЕРСОНАЛЬНЫХ ДАННЫХ ГРАЖДАН ТРЕТЬИМ ЛИЦАМ, НА ОСНОВАНИИ ЗАКЛЮЧАЕМОГО С ЭТИМИ ЛИЦАМИ ДОГОВОРА. \rЛИЦА, ОСУЩЕСТВЛЯЮЩИЕ ОБРАБОТКУ ПЕРСОНАЛЬНЫХ ДАННЫХ ПО ПОРУЧЕНИЮ ООО «ИСКОНИ», ОБЯЗУЮТСЯ СОБЛЮДАТЬ ПРИНЦИПЫ И ПРАВИЛА ОБРАБОТКИ И ЗАЩИТЫ ПЕРСОНАЛЬНЫХ ДАННЫХ, ПРЕДУСМОТРЕННЫЕ ФЕДЕРАЛЬНЫМ ЗАКОНОМ № 152-ФЗ «О ПЕРСОНАЛЬНЫХ ДАННЫХ». ДЛЯ КАЖДОГО ЛИЦА ОПРЕДЕЛЕНЫ ПЕРЕЧЕНЬ ДЕЙСТВИЙ (ОПЕРАЦИЙ) С ПЕРСОНАЛЬНЫМИ ДАННЫМИ, КОТОРЫЕ БУДУТ СОВЕРШАТЬСЯ ЮРИДИЧЕСКИМ ЛИЦОМ, ОСУЩЕСТВЛЯЮЩИМ ОБРАБОТКУ ПЕРСОНАЛЬНЫХ ДАННЫХ, ЦЕЛИ ОБРАБОТКИ, УСТАНОВЛЕНА ОБЯЗАННОСТЬ ТАКОГО ЛИЦА СОБЛЮДАТЬ КОНФИДЕНЦИАЛЬНОСТЬ И ОБЕСПЕЧИВАТЬ БЕЗОПАСНОСТЬ ПЕРСОНАЛЬНЫХ ДАННЫХ ПРИ ИХ ОБРАБОТКЕ, А ТАКЖЕ УКАЗАНЫ ТРЕБОВАНИЯ К ЗАЩИТЕ ОБРАБАТЫВАЕМЫХ ПЕРСОНАЛЬНЫХ ДАННЫХ. \r5. В СЛУЧАЯХ, УСТАНОВЛЕННЫХ ЗАКОНОДАТЕЛЬСТВОМ РОССИЙСКОЙ ФЕДЕРАЦИИ, ООО «ИСКОНИ» ВПРАВЕ ОСУЩЕСТВЛЯТЬ ПЕРЕДАЧУ ПЕРСОНАЛЬНЫХ ДАННЫХ ГРАЖДАН. \r6. В ЦЕЛЯХ ИНФОРМАЦИОННОГО ОБЕСПЕЧЕНИЯ В ОБЩЕСТВА МОГУТ СОЗДАВАТЬСЯ ОБЩЕДОСТУПНЫЕ ИСТОЧНИКИ ПЕРСОНАЛЬНЫХ ДАННЫХ РАБОТНИКОВ, В ТОМ ЧИСЛЕ СПРАВОЧНИКИ И АДРЕСНЫЕ КНИГИ. В ОБЩЕДОСТУПНЫЕ ИСТОЧНИКИ ПЕРСОНАЛЬНЫХ ДАННЫХ С СОГЛАСИЯ РАБОТНИКА МОГУТ ВКЛЮЧАТЬСЯ ЕГО ФАМИЛИЯ, ИМЯ, ОТЧЕСТВО, ДАТА И МЕСТО РОЖДЕНИЯ, ДОЛЖНОСТЬ, НОМЕРА КОНТАКТНЫХ ТЕЛЕФОНОВ, АДРЕС ЭЛЕКТРОННОЙ ПОЧТЫ. СВЕДЕНИЯ О РАБОТНИКЕ ДОЛЖНЫ БЫТЬ В ЛЮБОЕ ВРЕМЯ ИСКЛЮЧЕНЫ ИЗ ОБЩЕДОСТУПНЫХ ИСТОЧНИКОВ ПЕРСОНАЛЬНЫХ ДАННЫХ ПО ТРЕБОВАНИЮ РАБОТНИКА ЛИБО ПО РЕШЕНИЮ СУДА ИЛИ ИНЫХ УПОЛНОМОЧЕННЫХ ГОСУДАРСТВЕННЫХ ОРГАНОВ. \r7. ОБЩЕСТВО УНИЧТОЖАЕТ ЛИБО ОБЕЗЛИЧИВАЕТ ПЕРСОНАЛЬНЫЕ ДАННЫЕ ПО ДОСТИЖЕНИИ ЦЕЛЕЙ ОБРАБОТКИ ИЛИ В СЛУЧАЕ УТРАТЫ НЕОБХОДИМОСТИ ДОСТИЖЕНИЯ ЦЕЛИ ОБРАБОТКИ. \r5. ПРАВА СУБЪЕКТА ПЕРСОНАЛЬНЫХ ДАННЫХ \rГРАЖДАНИН, ПЕРСОНАЛЬНЫЕ ДАННЫЕ КОТОРОГО ОБРАБАТЫВАЮТСЯ ООО «ИСКОНИ», ИМЕЕТ ПРАВО: \r• ПОЛУЧАТЬ ОТ ООО «ИСКОНИ»: \r• ПОДТВЕРЖДЕНИЕ ФАКТА ОБРАБОТКИ ПЕРСОНАЛЬНЫХ ДАННЫХ ООО «ИСКОНИ»; \r• ПРАВОВЫЕ ОСНОВАНИЯ И ЦЕЛИ ОБРАБОТКИ ПЕРСОНАЛЬНЫХ ДАННЫХ; \r• СВЕДЕНИЯ О ПРИМЕНЯЕМЫХ ООО «ИСКОНИ» СПОСОБАХ ОБРАБОТКИ ПЕРСОНАЛЬНЫХ ДАННЫХ; \r• НАИМЕНОВАНИЕ И МЕСТОНАХОЖДЕНИЯ ООО «ИСКОНИ»; \r• СВЕДЕНИЯ О ЛИЦАХ, КОТОРЫЕ ИМЕЮТ ДОСТУП К ПЕРСОНАЛЬНЫМ ДАННЫМ ИЛИ КОТОРЫМ МОГУТ БЫТЬ РАСКРЫТЫ ПЕРСОНАЛЬНЫЕ ДАННЫЕ НА ОСНОВАНИИ ДОГОВОРА С ООО «ИСКОНИ» ИЛИ НА ОСНОВАНИИ ФЕДЕРАЛЬНОГО ЗАКОНА; \r• ПЕРЕЧЕНЬ ОБРАБАТЫВАЕМЫХ ПЕРСОНАЛЬНЫХ ДАННЫХ, ОТНОСЯЩИХСЯ К ГРАЖДАНИНУ, ОТ КОТОРОГО ПОСТУПИЛ ЗАПРОС И ИСТОЧНИК ИХ ПОЛУЧЕНИЯ, ЕСЛИ ИНОЙ ПОРЯДОК ПРЕДОСТАВЛЕНИЯ ТАКИХ ДАННЫХ НЕ ПРЕДУСМОТРЕН ФЕДЕРАЛЬНЫМ ЗАКОНОМ; \r• СВЕДЕНИЯ О СРОКАХ ОБРАБОТКИ ПЕРСОНАЛЬНЫХ ДАННЫХ, В ТОМ ЧИСЛЕ О СРОКАХ ИХ ХРАНЕНИЯ; \r• СВЕДЕНИЯ О ПОРЯДКЕ ОСУЩЕСТВЛЕНИЯ ГРАЖДАНИНОМ ПРАВ, ПРЕДУСМОТРЕННЫХ ФЕДЕРАЛЬНЫМ ЗАКОНОМ «О ПЕРСОНАЛЬНЫХ ДАННЫХ» № 152-ФЗ; \r• ИНФОРМАЦИЮ ОБ ОСУЩЕСТВЛЯЕМОЙ ИЛИ О ПРЕДПОЛАГАЕМОЙ ТРАНСГРАНИЧНОЙ ПЕРЕДАЧЕ ПЕРСОНАЛЬНЫХ ДАННЫХ; \r• НАИМЕНОВАНИЕ И АДРЕС ЛИЦА, ОСУЩЕСТВЛЯЮЩЕГО ОБРАБОТКУ ПЕРСОНАЛЬНЫХ ДАННЫХ ПО ПОРУЧЕНИЮ ООО «ИСКОНИ»; \r• ИНЫЕ СВЕДЕНИЯ, ПРЕДУСМОТРЕННЫЕ ФЕДЕРАЛЬНЫМ ЗАКОНОМ «О ПЕРСОНАЛЬНЫХ ДАННЫХ» № 152-ФЗ ИЛИ ДРУГИМИ ФЕДЕРАЛЬНЫМИ ЗАКОНАМИ; \r• ТРЕБОВАТЬ УТОЧНЕНИЯ СВОИХ ПЕРСОНАЛЬНЫХ ДАННЫХ, ИХ БЛОКИРОВАНИЯ ИЛИ УНИЧТОЖЕНИЯ В СЛУЧАЕ, ЕСЛИ ПЕРСОНАЛЬНЫЕ ДАННЫЕ ЯВЛЯЮТСЯ НЕПОЛНЫМИ, УСТАРЕВШИМИ, НЕТОЧНЫМИ, НЕЗАКОННО ПОЛУЧЕННЫМИ ИЛИ НЕ ЯВЛЯЮТСЯ НЕОБХОДИМЫМИ ДЛЯ ЗАЯВЛЕННОЙ ЦЕЛИ ОБРАБОТКИ; \r• ОТОЗВАТЬ СВОЕ СОГЛАСИЕ НА ОБРАБОТКУ ПЕРСОНАЛЬНЫХ ДАННЫХ; \r• ТРЕБОВАТЬ УСТРАНЕНИЯ НЕПРАВОМЕРНЫХ ДЕЙСТВИЙ ООО «ИСКОНИ» В ОТНОШЕНИИ ЕГО ПЕРСОНАЛЬНЫХ ДАННЫХ; \r• ОБЖАЛОВАТЬ ДЕЙСТВИЯ ИЛИ БЕЗДЕЙСТВИЕ ООО «ИСКОНИ» В ФЕДЕРАЛЬНУЮ СЛУЖБУ ПО НАДЗОРУ В СФЕРЕ СВЯЗИ, ИНФОРМАЦИОННЫХ ТЕХНОЛОГИЙ И МАССОВЫХ КОММУНИКАЦИЙ (РОСКОМНАДЗОР) ИЛИ В СУДЕБНОМ ПОРЯДКЕ В СЛУЧАЕ, ЕСЛИ ГРАЖДАНИН СЧИТАЕТ, ЧТО ООО «ИСКОНИ» ОСУЩЕСТВЛЯЕТ ОБРАБОТКУ ЕГО ПЕРСОНАЛЬНЫХ ДАННЫХ С НАРУШЕНИЕМ ТРЕБОВАНИЙ ФЕДЕРАЛЬНОГО ЗАКОНА № 152-ФЗ «О ПЕРСОНАЛЬНЫХ ДАННЫХ» ИЛИ ИНЫМ ОБРАЗОМ НАРУШАЕТ ЕГО ПРАВА И СВОБОДЫ; \r• НА ЗАЩИТУ СВОИХ ПРАВ И ЗАКОННЫХ ИНТЕРЕСОВ, В ТОМ ЧИСЛЕ НА ВОЗМЕЩЕНИЕ УБЫТКОВ И/ИЛИ КОМПЕНСАЦИЮ МОРАЛЬНОГО ВРЕДА В СУДЕБНОМ ПОРЯДКЕ. \r6. ОТВЕТСТВЕННОСТЬ \rВ СЛУЧАЕ НЕИСПОЛНЕНИЯ ПОЛОЖЕНИЙ НАСТОЯЩЕЙ ПОЛИТИКИ ООО «ИСКОНИ» НЕСЕТ ОТВЕТСТВЕННОСТЬ В СООТВЕТСТВИИ ДЕЙСТВУЮЩИМ ЗАКОНОДАТЕЛЬСТВОМ РОССИЙСКОЙ ФЕДЕРАЦИИ.\nОБРАЩАЕМ ВАШЕ ВНИМАНИЕ! \rПОЛУЧИТЬ РАЗЪЯСНЕНИЯ ПО ИНТЕРЕСУЮЩИМ ВАС ВОПРОСАМ ОБРАБОТКИ ВАШИХ ПЕРСОНАЛЬНЫХ ДАННЫХ, ОБРАТИВШИСЬ ЛИЧНО В ООО «ИСКОНИ» ЛИБО НАПРАВИВ ОФИЦИАЛЬНЫЙ ЗАПРОС ПО ПОЧТЕ РОССИИ ПО АДРЕСУ: 107078, Г. МОСКВА, 1ЫЙ БАСМАННЫЙ ПЕРЕУЛОК, 5/20, СТР 2, ОФ. 35.\rВ СЛУЧАЕ НАПРАВЛЕНИЯ ОФИЦИАЛЬНОГО ЗАПРОСА В ООО «ИСКОНИ» В ТЕКСТЕ ЗАПРОСА НЕОБХОДИМО УКАЗАТЬ: \r• ФИО; \r• НОМЕР ОСНОВНОГО ДОКУМЕНТА, УДОСТОВЕРЯЮЩЕГО ЛИЧНОСТЬ СУБЪЕКТА ПЕРСОНАЛЬНЫХ ДАННЫХ ИЛИ ЕГО ПРЕДСТАВИТЕЛЯ, СВЕДЕНИЯ О ДАТЕ ВЫДАЧИ УКАЗАННОГО ДОКУМЕНТА И ВЫДАВШЕМ ЕГО ОРГАНЕ; \r• СВЕДЕНИЯ, ПОДТВЕРЖДАЮЩИЕ ВАШЕ УЧАСТИЕ В ОТНОШЕНИЯХ С ООО «ИСКОНИ» ЛИБО СВЕДЕНИЯ, ИНЫМ СПОСОБОМ ПОДТВЕРЖДАЮЩИЕ ФАКТ ОБРАБОТКИ ПЕРСОНАЛЬНЫХ ДАННЫХ ООО «ИСКОНИ»; \r• ПОДПИСЬ ГРАЖДАНИНА (ИЛИ ЕГО ЗАКОННОГО ПРЕДСТАВИТЕЛЯ). ЕСЛИ ЗАПРОС ОТПРАВЛЯЕТСЯ В ЭЛЕКТРОННОМ ВИДЕ, ТО ОН ДОЛЖЕН БЫТЬ ОФОРМЛЕН В ВИДЕ ЭЛЕКТРОННОГО ДОКУМЕНТА И ПОДПИСАН ЭЛЕКТРОННОЙ ПОДПИСЬЮ В СООТВЕТСТВИИ С ЗАКОНОДАТЕЛЬСТВОМ РФ.\nНА САЙТЕ \rWWW.ISKONI.RU\rПУБЛИКУЕТСЯ АКТУАЛЬНАЯ ВЕРСИЯ «ПОЛИТИКИ «ООО «ИСКОНИ» В ОТНОШЕНИИ ОБРАБОТКИ ПЕРСОНАЛЬНЫХ ДАННЫХ».\nСВЕДЕНИЯ О РЕАЛИЗУЕМЫХ ТРЕБОВАНИЯХ К ЗАЩИТЕ ПЕРСОНАЛЬНЫХ ДАННЫХ \rООО «ИСКОНИ» ПРИ ОБРАБОТКЕ ПЕРСОНАЛЬНЫХ ДАННЫХ ПРИНИМАЕТ НЕОБХОДИМЫЕ ПРАВОВЫЕ, ОРГАНИЗАЦИОННЫЕ И ТЕХНИЧЕСКИЕ МЕРЫ ДЛЯ ЗАЩИТЫ ПЕРСОНАЛЬНЫХ ДАННЫХ ОТ НЕПРАВОМЕРНОГО ИЛИ СЛУЧАЙНОГО ДОСТУПА К НИМ, УНИЧТОЖЕНИЯ, ИЗМЕНЕНИЯ, БЛОКИРОВАНИЯ, КОПИРОВАНИЯ, ПРЕДОСТАВЛЕНИЯ, РАСПРОСТРАНЕНИЯ ПЕРСОНАЛЬНЫХ ДАННЫХ, А ТАКЖЕ ОТ ИНЫХ НЕПРАВОМЕРНЫХ ДЕЙСТВИЙ В ОТНОШЕНИИ ПЕРСОНАЛЬНЫХ ДАННЫХ.\nК ТАКИМ МЕРАМ В СООТВЕТСТВИИ С ФЕДЕРАЛЬНЫМ ЗАКОНОМ № 152-ФЗ «О ПЕРСОНАЛЬНЫХ ДАННЫХ» ОТНОСЯТСЯ: \r• ОПРЕДЕЛЕНИЕ УГРОЗ БЕЗОПАСНОСТИ ПЕРСОНАЛЬНЫХ ДАННЫХ ПРИ ИХ ОБРАБОТКЕ В ИНФОРМАЦИОННЫХ СИСТЕМАХ ПЕРСОНАЛЬНЫХ ДАННЫХ; \r• ПРИМЕНЕНИЕ ОРГАНИЗАЦИОННЫХ И ТЕХНИЧЕСКИХ МЕР ПО ОБЕСПЕЧЕНИЮ БЕЗОПАСНОСТИ ПЕРСОНАЛЬНЫХ ДАННЫХ ПРИ ИХ ОБРАБОТКЕ В ИНФОРМАЦИОННЫХ СИСТЕМАХ ПЕРСОНАЛЬНЫХ ДАННЫХ, НЕОБХОДИМЫХ ДЛЯ ВЫПОЛНЕНИЯ ТРЕБОВАНИЙ К ЗАЩИТЕ ПЕРСОНАЛЬНЫХ ДАННЫХ, ИСПОЛНЕНИЕ КОТОРЫХ ОБЕСПЕЧИВАЕТ УСТАНОВЛЕННЫЕ ПРАВИТЕЛЬСТВОМ РОССИЙСКОЙ ФЕДЕРАЦИИ УРОВНИ ЗАЩИЩЕННОСТИ ПЕРСОНАЛЬНЫХ ДАННЫХ; \r• ПРИМЕНЕНИЕ ПРОШЕДШИХ В УСТАНОВЛЕННОМ ПОРЯДКЕ ПРОЦЕДУРУ ОЦЕНКИ СООТВЕТСТВИЯ СРЕДСТВ ЗАЩИТЫ ИНФОРМАЦИИ; \r• ОЦЕНКА ЭФФЕКТИВНОСТИ ПРИНИМАЕМЫХ МЕР ПО ОБЕСПЕЧЕНИЮ БЕЗОПАСНОСТИ ПЕРСОНАЛЬНЫХ ДАННЫХ ДО ВВОДА В ЭКСПЛУАТАЦИЮ ИНФОРМАЦИОННОЙ СИСТЕМЫ ПЕРСОНАЛЬНЫХ ДАННЫХ; \r• ОБНАРУЖЕНИЕ ФАКТОВ НЕСАНКЦИОНИРОВАННОГО ДОСТУПА К ПЕРСОНАЛЬНЫМ ДАННЫМ И ПРИНЯТИЕМ МЕР; \r• ВОССТАНОВЛЕНИЕ ПЕРСОНАЛЬНЫХ ДАННЫХ, МОДИФИЦИРОВАННЫХ ИЛИ УНИЧТОЖЕННЫХ ВСЛЕДСТВИЕ НЕСАНКЦИОНИРОВАННОГО ДОСТУПА К НИМ; \r• УСТАНОВЛЕНИЕ ПРАВИЛ ДОСТУПА К ПЕРСОНАЛЬНЫМ ДАННЫМ, ОБРАБАТЫВАЕМЫМ В ИНФОРМАЦИОННОЙ СИСТЕМЕ ПЕРСОНАЛЬНЫХ ДАННЫХ, А ТАКЖЕ ОБЕСПЕЧЕНИЕМ РЕГИСТРАЦИИ И УЧЕТА ВСЕХ ДЕЙСТВИЙ, СОВЕРШАЕМЫХ С ПЕРСОНАЛЬНЫМИ ДАННЫМИ В ИНФОРМАЦИОННОЙ СИСТЕМЕ ПЕРСОНАЛЬНЫХ ДАННЫХ; \r• КОНТРОЛЬ ЗА ПРИНИМАЕМЫМИ МЕРАМИ ПО ОБЕСПЕЧЕНИЮ БЕЗОПАСНОСТИ ПЕРСОНАЛЬНЫХ ДАННЫХ И УРОВНЯ ЗАЩИЩЕННОСТИ ИНФОРМАЦИОННЫХ СИСТЕМ ПЕРСОНАЛЬНЫХ ДАННЫХ; \r• УЧЕТ МАШИННЫХ НОСИТЕЛЕЙ ПЕРСОНАЛЬНЫХ ДАННЫХ; \r• ОРГАНИЗАЦИЯ ПРОПУСКНОГО РЕЖИМА НА ТЕРРИТОРИЮ ОБЩЕСТВА; \r• РАЗМЕЩЕНИЕ ТЕХНИЧЕСКИХ СРЕДСТВ ОБРАБОТКИ ПЕРСОНАЛЬНЫХ ДАННЫХ В ПРЕДЕЛАХ ОХРАНЯЕМОЙ ТЕРРИТОРИИ; \r• ПОДДЕРЖАНИЕ ТЕХНИЧЕСКИХ СРЕДСТВ ОХРАНЫ, СИГНАЛИЗАЦИИ В ПОСТОЯННОЙ ГОТОВНОСТИ; \r• ПРОВЕДЕНИЕ МОНИТОРИНГА ДЕЙСТВИЙ ПОЛЬЗОВАТЕЛЕЙ, ПРОВЕДЕНИЕ РАЗБИРАТЕЛЬСТВ ПО ФАКТАМ НАРУШЕНИЯ ТРЕБОВАНИЙ БЕЗОПАСНОСТИ ПЕРСОНАЛЬНЫХ ДАННЫХ\nВ ЦЕЛЯХ КООРДИНАЦИИ ДЕЙСТВИЙ ПО ОБЕСПЕЧЕНИЮ БЕЗОПАСНОСТИ ПЕРСОНАЛЬНЫХ ДАННЫХ В ООО «ИСКОНИ» НАЗНАЧЕНЫ ЛИЦА, ОТВЕТСТВЕННЫЕ ЗА ОБЕСПЕЧЕНИЕ БЕЗОПАСНОСТИ ПЕРСОНАЛЬНЫХ ДАННЫХ.\r\n'),(50,'a95212fc2740c04a2a79fb9b0ddc36f8','МОЙ ПРОФИЛЬ\r\n\r\n'),(23,'3e11df259cd29af14e34c9f754e32d5c','КОНТАКТЫ\r\nКОНТАКТЫ\nГ. МОСКВА\rМЕТРО КОМСОМОЛЬСКАЯ\rАДРЕС: 1ЫЙ БАСМАННЫЙ ПЕРЕУЛОК, Д. 5/20, СТР. 2, ОФ. 35.\rОХРАНЯЕМАЯ ПАРКОВКА НА ТЕРРИТОРИИ.\r8-495-744-78-99\r8-800-222-35-77\rПН-ПТ С 10:00 ДО 19:00.\rМЫ ВСЕГДА РАДЫ ВАС ВИДЕТЬ В НАШЕМ ОФИСЕ, С УДОВОЛЬСТВИЕМ ПОКАЖЕМ ВАМ ВЫСТАВОЧНЫЕ ОБРАЗЦЫ И ПРОКОНСУЛЬТИРУЕМ ПО ТЕХНОЛОГИЯМ СТРОИТЕЛЬСТВА, А ТАКЖЕ УГОСТИМ САМЫМ ВКУСНЫМ ЧАЕМ И КОФЕ, НО УБЕДИТЕЛЬНО ПРОСИМ ПРЕДВАРИТЕЛЬНО СОГЛАСОВАТЬ ВАШ ВИЗИТ.\nГ. КАЗАНЬ\rПРЕДСТАВИТЕЛЬСТВО КОМПАНИИ «КЕДР»\rАДРЕС: Г. КАЗАНЬ, УЛ. ГЛАДИЛОВА, Д. 27, ОФ. 426\r8-800-222-35-77\rПН-ПТ С 10:00 ДО 19:00\r\n'),(53,'8d17afa4625c1d67b7d2156d8e06da89','404 NOT FOUND\r\n404 СТРАНИЦА НЕ НАЙДЕНА\rЗАПРОШЕННАЯ СТРАНИЦА НЕ НАЙДЕНА\r\n'),(54,'70a7260c28b5507a7cb3d04b3bdcee17','ПРОИЗВОДСТВО\r\nПРОИЗВОДСТВО ISKONI \rПРОИЗВОДСТВО ISKONI НАХОДИТСЯ В РЕСПУБЛИКЕ МАРИЙ ЭЛ.\nПРОИЗВОДСТВО ISKONI - ЭТО СЕМЕЙНАЯ ФАБРИКА «КЕДР» С БОЛЕЕ ЧЕМ 20-ЛЕТНИМ ОПЫТОМ РАБОТЫ. БЛАГОДАРЯ СВОЕМУ ГЕОГРАФИЧЕСКОМУ РАСПОЛОЖЕНИЮ В ПРОИЗВОДСТВЕ МОЖЕТ БЫТЬ ЗАДЕЙСТВОВАНА ДРЕВЕСИНА ИЗ РЕСПУБЛИКИ МАРИЙ ЭЛ, КИРОВСКОЙ ОБЛАСТИ И ПЕРМСКОГО КРАЯ. ПРИРОДА ЭТИХ МЕСТ БОГАТА ЛЕСАМИ, А КЛИМАТИЧЕСКИЕ И ПОГОДНЫЕ УСЛОВИЯ ДЛЯ РАСТУЩИХ ТАМ ЛЕСОВ СПОСОБСТВУЮТ ВЫСОКОМУ КАЧЕСТВУ ИСХОДНОГО СЫРЬЯ.\nПРОИЗВОДСТВО ISKONI ЭТО:\rИННОВАЦИОННАЯ ЗАПАТЕНТОВАННАЯ ТЕХНОЛОГИЯ\rЛИНЕЙКА ПРОДУКЦИИ ISKONI – БОЛЬШОЙ ВЫБОР МАТЕРИАЛА ДЛЯ СТРОИТЕЛЬСТВА ДОМОКОМПЛЕКТА ИЗ КЛЕЕНОГО БРУСА, ВКЛЮЧАЯ ИННОВАЦОННЫЙ ПРОДУКТ – УТЕПЛЕННЫЙ КЛЕЕНЫЙ БРУС, СУХОГО ПРОФИЛИРОВАННОГО БРУСА И ОЦИЛИНДРОВАННОГО БРЕВНА, А ТАКЖЕ БРУСА, ПОДГОТОВЛЕННОГО ДЛЯ ВОЗВЕДЕНИЯ ДОМА ПО КАРКАСНОЙ ТЕХНОЛОГИИ.\nКАЖДЫЙ ИЗ ЭТИХ МАТЕРИАЛОВ МОЖЕТ БЫТЬ ВЫПОЛНЕН ИЗ СОСНЫ, ЕЛИ, КЕДРА ИЛИ ЛЮБОГО ДРУГОГО МАТЕРИАЛА ПО ВАШЕМУ ВЫБОРУ.\nХАРАКТЕРИСТИКИ: \rСТЕНА ИЗ КЛЕЕНОГО БРУСА С УТЕПЛИТЕЛЕМ ОБЛАДАЕТ ВСЕМИ ПРЕИМУЩЕСТВАМИ КЛЕЕНОГО БРУСА: ПРАКТИЧЕСКИ НЕТ УСАДКИ, НЕТ КОРОБЛЕНИЯ И РАСТРЕСКИВАЕТСЯ. ПРИ ЭТОМ УГЛОВОЙ ЗАМОК ПОЛУЧАЕТСЯ АБСОЛЮТНО НЕПРОДУВАЕМЫМ И ЗВУКОИЗОЛИРУЮЩИМ.\nУТЕПЛИТЕЛЬ ЭКОВАТА ИЛИ GUTEX ЯВЛЯЕТСЯ ХОРОШИМ АНТИСЕПТИКОМ, ЗАЩИЩАЮЩИМ БРУС ОТ БОЛЕЗНЕЙ И ГРЫЗУНОВ ИЗНУТРИ.\nНЕТ НЕОБХОДИМОСТИ СВЕРЛИТЬ ОТВЕРСТИЯ ДЛЯ КОММУНИКАЦИЙ: ВСЕ ТРУБЫ И ПРОВОДА МОЖНО СВОБОДНО МОНТИРОВАТЬ ДАЖЕ ПОСЛЕ ВОЗВЕДЕНИЯ СТЕН И КРЫШИ, ДАЖЕ ПОСЛЕ УКЛАДКИ УТЕПЛИТЕЛЯ.\nКРОМЕ ТОГО, ПРИ СТРОИТЕЛЬСТВЕ ДОМОВ ИЗ УТЕПЛЕННОГО БРУСА СТОИМОСТЬ ДОМОКОМПЛЕКТА ПО СРАВНЕНИЮ С КЛАССИЧЕСКИМ КЛЕЕНЫМ БРУСОМ ВЫГОДНЕЕ НА 30%, ЧТО ПРИВОДИТ К 10% ЭКОНОМИИ НА СТОИМОСТИ ВСЕГО ПРОЕКТА.\nИСПОЛЬЗУЕМЫЕ МАТЕРИАЛЫ В СТРОИТЕЛЬСТВЕ \rПРИ СТРОИТЕЛЬСТВЕ ДОМОВ ИЗ БРУСА МЫ ИСПОЛЬЗУЕМ СЛЕДУЮЩИЕ ВИДЫ МАТЕРИАЛОВ ИЗ СОСНЫ И ЕЛИ.\r\n'),(70,'16e7dbea19ae6c92203154a4ad9b6da3','КЛЕЕНЫЙ БРУС\r\nЦЕНЫ НА КЛЕЕНЫЙ БРУС - СОСНА / ЕЛЬ \rХАРАКТЕРИСТИКИ\n(200, 220, 240) Х 180 ММ. \rС НАРЕЗКОЙ ЧАШЕК ПОД ПРОЕКТ БЕЗ СРАЩИВАНИЯ ПО ДЛИНЕ (6 М.)\nШИРИНА\n200 ММ\r220 ММ\r240 ММ\rСТОИМОСТЬ ЗА М\r3\r23 500 РУБ.\r23 500 РУБ.\r23 500 РУБ.\rХАРАКТЕРИСТИКИ\n(200, 220, 240) Х 270 ММ. \rС НАРЕЗКОЙ ЧАШЕК ПОД ПРОЕКТ\nШИРИНА\n200 ММ\r220 ММ\r240 ММ\rСТОИМОСТЬ ЗА М\r3\r29 000 РУБ.\r29 000 РУБ.\r29 000 РУБ.\rХАРАКТЕРИСТИКИ\n120 Х 135 ММ. \rС НАРЕЗКОЙ ЧАШЕК ПОД ПРОЕКТ \rВЛАЖНОСТЬ 12%\nШИРИНА\n200 ММ\r220 ММ\r240 ММ\rСТОИМОСТЬ ЗА М\r3\r27 000 РУБ.\rХАРАКТЕРИСТИКИ\n120 Х 135 ММ. \rС НАРЕЗКОЙ ЧАШЕК ПОД ПРОЕКТ \rВЛАЖНОСТЬ 12%\nШИРИНА\n200 ММ\r220 ММ\r240 ММ\rСТОИМОСТЬ ЗА М\r3\r27 000 РУБ.\rХАРАКТЕРИСТИКИ\n120 Х 135 ММ. \rС НАРЕЗКОЙ ЧАШЕК ПОД ПРОЕКТ \rВЛАЖНОСТЬ 12%\nШИРИНА\n200 ММ\r220 ММ\r240 ММ\rСТОИМОСТЬ ЗА М\r3\r27 000 РУБ.\rХАРАКТЕРИСТИКИ\n120 Х 135 ММ. \rС НАРЕЗКОЙ ЧАШЕК ПОД ПРОЕКТ \rВЛАЖНОСТЬ 12%\nШИРИНА\n200 ММ\r220 ММ\r240 ММ\rСТОИМОСТЬ ЗА М\r3\r27 000 РУБ.\rХАРАКТЕРИСТИКИ\n200 Х 135 ММ. \rС НАРЕЗКОЙ ЧАШЕК ПОД ПРОЕКТ \rВЛАЖНОСТЬ 12%\nШИРИНА\n200 ММ\r220 ММ\r240 ММ\rСТОИМОСТЬ ЗА М\r3\r27 000 РУБ.\rХАРАКТЕРИСТИКИ\n(200, 220, 240) Х 180 ММ. \rС НАРЕЗКОЙ ЧАШЕК ПОД ПРОЕКТ\nШИРИНА\n200 ММ\r220 ММ\r240 ММ\rСТОИМОСТЬ ЗА М\r3\r27 000 РУБ.\r27 000 РУБ.\r27 000 РУБ.\rЦЕНЫ НА КЛЕЕНЫЙ БРУС - КЕДР \rХАРАКТЕРИСТИКИ\n(200, 220, 240) Х 270 ММ. \r(ПОЛНОСТЬЮ ИЗ КЕДРА) \rС НАРЕЗКОЙ ЧАШЕК ПОД ПРОЕКТ\nШИРИНА\n200 ММ\r220 ММ\r240 ММ\rСТОИМОСТЬ ЗА М\r3\r36 000 РУБ.\r36 000 РУБ.\r36 000 РУБ.\rХАРАКТЕРИСТИКИ\n(200, 220, 240) Х 270 ММ. \r(ЛИЦЕВЫЕ ЛАМЕЛИ ИЗ КЕДРА) \rС НАРЕЗКОЙ ЧАШЕК ПОД ПРОЕКТ\nШИРИНА\n200 ММ\r220 ММ\r240 ММ\rСТОИМОСТЬ ЗА М\r3\r34 000 РУБ.\r34 000 РУБ.\r34 000 РУБ.\rХАРАКТЕРИСТИКИ\n(200, 220, 240) Х 180 ММ. \r(ПОЛНОСТЬЮ ИЗ КЕДРА) \rС НАРЕЗКОЙ ЧАШЕК ПОД ПРОЕКТ\nШИРИНА\n200 ММ\r220 ММ\r240 ММ\rСТОИМОСТЬ ЗА М\r3\r34 000 РУБ.\r34 000 РУБ.\r34 000 РУБ.\rХАРАКТЕРИСТИКИ\n(200, 220, 240) Х 180 ММ. \r(ЛИЦЕВЫЕ ЛАМЕЛИ ИЗ КЕДРА) \rС НАРЕЗКОЙ ЧАШЕК ПОД ПРОЕКТ\nШИРИНА\n200 ММ\r220 ММ\r240 ММ\rСТОИМОСТЬ ЗА М\r3\r32 000 РУБ.\r32 000 РУБ.\r32 000 РУБ.\rКЛЕЕНЫЙ БРУС\rПЛЮСЫ\nМИНИМАЛЬНАЯ УСАДКА, ПРАКТИЧЕСКИ ОТСУТСТВИЕ ТРЕЩИН, НЕ ТРЕБУЕТСЯ КОНОПАТКА. МИНИМАЛЬНОЕ ИЗМЕНЕНИЕ ГЕОМЕТРИИ МАТЕРИАЛА. ПОРОКИ ДРЕВЕСИНЫ ВЫРЕЗАЮТСЯ НА ЭТАПЕ ПРОИЗВОДСТВА.\nМИНУСЫ\nСТОИМОСТЬ 1 КУБ.М. КЛЕЕНОГО БРУСА ДОРОЖЕ ОСТАЛЬНЫХ МАТЕРИАЛОВ ДЛЯ ДЕРЕВЯННОГО ДОМА, ОДНАКО МИНИМАЛЬНЫЕ ЗАТРАТЫ ПРИ ПОСЛЕДУЮЩЕЙ ЭКСПЛУАТАЦИИ, КОМПЕНСИРУЮТ КАЖУЩУЮСЯ ДОРОГОВИЗНУ.\r\n'),(60,'fa6d9d851b96ca514588a2180593d2ad','УТЕПЛЕННЫЙ КЛЕЕНЫЙ БРУС\r\nЧТО ТАКОЕ УТЕПЛЕННЫЙ КЛЕЕНЫЙ БРУС? \rУТЕПЛЕННЫЙ КЛЕЕНЫЙ БРУС - ЭТО ИННОВАЦИОННАЯ РАЗНОВИДНОСТЬ КЛЕЕНОГО БРУСА, ПРОСТРАНСТВО МЕЖДУ ВНЕШНИМИ ЛАМЕЛЯМИ КОТОРОГО ЗАПОЛНЯЕТСЯ СПЕЦИАЛЬНЫМ УТЕПЛИТЕЛЕМ. ОБЪЕМ УТЕПЛИТЕЛЯ РАССЧИТАН С УЧЕТОМ СОХРАНЕНИЯ ПРОЧНОСТИ КОНСТРУКЦИИ.\nПРИМЕНЯЕМЫЕ УТЕПЛИТЕЛИ ИМЕЮТ БОЛЕЕ НИЗКИЙ КОЭФФИЦИЕНТ ТЕПЛОПРОВОДНОСТИ, ПО СРАВНЕНИЮ С ДЕРЕВОМ.\nПРОЦЕСС УТЕПЛЕНИЯ, КАК ПРАВИЛО, ПРОИЗВОДИТСЯ НА СТРОЙПЛОЩАДКЕ. НО В ЗАВИСИМОСТИ ОТ ВИДА УТЕПЛИТЕЛЯ ЭТОТ ПРОЦЕСС МОЖЕТ БЫТЬ ОСУЩЕСТВЛЕН И В ЗАВОДСКИХ УСЛОВИЯХ. \nКОНСТРУКЦИЯ УТЕПЛЕННОГО КЛЕЕНОГО БРУСА ПОЗВОЛЯЕТ ПРОИЗВОДИТЬ УТЕПЛЕНИЕ РАЗЛИЧНЫМИ ВИДАМИ МАТЕРИАЛОВ: ОТ ЭКОНОМИЧНЫХ (ЭКОВАТА), СРЕДНЕГО СЕГМЕНТА (GUTEX (ГЕРМАНИЯ)) ДО УТЕПЛИТЕЛЕЙ ПРЕМИУМ-КЛАССА (КОРА ПРОБКОВОГО ДУБА). ТЕХНОЛОГИИ СБОРКИ ОБЕСПЕЧИВАЮТ ПОВЫШЕННЫЕ ЭКСПЛУАТАЦИОННЫЕ СВОЙСТВА ДОМОВ, ПОСТРОЕННЫХ ИЗ ЭТОГО МАТЕРИАЛА.\nХАРАКТЕРИСТИКИ: \rПРЕИМУЩЕСТВА КЛЕЕНОГО БРУСА \rКОЭФФИЦИЕНТ ТЕПЛОПРОВОДНОСТИ УТЕПЛЕННОГО КЛЕЕНОГО БРУСА НИЖЕ, ЧТО УЛУЧШАЕТ ЕГО ТЕПЛОВЫЕ ХАРАКТЕРИСТИКИ, ПРИВОДИТ К УЛУЧШЕНИЮ ЭКСПЛУАТАЦИОННЫХ ХАРАКТЕРИСТИК СТРОЕНИЙ, ВЫПОЛНЕННЫХ ИЗ ЭТОГО МАТЕРИАЛА. СЛЕДСТВИЕМ ЧЕГО ЯВЛЯЕТСЯ ЭКОНОМИЯ НА ЭКСПЛУАТАЦИОННЫЕ ЗАТРАТЫ ДОМА БОЛЕЕ ЧЕМ НА 25%, ОСОБЕННО В ХОЛОДНОЕ ВРЕМЯ ГОДА;\nРАВНОМЕРНОЕ ЗАПОЛНЕНИЕ УТЕПЛИТЕЛЕМ ЧАШЕК ПЕРЕРУБОВ ОБЕСПЕЧИВАЕТ НАДЕЖНОЕ УТЕПЛЕНИЕ ЭТИХ УЗЛОВ И НЕ ТРЕБУЕТ ДОПОЛНИТЕЛЬНЫХ МЕР ГЕРМЕТИЗАЦИИ ЧАШЕК,- САМОГО РАСПРОСТРАНЕННОГО НЕДОСТАТКИ ДОМОВ ИЗ КЛЕЕНОГО БРУСА;\nСТЕНОВОЙ МАТЕРИАЛ ИЗ УТЕПЛЕННОГО КЛЕЕНОГО БРУСА ИМЕЕТ ПОВЫШЕННЫЕ ШУМОИЗОЛЯЦИОННЫЕ ХАРАКТЕРИСТИКИ;\nУДЕЛЬНЫЙ ВЕС МАТЕРИАЛОВ, СОСТОЯЩИХ ИЗ РАЗЛИЧНЫХ ХИМИЧЕСКИХ СОЕДИНЕНИЙ, В ЧАСТНОСТИ,- КЛЕЯ,- НА 50% НИЖЕ, ЧТО ОБЕСПЕЧИВАЕТ ПОВЫШЕННЫЕ ЭКОЛОГИЧЕСКИЕ ХАРАКТЕРИСТИКИ УТЕПЛЕННОГО КЛЕЕНОГО БРУСА;\nИСХОДНЫЙ СТРОИТЕЛЬНЫЙ МАТЕРИАЛ ВДВОЕ ЛЕГЧЕ, ЧТО СУЩЕСТВЕННО УПРОЩАЕТ ПРОЦЕССЫ ТРАНСПОРТИРОВКИ ДОМОКОМПЛЕКТА И МОНТАЖА ЗДАНИЙ ИЗ ЭТОГО МАТЕРИАЛА, НЕ ТРЕБУЕТ ПРИМЕНЕНИЯ СПЕЦИАЛЬНОЙ ТЕХНИКИ ПРИ СТРОИТЕЛЬСТВЕ , ЧТО ПРИВОДИТ К ЭКОНОМИИ СТОИМОСТИ РАБОТ И ТРАНСПОРТНЫХ ЗАТРАТ;\nТЕХНОЛОГИЧЕСКИЕ ОСОБЕННОСТИ МАТЕРИАЛА ПОЗВОЛЯЮТ ПРОКЛАДЫВАТЬ В ТЕЛЕ УТЕПЛЕННОГО КЛЕЕНОГО БРУСА МНОГИЕ ЭЛЕМЕНТЫ ИНЖЕНЕРНЫХ СИСТЕМ, ЧТО ИСКЛЮЧАЕТ ДОРОГОСТОЯЩИЕ РАБОТЫ ПРИ ИХ МОНТАЖЕ И ПРИВОДИТ К СНИЖЕНИЮ СТОИМОСТИ ПОДГОТОВИТЕЛЬНЫХ РАБОТ ПО МОНТАЖУ ИНЖЕНЕРНЫХ СИСТЕМ.\nВ НАСТОЯЩЕЕ ВРЕМЯ ТЕХНОЛОГИЯ УТЕПЛЕННОГО КЛЕЕНОГО БРУСА ОТРАБОТАНА ДЛЯ ДВУХ РАЗМЕРОВ БРУСА ПО ТОЛЩИНЕ (200 И 240 ММ), ЧЕТЫРЕХ ВАРИАНТОВ ИЗГОТОВЛЕНИЯ: ИЗ СОСНЫ, ЕЛИ, КЕДРА И КОМБИНИРОВАННОГО ВАРИАНТА, КОГДА ОДНА ВНЕШНЯЯ ЛАМЕЛЬ ИЗГОТАВЛИВАЕТСЯ ИЗ КЕДРА, А ДРУГАЯ- ИЗ СОСНЫ (ИЛИ ЕЛИ), А ТАКЖЕ ДЛЯ НЕСКОЛЬКИХ ВИДОВ И ТИПОВ УТЕПЛИТЕЛЕЙ- ОТ ЭКОНОМНОГО ВАРИАНТА УТЕПЛЕНИЯ ЭКОВАТОЙ ДО БОЛЕЕ ДОРОГИХ ТВЕРДЫХ УТЕПЛИТЕЛЕЙ НА ОСНОВЕ ДРЕВЕСИНЫ И КОРЫ ПРОБКОВОГО ДУБА.\nВНЕШНИЙ ВИД ДОМОВ ИЗ УТЕПЛЕННОГО БРУСА НИЧЕМ НЕ ОТЛИЧАЕТСЯ ОТ ДОМОВ ИЗ ОБЫЧНОГО БРУСА; КРОМЕ ТОГО, ПРИ ИСПОЛЬЗОВАНИИ ОПРЕДЕЛЕННЫХ ТИПОВ УТЕПЛИТЕЛЯ, СТОИМОСТЬ ДОМОКОМПЛЕТКОВ ИЗ УТЕПЛЕННОГО КЛЕЕНОГО БРУСА НА 30% НИЖЕ, ЧЕМ СТОИМОСТЬ ОБЫЧНОГО КЛЕЕНОГО БРУСА, ЧТО ПОЗВОЛЯЕТ ДОБИТЬСЯ ЭКОНОМИИ В 10% ОТ ОБЩЕЙ ЦЕНЫ БУДУЩЕГО ДОМА!\nИСПЫТАНИЯ УТЕПЛЕННОГО БРУСА\rУ НАШИХ ЗАКАЗЧИКОВ ЕСТЬ ДВА ОСНОВНЫХ ОПАСЕНИЯ, СВЯЗАННЫЕ СО СТРОИТЕЛЬСТВОМ ДОМОВ ИЗ УТЕПЛЕННОГО КЛЕЕНОГО БРУСА : ПРОЧНОСТЬ И ПОЖАРОУСТОЙЧИВОСТЬ.\nЧТОБЫ РАЗВЕЯТЬ ВАШИ СОМНЕНИЯ В ЦЕЛЯХ ПОПУЛЯРИЗАЦИИ РАЗРАБОТАННОГО НАМИ ПРОДУКТА И ПОДТВЕРЖДЕНИЯ ЕГО ЭКСПЛУАТАЦИОННЫХ СВОЙСТВ ЗАЯВЛЯЕМЫМ, НИЖЕ ПРИВОДЯТСЯ РЕЗУЛЬТАТЫ ИСПЫТАНИЙ УТЕПЛЕННОГО КЛЕЕНОГО БРУСА.\nИСПЫТАНИЯ НА ПОЖАРОСТОЙКОСТЬ\nИСПАТЫНИЯ НА ПРОЧНОСТЬ\nУТЕПЛЕННЫЙ БРУС ИЗ СОСНЫ ИЛИ ЕЛИ \rСЕЧЕНИЕ БРУСА, ММ\nМАТЕРИАЛ УТЕПЛИТЕЛЯ\nЦЕНА С 01.03.2020 РУБ./ М³\nЭКОВАТА\n22,000\nGUTEX МЯГКИЙ\n23,400\nGUTEX ТВЕРДЫЙ\n31,100\nКОРА ПРОБКОВОГО ДУБА\n45,500\nЭКОВАТА РУБ.\n25100\nGUTEX МЯГКИЙ\n26000\nGUTEX ТВЕРДЫЙ\n33700\nКОРА ПРОБКОВОГО ДУБА\n48100\nУТЕПЛЕННЫЙ БРУС ИЗ КЕДРА\rСЕЧЕНИЕ БРУСА, ММ\nМАТЕРИАЛ УТЕПЛИТЕЛЯ\nЦЕНА С 01.03.2020 РУБ./ М³\nЭКОВАТА\n39,900\nGUTEX МЯГКИЙ\n41,300\nGUTEX ТВЕРДЫЙ\n49,900\nКОРА ПРОБКОВОГО ДУБА\n63400\nЭКОВАТА РУБ.\n42500\nGUTEX МЯГКИЙ\n43900\nGUTEX ТВЕРДЫЙ\n51600\nКОРА ПРОБКОВОГО ДУБА\n66000\nУТЕПЛЕННЫЙ КОМБИНИРОВАННЫЙ БРУС (КЕДРА + СОСНА/ЕЛЬ)\rСЕЧЕНИЕ БРУСА, ММ\nМАТЕРИАЛ УТЕПЛИТЕЛЯ\nЦЕНА С 01.03.2020 РУБ./ М³\nЭКОВАТА\n28,600\nGUTEX МЯГКИЙ\n30,000\nGUTEX ТВЕРДЫЙ\n37,700\nКОРА ПРОБКОВОГО ДУБА\n52,100\nЭКОВАТА РУБ.\n29,800\nGUTEX МЯГКИЙ\n31,200\nGUTEX ТВЕРДЫЙ\n38,900\nКОРА ПРОБКОВОГО ДУБА\n53,300\r\n'),(123,'b270c7562b7f9f837765d61defb97171','ПОЗДРАВЛЯЕМ ВАС С НОВЫМ ГОДОМ И РОЖДЕСТВОМ\r\nПОЗДРАВЛЯЕМ ВАС С НОВЫМ ГОДОМ И РОЖДЕСТВОМ!\rЖЕЛАЕМ ВАМ И ВАШЕМУ ДОМУ ТЕПЛА, ГАРМОНИИ И ЛЮБВИ!\r\n\rДОРОГИЕ ДРУЗЬЯ!\rПОЗДРАВЛЯЕМ ВАС С НОВЫМ ГОДОМ И РОЖДЕСТВОМ!\rЖЕЛАЕМ ВАМ И ВАШЕМУ ДОМУ ТЕПЛА, ГАРМОНИИ И ЛЮБВИ!\r\n'),(124,'9a0373dbe304fd34b2a038352b6cd979','ВЫСТАВКА В ЭКСПОЦЕНТРЕ\r\nКОМПАНИЯ ISKONI ПРИНИМАЕТ УЧАСТИЕ В ВЫСТАВКЕ «ОТЕЧЕСТВЕННЫЕ СТРОИТЕЛЬНЫЕ МАТЕРИАЛЫ», КОТОРАЯ ПРОЙДЕТ В ЭКСПОЦЕНТРЕ НА КРАСНОПРЕСНЕНСКОЙ НАБЕРЕЖНОЙ С 28 ПО 31 ЯНВАРЯ. \r\n\rКОМПАНИЯ ISKONI ПРИНИМАЕТ УЧАСТИЕ В ВЫСТАВКЕ «ОТЕЧЕСТВЕННЫЕ СТРОИТЕЛЬНЫЕ МАТЕРИАЛЫ»,\rКОТОРАЯ ПРОЙДЕТ В ЭКСПОЦЕНТРЕ НА КРАСНОПРЕСНЕНСКОЙ НАБЕРЕЖНОЙ С 28 ПО 31 ЯНВАРЯ. \rНА ВЫСТАВКЕ МЫ ПРЕДСТАВЛЯЕМ НАШ ИННОВАЦИОННЫЙ МАТЕРИАЛ — УТЕПЛЕННЫЙ КЛЕЕНЫЙ БРУС! \rСОВМЕСТНО С КОМПАНИЕЙ ЭКОКЕРАМ ПРЕДЛАГАЕМ ВАШЕМУ ВНИМАНИЮ ПРОЕКТ КОМБИНИРОВАННОГО ДОМА ИЗ КЕРАМИЧЕСКОГО БЕТОНА И КЛЕЕНОГО БРУСА.\rПРИМЕНЕНИЕ ИННОВАЦИОННЫХ ТЕХНОЛОГИЙ ПОЗВОЛЯЕТ НАМ СТРОИТЬ ДОМА ДО 30% ДЕШЕВЛЕ ПО СРАВНЕНИЮ С АНАЛОГАМИ НА РЫНКЕ.\rПОДРОБНЕЕ РАССКАЖЕМ НА ВЫСТАВКЕ. ПАВИЛЬОН № 7, СТЕНД В105. ЖДЕМ ВАС!\rОБЯЗАТЕЛЬНО ЗАРЕГИСТРИРУЙТЕСЬ ДЛЯ БЕСПЛАТНОГО ПОСЕЩЕНИЯ: \rHTTPS://WWW.OSMEXPO.RU\r\n'),(89,'d8392ffcfad9d9932414957ebe4b9a46','БЛОГ\r\n\r\n'),(77,'6df167a5c0184ebc6d69b32284b22317','ПРОИЗВОДСТВО\r\nУНИКАЛЬНЫЕ ТЕХНОЛОГИИ\rПРИ ПРОИЗВОДСТВЕ ДОМОВ ИЗ КЛЕЕНОГО БРУСА СТРОИТЕЛЬНАЯ КОМПАНИЯ ISKONI ИСПОЛЬЗУЕТ СОБСТВЕННЫЕ ЗАПАТЕНТОВАННЫЕ, А ТАКЖЕ САМЫЕ ПЕРЕДОВЫЕ НЕМЕЦКИЕ ТЕХНОЛОГИИ, ЧТО ПОЗВОЛЯЕТ НАМ ГАРАНТИРОВАТЬ ВЫСОКОЕ КАЧЕСТВО ПО ВЫГОДНЫМ ЦЕНАМ.\nКОМПАНИЯ ISKONI ЯВЛЯЕТСЯ МОСКОВСКИМ ПРЕДСТАВИТЕЛЬСТВОМ СТРОИТЕЛЬНО-ПРОИЗВОДСТВЕННОЙ КОМПАНИИ «КЕДР» (РЕСПУБЛИКА МАРИЙ ЭЛ). \r\"КЕДР\"- ЭТО СЕМЕЙНАЯ ФАБРИКА С 25-ЛЕТНИМ ОПЫТОМ ДЕРЕВЯННОГО ДОМОСТРОИТЕЛЬСТВА.\nПРОИЗВОДСТВО КОМПАНИИ \"КЕДР\"\rПРОИЗВОДСТВО КОМПАНИИ \"КЕДР\"\rОСОБЕННОСТИ ПРОИЗВОДСТВА\rИННОВАЦИОННАЯ ЗАПАТЕНТОВАННАЯ ТЕХНОЛОГИЯ\rЭТО УНИКАЛЬНАЯ ЗАПАТЕНТОВАННАЯ РАЗРАБОТКА НАШЕЙ ПРОИЗВОДСТВЕННОЙ КОМПАНИИ! НАША РАЗРАБОТКА ЯВЛЯЕТСЯ БУДУЩИМ ДЕРЕВЯННОГО ДОМОСТРОЕНИЕ, КОТОРОЕ УЖЕ НАСТУПИЛО!\nУТЕПЛЕННЫЙ КЛЕЕНЫЙ БРУС - ЭТО ИННОВАЦИОННАЯ РАЗНОВИДНОСТЬ КЛЕЕНОГО БРУСА, ПРОСТРАНСТВО МЕЖДУ ВНЕШНИМИ ЛАМЕЛЯМИ КОТОРОГО ЗАПОЛНЯЕТСЯ СПЕЦИАЛЬНЫМ УТЕПЛИТЕЛЕМ. ОБЪЕМ УТЕПЛИТЕЛЯ РАССЧИТАН С УЧЕТОМ СОХРАНЕНИЯ ПРОЧНОСТИ КОНСТРУКЦИИ.\nПРИМЕНЯЕМЫЕ УТЕПЛИТЕЛИ ИМЕЮТ БОЛЕЕ НИЗКИЙ КОЭФФИЦИЕНТ ТЕПЛОПРОВОДНОСТИ, ПО СРАВНЕНИЮ С ДЕРЕВОМ. ПРОЦЕСС УТЕПЛЕНИЯ, КАК ПРАВИЛО, ПРОИЗВОДИТСЯ НА СТРОЙПЛОЩАДКЕ, НО, В ЗАВИСИМОСТИ ОТ ВИДА УТЕПЛИТЕЛЯ, МОЖЕТ БЫТЬ ОСУЩЕСТВЛЕН И В ЗАВОДСКИХ УСЛОВИЯХ. КОНСТРУКЦИЯ УТЕПЛЕННОГО КЛЕЕНОГО БРУСА ПОЗВОЛЯЕТ ПРОИЗВОДИТЬ УТЕПЛЕНИЕ РАЗЛИЧНЫМИ ВИДАМИ МАТЕРИАЛОВ: ОТ ЭКОНОМИЧНЫХ (ЭКОВАТА), СРЕДНЕГО СЕГМЕНТА (GUTEX (ГЕРМАНИЯ)) ДО УТЕПЛИТЕЛЕЙ ПРЕМИУМ-КЛАССА (КОРА ПРОБКОВОГО ДУБА). ТЕХНОЛОГИИ СБОРКИ ОБЕСПЕЧИВАЮТ ПОВЫШЕННЫЕ ЭКСПЛУАТАЦИОННЫЕ СВОЙСТВА ДОМОВ, ПОСТРОЕННЫХ ИЗ ЭТОГО МАТЕРИАЛА.\nХАРАКТЕРИСТИКИ:\nСТЕНА ИЗ КЛЕЕНОГО БРУСА С УТЕПЛИТЕЛЕМ ОБЛАДАЕТ ВСЕМИ ПРЕИМУЩЕСТВАМИ КЛЕЕНОГО БРУСА: ПРАКТИЧЕСКИ НЕТ УСАДКИ, НЕТ КОРОБЛЕНИЯ И РАСТРЕСКИВАЕТСЯ. ПРИ ЭТОМ УГЛОВОЙ ЗАМОК ПОЛУЧАЕТСЯ АБСОЛЮТНО НЕПРОДУВАЕМЫМ И ЗВУКОИЗОЛИРУЮЩИМ.\nУТЕПЛИТЕЛЬ ЭКОВАТА ИЛИ GUTEX ЯВЛЯЕТСЯ ХОРОШИМ АНТИСЕПТИКОМ, ЗАЩИЩАЮЩИМ БРУС ОТ БОЛЕЗНЕЙ И ГРЫЗУНОВ ИЗНУТРИ.\nНЕТ НЕОБХОДИМОСТИ СВЕРЛИТЬ ОТВЕРСТИЯ ДЛЯ КОММУНИКАЦИЙ: ВСЕ ТРУБЫ И ПРОВОДА МОЖНО СВОБОДНО МОНТИРОВАТЬ ДАЖЕ ПОСЛЕ ВОЗВЕДЕНИЯ СТЕН И КРЫШИ, ДАЖЕ ПОСЛЕ УКЛАДКИ УТЕПЛИТЕЛЯ.\nКРОМЕ ТОГО, ПРИ СТРОИТЕЛЬСТВЕ ДОМОВ ИЗ УТЕПЛЕННОГО БРУСА СТОИМОСТЬ ДОМОКОМПЛЕКТА ПО СРАВНЕНИЮ С КЛАССИЧЕСКИМ КЛЕЕНЫМ БРУСОМ ВЫГОДНЕЕ НА 30%, ЧТО ПРИВОДИТ К 10% ЭКОНОМИИ НА СТОИМОСТИ ВСЕГО ПРОЕКТА.\nИСПОЛЬЗУЕМЫЕ МАТЕРИАЛЫ В СТРОИТЕЛЬСТВЕ\nПРИ СТРОИТЕЛЬСТВЕ ДОМОВ ИЗ БРУСА МЫ ИСПОЛЬЗУЕМ СЛЕДУЮЩИЕ ВИДЫ МАТЕРИАЛОВ ИЗ СОСНЫ И ЕЛИ.\r\n'),(61,'4f18f21dd19c29c90516d9e2d9fc5ed6','БЕКАС\r\n\r\n'),(63,'9f38433448bbe97d4825b73d7e624b43','ИНДИВИДУАЛЬНЫЙ ПРОЕКТ ДОМА. РЕСПУБЛИКА ТАТАРСТАН\r\nДВУХЭТАЖНЫЙ ДОМ ИЗ КЛЕЕНОГО БРУСА. ПЛОЩАДЬ ДОМА 180 КВ М. ДОПОЛНИТЕЛЬНО ИЗ КЛЕЕНОГО БРУСА БЫЛИ ПОСТРОЕНЫ БАНЯ И ЛЕТНЯЯ ПОЛЕВАЯ КУХНЯ.\r\n'),(72,'4fbf42fbbc8df09e8010481fbbdcb3cf','ОЦИЛИНДРОВАННОЕ БРЕВНО\r\nОЦИЛИНДРОВАННОЕ БРЕВНО \rСТРОИТЕЛЬСТВО ДОМА ИЗ ОЦИЛИНДРОВАННОГО БРЕВНА ПО СВОЕЙ ЦЕНЕ ВЫГОДНО ОТЛИЧАЕТСЯ СРЕДИ ДЕРЕВЯННЫХ ДОМОВ, ПОСТРОЕННЫХ ПО ДРУГИМ ТЕХНОЛОГИЯМ. ФИРМА КЕДР РЕАЛИЗУЕТ ОЦИЛИНДРОВАННОЕ БРЕВНО ПО ЦЕНЕ 8 900 РУБ/М3. \rДЛЯ СРАВНЕНИЯ КЛЕЕНЫЙ БРУС В НАШЕЙ ФИРМЕ СТОИТ 26 000 РУБ/М3. В СРЕДНЕМ НА СТРОИТЕЛЬСТВО ОДНОГО ДОМА ИСПОЛЬЗУЕТСЯ 50 КУБОВ СТРОЙМАТЕРИАЛА. ПРИ ДАННОЙ КУБАТУРЕ ДОМ ИЗ БРЕВНА ЭКОНОМИТ 875 000 РУБЛЕЙ. ОДНАКО НУЖНО ПОНИМАТЬ, ЧТО ОЦИЛИНДРОВАННОЕ БРЕВНО — ЭТО МАТЕРИАЛ ЕСТЕСТВЕННОЙ ВЛАЖНОСТИ И В ОТЛИЧИЕ ОТ КЛЕЕНОГО БРУСА ПОДВЕРЖЕН ДЕФОРМАЦИИ И УСУШКЕ, ЧТО В ПОСЛЕДСТВИИ ВЫЗЫВАЕТ ТРЕЩИНЫ В БРЕВНЕ.\nСТОИТ ЛИ ЭТОГО БОЯТЬСЯ? ОТВЕТ НА ЭТОТ ВОПРОС У КАЖДОГО СВОЙ. КТО-ТО ИЩЕТ ВЫГОДУ В ЦЕНЕ ДОМА, ДЛЯ ДРУГИХ ВАЖНЕЕ КАЧЕСТВО И ЭСТЕТИКА, НО МЫ СМЕЛО МОЖЕМ СКАЗАТЬ, ЧТО ФИРМА КЕДР РАБОТАЕТ С ДАННЫМ МАТЕРИАЛОМ УЖЕ БОЛЕЕ 20 ЛЕТ. ЗА ЭТИ ГОДЫ МЫ ОТРАБОТАЛИ АЛГОРИТМЫ ПО НАРУЖНОЙ И ВНУТРЕННЕЙ ОТДЕЛКЕ. БОЛЕЕ ПОДРОБНУЮ ИНФОРМАЦИЮ ВЫ МОЖЕТЕ УЗНАТЬ У НАШИХ МЕНЕДЖЕРОВ. ВАМ РАССКАЖУТ КАКИЕ МАТЕРИАЛЫ НАША ФИРМА ИСПОЛЬЗУЕТ В СТРОИТЕЛЬСТВЕ ДЕРЕВЯННЫХ ДОМОВ. КАКИЕ РАБОТЫ ПРОВОДЯТСЯ ПО УСТРАНЕНИЮ ДЕФЕКТОВ И ПРИДАНИЮ ОЦИЛИНДРОВАННОМУ БРЕВНУ ЭСТЕТИЧЕСКОГО ВИДА.\nНАШЕ ПРОИЗВОДСТВО \rЦЕНЫ НА ОЦИЛИНДРОВАННОЕ БРЕВНО - СОСНА / ЕЛЬ \rХАРАКТЕРИСТИКИ\nДИАМЕТР 18 СМ\nСТОИМОСТЬ ЗА М\r3\r8 800 РУБ.\rХАРАКТЕРИСТИКИ\nДИАМЕТР 20 СМ\nСТОИМОСТЬ ЗА М\r3\r8 800 РУБ.\rХАРАКТЕРИСТИКИ\nДИАМЕТР 22 СМ\nСТОИМОСТЬ ЗА М\r3\r8 900 РУБ.\rХАРАКТЕРИСТИКИ\nДИАМЕТР 24 СМ\nСТОИМОСТЬ ЗА М\r3\r9 200 РУБ.\rХАРАКТЕРИСТИКИ\nДИАМЕТР 26 СМ\nСТОИМОСТЬ ЗА М\r3\r9 200 РУБ.\rХАРАКТЕРИСТИКИ\nДИАМЕТР 28 СМ\nСТОИМОСТЬ ЗА М\r3\r9 300 РУБ.\rХАРАКТЕРИСТИКИ\nДИАМЕТР 30 СМ\nСТОИМОСТЬ ЗА М\r3\r9 300 РУБ.\r*НЕСТАНДАРТНЫЕ ДЕТАЛИ СРУБА (ДЛИНА БОЛЕЕ 6М)- НАЦЕНКА 100%\r*ДЕТАЛИ ЭРКЕРОВ С ВЫПУСКОМ БРЕВНА – НАЦЕНКА 50%\r*ДЕТАЛИ ЭРКЕРОВ ВСТЫК – НАЦЕНКА 30%\nОЦИЛИНДРОВАННОЕ БРЕВНО\rПЛЮСЫ\nКАЖУЩАЯСЯ ДЕШЕВИЗНА НА НАЧАЛЬНОМ ЭТАПЕ.\nМИНУСЫ\nНЕЛЬЗЯ ПРИСТУПАТЬ К ОТДЕЛОЧНЫМ РАБОТАМ ОКОЛО ГОДА ПОКА СРУБ НЕ ДАСТ УСАДКУ, ОЧЕНЬ СИЛЬНО ТРЕСКАЕТСЯ, ТРЕБУЕТ В ДАЛЬНЕЙШЕМ СЕРЬЁЗНЫХ ЗАТРАТ НА КОНОПАТКУ И ШЛИФОВКУ, ТЕПЛОВОЙ ШОВ РАВЕН ТОЛЬКО ПОЛОВИНЕ ДИАМЕТРА БРЕВНА.\r\n'),(76,'024e493d6fd699121175d161dde14e0d','УТЕПЛЕННЫЙ КЛЕЕНЫЙ БРУС\r\nУТЕПЛЕННЫЙ КЛЕЕНЫЙ БРУС\nНАША КОМПАНИЯ УЖЕ БОЛЕЕ 20 ЛЕТ ЗАНИМАЕТСЯ СТРОИТЕЛЬСТВОМ ДЕРЕВЯННЫХ ДОМОВ. ПЕРВЫЕ ДОМА МЫ СТРОИЛИ ИЗ ОЦИЛИНДРОВАННОГО БРЕВНА, НО ВСЕ МЕНЯЕТСЯ, ТЕХНОЛОГИИ НЕ СТОЯТ НА МЕСТЕ. ПОСЛЕ ОЦИЛИНДРОВАННОГО БРЕВНА, НАЧАЛИ ПРОИЗВОДИТЬ ДОМА ИЗ ПРОФИЛИРОВАННОГО БРУСА. СПУСТЯ МНОГО ЛЕТ НАШ ЗАВОД НАЧАЛ ПРОИЗВОДИТЬ КЛЕЕНЫЙ БРУС, А СЕГОДНЯ МЫ РАДЫ ВАМ ПРЕДСТАВИТЬ СОВЕРШЕННО НОВЫЙ МАТЕРИАЛ ДЛЯ СТЕН: КЛЕЕНЫЙ ПОЛЫЙ БРУС. МЫ ПРОИЗВЕЛИ МНОЖЕСТВО ИСПЫТАНИЙ: НА ТЕПЛОПРОВОДНОСТЬ, ИСПЫТАНИЯ ПО НАГРУЗКАМ, И С ПОЛНОЙ УВЕРЕННОСТЬЮ ГОТОВЫ СКАЗАТЬ, ЧТО ОН ТЕПЛЕЕ В 3 РАЗА И НИЧЕМ НЕ УСТУПАЕТ ПО НЕСУЩИМ СВОЙСТВАМ ЦЕЛЬНОМУ ПРОФИЛИРОВАННМУ БРУСУ.\nНАШЕ ПРОИЗВОДСТВО\nЦЕНЫ НА УТЕПЛЕННЫЙ КЛЕЕНЫЙ БРУС - СОСНА / ЕЛЬ\nХАРАКТЕРИСТИКИ\nСЕЧЕНИЕ 220Х180 ММ.\rCРОЩЕНИЕ ЛАМЕЛИ 40 ММ.\rУТЕПЛИТЕЛЬ \"ЭКОВАТА\" + \r1600 ЗА М³.\rВЛАЖНОСТЬ (12%).\rСТОИМОСТЬ ЗА М\r2\r18 500 РУБ.\rСТОИМОСТЬ ЗА М\r3\r4 309 РУБ.\rХАРАКТЕРИСТИКИ\nСЕЧЕНИЕ 220Х180 ММ.\nCРОЩЕНИЕ ЛАМЕЛИ 60 ММ.\nУТЕПЛИТЕЛЬ \"ЭКОВАТА\" +\r1400 ЗА М³.\nВЛАЖНОСТЬ (12%).\nСТОИМОСТЬ ЗА М\r2\r21 500 РУБ.\rСТОИМОСТЬ ЗА М\r3\r5 008 РУБ.\rЧТО ТАКОЕ УТЕПЛЕННЫЙ КЛЕЕНЫЙ БРУС?\nПОЛЫЙ БРУС ПРЕДСТАВЛЯЕТ СОБОЙ ДЕРЕВЯННУЮ КОНСТРУКЦИЮ, ОЧЕНЬ ПОХОЖУЮ ВНЕШНЕ НА КЛЕЕНЫЙ БРУС, ТО ЖЕ ВРЕМЯ НАПОМИНАЮЩУЮ ТЕХНОЛОГИЮ «ДВОЙНОГО БРУСА». ТЕХНОЛОГИЯ ИЗГОТОВЛЕНИЯ КЛЕЕНОГО ПОЛНОГО БРУСА ПРАКТИЧЕСКИ АНАЛОГИЧНА ТЕХНОЛОГИИ ИЗГОТОВЛЕНИЯ КЛЕЕНЫХ ДЕРЕВЯННЫХ КОНСТРУКЦИЙ (СОКРАЩЕННО КДК) И ПРЕДСТАВЛЯЕТ СОБОЙ ДВЕ СУХИЕ СТРОГАНЫЕ ДОСКИ (ЛАМЕЛИ) СОЕДИНЕННЫЕ (СКЛЕЕННЫЕ) МЕЖДУ СОБОЙ СОЕДИНИТЕЛЬНЫМИ ЭЛЕМЕНТАМИ ИЗ СКЛЕЕННЫХ ЛАМЕЛЕЙ ДЛИНОЙ 12-15 СМ.\nРАССТОЯНИЕ МЕЖДУ ЭЛЕМЕНТАМИ ПО ДЛИНЕ БРУСА СОСТАВЛЯЕТ 60 СМ. ВСЯ ЭТА СИСТЕМА ИЗ ДОСОК И СОЕДИНИТЕЛЬНЫХ ЭЛЕМЕНТОВ «БОБЫШЕК» ПОДВЕРГАЕТСЯ ФРЕЗЕРОВАНИЮ НА ЧЕТЫРЕХСТОРОННЕМ СТАНКЕ С ПОСЛЕДУЮЩЕЙ НАРЕЗКОЙ ЧАШЕК ПОД ПРОЕКТ ДОМА. В КОНСТРУКЦИИ СТЕНЫ, ФОРМИРУЕМОЙ ДЕТАЛЯМИ ИЗ ПОЛОГО БРУСА, СОЕДИНИТЕЛЬНЫЕ ЭЛЕМЕНТЫ РАСПОЛАГАЮТСЯ СТРОГО ДРУГ НАД ДРУГОМ, ОБРАЗУЯ ВЕРТИКАЛЬНЫЕ КОЛОННЫ В СТЕНЕ ОТ ФУНДАМЕНТА ДО САМОЙ КРЫШИ.\nОЧЕНЬ ВАЖНЫМ ЯВЛЯЕТСЯ ТОТ ФАКТ, ЧТО КАК И В ПРОФИЛИРОВАННОМ БРУСЕ И В ОЦИЛИНДРОВАННОМ БРЕВНЕ, ДА И В КЛЕЕНОМ БРУСЕ, МЫ ПРИМЕНЯЕМ ПРАКТИКУ МОНТАЖА ПОЛОГО БРУСА НА ШПИЛЬКИ ДИАМЕТРОМ 12-14 ММ С ШАГОМ 2М НА ВСЮ ВЫСОТУ СТЕНЫ. ЭТО ПРИДАЕТ СТЕНЕ ДОПОЛНИТЕЛЬНУЮ КОНСТРУКТИВНУЮ ЖЕСТКОСТЬ И ПРОЧНОСТЬ ВСЕЙ КОНСТРУКЦИИ ЗДАНИЯ. КОНСТРУКЦИЯ ВЕРХНЕГО БРЕВНА В СТЕНЕ «МАУЭРЛАТА» ПРЕДПОЛАГАЕТ ВОЗМОЖНОСТЬ ПОДТЯГИВАНИЯ ГАЕК НА ШПИЛЬКАХ ПО МЕРЕ УСАДКИ СРУБА. ОБРАЗУЮЩИЕСЯ В КОНСТРУКЦИИ СТЕНЫ ВЕРТИКАЛЬНЫЕ ПОЛОСТИ РАЗМЕРОМ 60 НА 14 СМ ЗАПОЛНЯЮТСЯ ЭФФЕКТИВНЫМ УТЕПЛИТЕЛЕМ - ЭКОВАТОЙ, ЧТО ДЕЛАЕТ СТЕНУ ИЗ КЛЕЕНОГО ПОЛОГО БРУСА ОЧЕНЬ ТЕПЛОЙ. КОЭФФИЦИЕНТ ТЕПЛОВОГО СОПРОТИВЛЕНИЯ СТЕНЫ R ДОСТИГАЕТ 3,1. ДЛЯ СРАВНЕНИЯ - R СТАНДАРТНОГО КЛЕЕНОГО БРУСА РАВНО 1,2 ПРИ ТАКОЙ ЖЕ ТОЛЩИНЕ СТЕНЫ (22 СМ).\nПРЕИМУЩЕСТВА КЛЕЕНОГО ПОЛОГО БРУСА\nЛУЧШИЕ ТЕПЛОИЗОЛЯЦИОННЫЕ\nПОКАЗАТЕЛИ \nБОЛЬШАЯ ЭКОЛОГИЧНОСТЬ \nТЕХНОЛОГИЧНОСТЬ И УДОБСТВО ПРОКЛАДКИ ВСЕХ КОММУНИКАЦИЙ \nGРОЧНОСТЬ, НАДЕЖНОСТЬ И ДОЛГОВЕЧНОСТЬ СТЕН ДОМА\nДЕШЕВИЗНА ТРАНСПОРТИРОВКИ И МОНТАЖА\nОТЛИЧНАЯ ЦЕНА - КАЧЕСТВО\nЦЕНЫ И ХАРАКТЕРИСТИКИ\n \nКЛЕЕНЫЙ ПОЛЫЙ БРУС (УТЕПЛИТЕЛЬ \"ЭКОВАТА\")\nКЛЕЕНЫЙ ПОЛЫЙ БРУС (УТЕПЛИТЕЛЬ \"GUTEX\")\nОБЫЧНЫЙ КЛЕЕНЫЙ БРУС\nЦЕНА БРУСА ЗА М³\n18 500 Р\r18 500 Р\r26 000 Р\rЗАКАЧКА УТЕПЛИТЕЛЯ НА 1 М³ БРУСА\n1 600 Р\r5 800 Р\r0 Р\rЗАСВЕРЛОВКА ЭЛЕКТРОКАНАЛОВ ДЛЯ СКРЫТОЙ ЭЛЕКТРОПРОВОДКИ (ПРИМЕРНАЯ СТОИМОСТЬ НА КУБ. БРУСА)\n0 Р\r0 Р\r500 Р\rЗАСВЕРЛОВКА КАНАЛОВ ПОД ШПИЛЬКИ ДЛЯ СТЯЖКИ БРУСА СНИЗУ ДО ВЕРХУ (ПРИМЕРНАЯ СТОИМОСТЬ)\n0 Р\r0 Р\r500 Р\rИТОГО ЗА М³\n21 100 Р\r24 300 Р\r27 000 Р\rИТОГО ЗА М²\n4 682 Р\r5 660 Р\r6 289 Р\r\n'),(74,'e1d0e1ad1b395c8dc2f6e817008b3d93','ПРОФИЛИРОВАННЫЙ БРУС\r\nПРОФИЛИРОВАННЫЙ СУХОЙ БРУС\rВ ОТЛИЧИЕ ОТ КЛЕЕНОГО БРУСА ГДЕ ЗАГОТОВКА И СУШКА МАТЕРИАЛА ПРОИЗВОДИТСЯ ИЗ ДОСОК СРЕДНИМ СЕЧЕНИЕМ 45 НА 180 ММ ДЛЯ ПРОФИЛИРОВАННОГО БРУСА В СУШИЛЬНУЮ КАМЕРУ ЗАКЛАДЫВАЮТСЯ МАТЕРИАЛЫ БОЛЬШИМ СЕЧЕНИЕМ 190 НА 140 ММ. СУШКА МАТЕРИАЛА ТАКОГО СЕЧЕНИЯ ДЕЛО ХЛОПОТНОЕ, И ЧТОБЫ ПРОСУШИТЬ НЕ ТОЛЬКО ЕГО ПОВЕРХНОСТЬ, НО И ВЫСУШИТЬ ЕГО ИЗНУТРИ НЕОБХОДИМО СДЕЛАТЬ СПЕЦИАЛЬНЫЕ ПРОПИЛЫ, КОТОРЫЕ ПОМОГУТ СДЕЛАТЬ МАТЕРИАЛ ПОЛНОСТЬЮ СУХИМ.\rНА ПРОТЯЖЕНИИ ВСЕГО ВРЕМЕНИ СУШКИ МАТЕРИАЛА ДЛЯ ПРОФИЛИРОВАННОГО БРУСА, НЕОБХОДИМО ПОСТОЯННО ПРОИЗВОДИТЬ УВЛАЖНЕНИЕ, ТАК НАЗЫВАЕМУЮ ПРОПАРКУ.\rПРИ ВСЕХ НЮАНСАХ СУШКИ И ОТТОЧЕННЫХ ТЕХНОЛОГИЙ НЕЛЬЗЯ ДОБИТЬСЯ АБСОЛЮТНО ИДЕАЛЬНОЙ ГЕОМЕТРИИ МАТЕРИАЛА. НА НЕМ ОБРАЗУЮТСЯ НЕБОЛЬШИЕ ТРЕЩИНЫ И ИЗГИБЫ.\rЕСЛИ ПРОФИЛИРОВАННЫЙ БРУС СРАВНИВАТЬ С КЛЕЕНЫМ БРУСОМ, ТО БУДЕТ ЧЕСТНЫМ СРАВНИТЬ И ЦЕНУ ЭТИХ МАТЕРИАЛОВ. ЦЕНА ПРОФИЛИРОВАННОГО БРУСА ВЫГОДНО ОТЛИЧАЕТСЯ ОТ КЛЕЕНОГО БРУСА. ЭТО ЯВЛЯЕТСЯ ОСНОВНЫМ ФАКТОРОМ ДЛЯ КЛИЕНТОВ НАШЕЙ КОМПАНИИ, КОТОРЫЕ ОТДАЮТ ПРЕДПОЧТЕНИЕ ЭКОЛОГИЧНОСТИ МАТЕРИАЛА. МЫ ПРОИЗВОДИМ ЛЮБЫЕ ДЕРЕВЯННЫЕ ДОМА. ВЫБОР МАТЕРИАЛА ВСЕГДА ОСТАЕТСЯ ЗА НАШИМИ КЛИЕНТАМИ.\nБРЕВНО ИЛИ БРУС, ЧТО ЛУЧШЕ?\rЦЕНЫ НА ПРОФИЛИРОВАННЫЙ СУХОЙ БРУС - СОСНА / ЕЛЬ\rХАРАКТЕРИСТИКИ\n140 Х 140 ММ. \rС НАРЕЗКОЙ ЧАШЕК ПОД ПРОЕКТ \rВЛАЖНОСТЬ 18 - 20%\nСТОИМОСТЬ ЗА М\r3\r16 500 РУБ.\rХАРАКТЕРИСТИКИ\n190 Х 140 ММ.\rС НАРЕЗКОЙ ЧАШЕК ПОД ПРОЕКТ\rВЛАЖНОСТЬ 18 - 20%\nСТОИМОСТЬ ЗА М\r3\r16 500 РУБ.\rПРЕИМУЩЕСТВА И НЕДОСТАТКИ ПРОФИЛИРОВАННОГО БРУСА\rПРОФИЛИРОВАННЫЙ СУХОЙ БРУС\rПЛЮСЫ\nВ ОТЛИЧИЕ ОТ БРЕВНА, НЕ ТРЕБУЕТ КОНОПАТКИ, БОЛЕЕ ЛЕГКАЯ ШЛИФОВКА, НЕ НУЖНО ОТКЛАДЫВАТЬ ОТДЕЛОЧНЫЕ РАБОТЫ НА ГОД ИЗ-ЗА УСАДКИ.\nМИНУСЫ\nПОДВЕРЖЕН ДЕФОРМАЦИЯМ, ВЕРТИКАЛЬНОСТЬ СТЕН НЕ ИДЕАЛЬНА. ГОРАЗДО БОЛЬШЕ ТРЕЩИН ПО СРАВНЕНИЮ С КЛЕЕНЫМ БРУСОМ. ПРИ ПРОИЗВОДСТВЕ НЕ ВЫРЕЗАЮТСЯ ВЫПАДНЫЕ СУЧКИ И ДЕФЕКТЫ ДРЕВЕСИНЫ.\r\n'),(78,'04a500a514470b182fd2314393a2c1d4','РЕКВИЗИТЫ КОМПАНИИ\r\nРЕКВИЗИТЫ КОМПАНИИ\rООО «ИСКОНИ»\rИНН 7708335818\rКПП 770801001\rОГРН 1187746885259\r107078, Г. МОСКВА, 1-Й БАСМАННЫЙ ПЕР., Д. 5/20, СТР.2, К 35\rР/С 40702810301890000781\rБАНК АО \"АЛЬФА-БАНК\"\rБИК БАНКА 044525593\rООО «МОДНЫЙ ДВОРИК»\rИНН7703551452\rКПП 770801001\rОГРН 1057747030846\r107078, Г. МОСКВА, 1-Й БАСМАННЫЙ ПЕР., Д. 5/20, СТР.2, К 36\rР/С 40702810301890000833\rБАНК АО \"АЛЬФА-БАНК\"\rБИК БАНКА 044525593\rРУКОВОДИТЕЛЬ\rМАЛЫГИН АЛЕКСАНДР ВЕНИАМИНОВИЧ\rКОНТАКТЫ\rПОЧТОВЫЙ АДРЕС: 107078, Г. МОСКВА, 1-Й БАСМАННЫЙ ПЕР., Д. 5/20, СТР.2,\nК 35\rТЕЛЕФОНЫ: +7 (495) 744 78 99\r8 800 222 35 77 (БЕСПЛАТНЫЙ)\rE-MAIL: INFO@ISKONI.RU\r\n'),(25,'6f89071cfd59e694c21a0edc302379fb','О КОМПАНИИ\r\nКОРОТКО О КОМПАНИИ\r«ИСКОНИ» – МОСКОВСКОЕ ПРЕДСТАВИТЕЛЬСТВО КРУПНЕЙШЕЙ В ТАТАРСТАНЕ СТРОИТЕЛЬНО-ПРОИЗВОДСТВЕННОЙ КОМПАНИИ «КЕДР», КОТОРАЯ С 1995 ГОДА ЗАНИМАЕТСЯ СТРОИТЕЛЬСТВОМ ДЕРЕВЯННЫХ ДОМОВ ИЗ ОЦИЛИНДРОВАННОГО БРЕВНА, ПРОФИЛИРОВАННОГО И КЛЕЕНОГО БРУСА СОБСТВЕННОГО ПРОИЗВОДСТВА.\nПОЧЕМУ «ИСКОНИ» И ЧТО ЭТО ЗНАЧИТ\r«ИСКОНИ» – С ДРЕВНЕРУССКОГО ОЗНАЧАЕТ «С ДАВНИХ, НЕЗАПАМЯТНЫХ ВРЕМЕН». СОБСТВЕННЫЙ ДОМ ИСКОНИ БЫЛ СИМВОЛОМ НАДЕЖНОСТИ, ОСНОВАТЕЛЬНОСТИ И СОСТОЯТЕЛЬНОСТИ. ДОМ – ЭТО ОСНОВА НАШЕЙ ЖИЗНИ И ПРОДОЛЖЕНИЯ РОДА, КРЕПКИЙ И НАДЕЖНЫЙ ТЫЛ. СВОЙ ДОМ – ЭТО ОБРАЗ ЖИЗНИ.\nДОМ – ЭТО СЛОЖНАЯ ЭКОСИСТЕМА, СОЗДАТЬ И РАЗОБРАТЬСЯ В КОТОРОЙ НАША ОБЩАЯ И ОЧЕНЬ УВЛЕКАТЕЛЬНАЯ ЗАДАЧА! ДОМ СТОИТ НА ЗЕМЛЕ, К ДОМУ ПОСТУПАЮТ ТЕПЛО И СВЕТ. В ДОМЕ НАХОДЯТСЯ ВАШИ БЛИЗКИЕ И САМЫЕ ЛЮБИМЫЕ ЛЮДИ, В ДОМЕ ТЕПЛО И УЮТНО, ИЗ КУХНИ СЛЫШЕН ВКУСНЫЙ ЗАПАХ СВЕЖЕЙ ВЫПЕЧКИ, А ИЗ-ЗА ОКНА ИЗ САДА ДОНОСИТСЯ СЧАСТЛИВЫЙ ДЕТСКИЙ СМЕХ. ВСЕ ЧТО НАХОДИТСЯ ВОКРУГ ДОМА И ВНУТРИ НЕГО СОЗДАЮТ СРЕДУ ОБИТАНИЯ ЧЕЛОВЕКА, ЕГО ВНУТРЕННИЙ МИР, КОТОРЫЙ ДОЛЖЕН БЫТЬ В ГАРМОНИИ С ОКРУЖАЮЩИМ.\nНАША РАБОТА – ЭТО СОВМЕСТНОЕ С ВАМИ СОЗДАНИЕ ОСОБОЙ ФОРМЫ ПОЗНАНИЯ МИРА – СВОЕГО ДОМА. ПОЭТОМУ МЫ = ВЫ, А ВМЕСТЕ МЫ ИСКОНИ.\n«ИСКОНИ» В ЛИЦАХ\rКАРИМОВ ТАГИР\rОСНОВАТЕЛЬ ПРОИЗВОДСТВЕННОЙ КОМПАНИИ «КЕДР».\nИДЕЙНЫЙ ВДОХНОВИТЕЛЬ КОМПАНИИ «ИСКОНИ».\nИМЕЕТ 25-ТИ ЛЕТНИЙ ОПЫТ СТРОИТЕЛЬСТВА ДОМОВ ИЗ ДЕРЕВА. БАЗОВОЕ ОБРАЗОВАНИЕ, ПОЛУЧЕННОЕ В АКАДЕМИИ ИМ. МОЖАЙСКОГО, В СОВОКУПНОСТИ С ПРАКТИЧЕСКИМ ПОДХОДОМ ВТОРОГО ОБРАЗОВАНИЯ ПОВОЛЖСКОГО ГОСУДАРСТВЕННОГО ТЕХНОЛОГИЧЕСКОГО УНИВЕРСИТЕТА, ПОЗВОЛЯЮТ ЕМУ ВСЕГДА НАХОДИТЬСЯ НА ПИКЕ СОВРЕМЕННЫХ, АКТУАЛЬНЫХ И ВОСТРЕБОВАННЫХ ТЕХНОЛОГИЙ В СФЕРЕ ДЕРЕВЯННОГО ДОМОСТРОЕНИЯ. АВТОР СОБСТВЕННЫХ УНИКАЛЬНЫХ ЗАПАТЕНТОВАННЫХ ТЕХНОЛОГИЙ В ОБЛАСТИ ПРОИЗВОДСТВА БРУСА. УВЛЕКАЕТСЯ ФУТБОЛОМ, ПРОФЕССИОНАЛЬНО ИГРАЕТ В БИЛЬЯРД.\nМАЛЫГИН АЛЕКСАНДР\rГЕНЕРАЛЬНЫЙ ДИРЕКТОР КОМПАНИИ «ИСКОНИ».\nКАНДИДАТ ТЕХНИЧЕСКИХ НАУК.\nПОДПОЛКОВНИК ЗАПАСА ВОЕННО-КОСМИЧЕСКИХ СИЛ РОССИЙСКОЙ ФЕДЕРАЦИИ. НА ЗАРЕ КАРЬЕРЫ СЛУЖИЛ НА КОСМОДРОМЕ «ПЛЕСЕЦК». ИМЕЛ ПРЯМОЕ ОТНОШЕНИЕ К ПОДГОТОВКЕ И ЗАПУСКУ КОСМИЧЕСКИХ АППАРАТОВ. ПО ОКОНЧАНИИ СЛУЖБЫ ЗАКОНЧИЛ РЭА ИМ. ПЛЕХАНОВА. ПРИНЕС В ДЕРЕВЯННОЕ ДОМОСТРОИТЕЛЬСТВО КОСМИЧЕСКУЮ ТОЧНОСТЬ НА ВСЕХ ЭТАПАХ ПРОИЗВОДСТВА. УВЛЕКАЕТСЯ СПОРТОМ И КОЛЛЕКЦИОНИРОВАНИЕМ.\nМАЛЫГИН АЛЕКСЕЙ\rДИРЕКТОР ПО РАЗВИТИЮ КОМПАНИИ «ИСКОНИ».\nИМЕЕТ ДВА ВЫСШИХ ОБРАЗОВАНИЯ: МГТУ ИМ. БАУМАНА И МГЮА ИМ. КУТАФИНА, ЧТО ПОЗВОЛЯЕТ ПРЕКРАСНО РАЗБИРАТЬСЯ В ТЕХНИЧЕСКИХ НЮАНСАХ СТРОИТЕЛЬСТВА ДОМОВ ИЗ КЛЕЕНОГО БРУСА, НЕ УПУСТИТЬ НИ ОДНОЙ ДЕТАЛИ И ДОГОВОРИТЬСЯ ПО ВСЕМ ВОПРОСАМ С ПАРТНЕРАМИ И ЗАКАЗЧИКАМИ. ПОМИМО РАЗВИТИЯ КОМПАНИИ «ИСКОНИ» ПРОФЕССИОНАЛЬНО ЗАНИМАЕТСЯ ОРГАНИЗАЦИЕЙ МЕРОПРИЯТИЙ И ПУТЕШЕСТВИЙ. УВЛЕКАЕТСЯ ВОДНЫМ И ГОРНОЛЫЖНЫМ СПОРТОМ.\nСАБИРОВА ЭЛЬМИРА\rАРХИТЕКТОР КОМПАНИИ «ИСКОНИ».\nЗАКОНЧИЛА КАЗАНСКИЙ ГОСУДАРСТВЕННЫЙ АРХИТЕКТУРНО-СТРОИТЕЛЬНЫЙ УНИВЕРСИТЕТ (КИСИ). КВАЛИФИКАЦИЯ : АРХИТЕКТОР-БАКАЛАВР ПО СПЕЦИАЛЬНОСТИ «АРХИТЕКТОР». ВЛАДЕЕТ МНОГИМИ ПРОФЕССИОНАЛЬНЫМИ АРХИТЕКТУРНЫМИ ПРОГРАММАМИ. С ПОМОЩЬЮ КОТОРЫХ ПОМОЖЕТ ВОПЛОТИТЬ В РЕАЛЬНОСТЬ ДОМ ВАШЕЙ МЕЧТЫ ОТ ЭСКИЗНЫХ РЕШЕНИЙ ДО РАБОЧИХ ЧЕРТЕЖЕЙ. БОЛЕЕ 4Х ЛЕТ ОПЫТА РАБОТЫ В ПРОЕКТИРОВАНИИ МАЛОЭТАЖНЫХ СТРОЕНИЙ. УВЛЕКАЕТСЯ ЖИВОПИСЬЮ, ФОТОГРАФИЕЙ И СПОРТОМ.\nСАФИУЛЛИН РАШИТ\rПРОИЗВОДИТЕЛЬ РАБОТ.\nПРОФЕССИОНАЛЬНЫЙ СТРОИТЕЛЬ. ЗАКОНЧИЛ КАЗАНСКИЙ СТРОИТЕЛЬНЫЙ ТЕХНИКУМ (НЫНЕ КАЗАНСКИЙ СТРОИТЕЛЬНЫЙ КОЛЛЕДЖ),- ВЕДУЩЕЕ В ТАТАРСТАНЕ ГОСУДАРСТВЕННОЕ ОБРАЗОВАТЕЛЬНОЕ УЧРЕЖДЕНИЕ ПО ПОДГОТОВКЕ ПРОФЕССИОНАЛЬНЫХ КАДРОВ В ОБЛАСТИ СТРОИТЕЛЬСТВА. ИМЕЕТ БОЛЕЕ ЧЕМ 30-ЛЕТНИЙ ОПЫТ СТРОИТЕЛЬСТВА ПРОМЫШЛЕННЫХ ОБЪЕКТОВ И ИНДИВИДУАЛЬНОГО ДОМОСТРОЕНИЯ. В «ПОСЛУЖНОМ СПИСКЕ» РАШИТА – БОЛЕЕ ТРЕХ СОТЕН ПОСТРОЕННЫХ ДОМОВ ИЗ КЛЕЕНОГО БРУСА. ОДНИМ ИЗ ПЕРВЫХ ОСВОИЛ ИННОВАЦИОННЫЙ ПРОДУКТ НАШЕЙ КОМПАНИИ– УТЕПЛЕННЫЙ КЛЕЕНЫЙ БРУС. НАДЕЖНОСТЬ, ОСНОВАТЕЛЬНОСТЬ, ВЫСОКОЕ КАЧЕСТВО СДАВАЕМЫХ ОБЪЕКТОВ,- ЕГО ВИЗИТНАЯ КАРТОЧКА. УВЛЕКАЕТСЯ ХОККЕЕМ.\nМАЗИН АРТЕМ\rАРХИТЕКТОР КОМПАНИИ «ИСКОНИ».\nВ ДЕРЕВЯННОМ ДОМОСТРОЕНИИ БОЛЕЕ 20 ЛЕТ. ЛАУРЕАТ АРХИТЕКТУРНОГО КОНКУРСА ЗАГОРОДНЫЙ ДОМ 21 ВЕКА (ОТ WEG)\n- 1988Г. – ОКОНЧИЛ МГСУ (МОСКОВСКИЙ ГОСУДАРСТВЕННЫЙ СТРОИТЕЛЬНЫЙ УНИВЕРСИТЕТ ИМ.КУЙБЫШЕВА)\r- 1999Г. – НАЧАЛО РАБОТЫ В ДЕРЕВЯННОМ ДОМОСТРОЕНИИ.\nЧТО МЫ ДЕЛАЕМ\rСОБСТВЕННЫЕ УНИКАЛЬНЫЕ ТЕХНОЛОГИИ ПО ПРОИЗВОДСТВУ УТЕПЛЕННОГО КЛЕЕНОГО БРУСА, А ТАКЖЕ ПЕРЕДОВЫЕ НЕМЕЦКИЕ И АВСТРИЙСКИЕ РАЗРАБОТКИ ПОЗВОЛЯЮТ НАМ ВОЗВОДИТЬ ДОМА С УЛУЧШЕННЫМИ ЭКСПЛУАТАЦИОННЫМИ И ЭКОЛОГИЧЕСКИМИ ПАРАМЕТРАМИ. ЧТО ТАКЖЕ СОЗДАЕТ ЗНАЧИТЕЛЬНОЕ ПРЕИМУЩЕСТВО В ЦЕНООБРАЗОВАНИИ.\nСПЕКТР РАБОТ ПОМИМО СТРОИТЕЛЬСТВА ДОМОВ\rПРИ ПОСТРОЙКЕ ДОМОВ ИЗ КЛЕЕНОГО БРУСА, ПОМИМО НЕПОСРЕДСТВЕННОГО СТРОИТЕЛЬСТВА И МОНТАЖА ИНЖЕНЕРНЫХ СИСТЕМ, МЫ ПРЕДЛАГАЕМ ЦЕЛЫЙ СПЕКТР УСЛУГ ПО РАЗРАБОТКЕ ДИЗАЙН-ПРОЕКТОВ, СОЗДАНИЮ И ДЕКОРИРОВАНИЮ ВНУТРЕННЕГО ИНТЕРЬЕРА, ЛАНДШАФТНОМУ ДИЗАЙНУ И ПРИУСАДЕБНОМУ СТРОИТЕЛЬСТВУ.\nМЫ ОСУЩЕСТВЛЯЕМ ГАРАНТИЙНОЕ И ПОСТГАРАНТИЙНОЕ ОБСЛУЖИВАНИЕ СВОИХ ОБЪЕКТОВ, А ТАКЖЕ РАЗРАБОТАЛИ РЯД УНИКАЛЬНЫХ СЕРВИСОВ, НАПРАВЛЕННЫХ НА УЛУЧШЕНИЕ КАЧЕСТВА ЖИЗНИ НА ЧАСТНОЙ ТЕРРИТОРИИ БЛИЗ ТАКОГО КРУПНОГО МЕГАПОЛИСА, КАК МОСКВА.\nФИЛОСОФИЯ СВОЕГО ДОМА\rОСНОВНОЕ ВНИМАНИЕ КОМПАНИЯ «ИСКОНИ» УДЕЛЯЕТ СОЗДАНИЮ КОМФОРТНОЙ, УЮТНОЙ И ЭКОЛОГИЧНОЙ ВО ВСЕХ СМЫСЛАХ АТМОСФЕРЫ ДЛЯ ВЫСОКОГО УРОВНЯ ЖИЗНИ И ОТДЫХА В СОБСТВЕННОМ ДОМЕ.\r\n'),(125,'137485bc6e68dd56e6dc86cdc6be313b','ВЫСТАВКА В КРОКУС ЭКСПО «ДЕРЕВЯННЫЙ ДОМ. ВЕСНА 2020»\r\nКОМПАНИЯ ISKONI ПРИНИМАЕТ УЧАСТИЕ В МЕЖДУНАРОДНОЙ ВЫСТАВКЕ «ДЕРЕВЯННЫЙ ДОМ.\r\n ВЕСНА 2020» В КРОКУС ЭКСПО С 12 ПО 15 МАРТА. \r\n\rКОМПАНИЯ ISKONI ПРИНИМАЕТ УЧАСТИЕ В МЕЖДУНАРОДНОЙ ВЫСТАВКЕ «ДЕРЕВЯННЫЙ ДОМ. \rВЕСНА 2020» В КРОКУС ЭКСПО С 12 ПО 15 МАРТА. \rНА ВЫСТАВКЕ МЫ ПРЕДСТАВЛЯЕМ НАШЕ НОУ-ХАУ В ОБЛАСТИ ДЕРЕВЯННОГО ДОМОСТРОЕНИЯ — УТЕПЛЕННЫЙ КЛЕЕНЫЙ БРУС.\rТАКЖЕ МЫ ПОЗНАКОМИМ ВАС С ЛУЧШИМИ ПРОЕКТАМИ НАШЕЙ КОМПАНИИ.\rПРИГЛАШАЕМ ВАС ПОСЕТИТЬ НАШ СТЕНД - ЗАЛ №8, СТЕНД №Д47-2 И ОБСУДИТЬ ВСЕ ВОПРОСЫ СТРОИТЕЛЬСТВА ДОМА ВАШЕЙ МЕЧТЫ!\rДЛЯ БЕСПЛАТНОГО ПОСЕЩЕНИЯ НЕОБХОДИМО ЗАРЕГИСТРИРОВАТЬСЯ НА САЙТЕ ВЫСТАВКИ — \rHTTPS://SIGNUP.WEG.RU/ANKETA.PHP\r\n'),(126,'d06daf025b4e8dc40c080ee48cc6770f','СЕДЬМОЕ НЕБО\r\n\r\n'),(127,'85b76975337388f2d6df7751d480547b','ОЛИВА\r\n\r\n'),(128,'2a8da0afda940e6a922f60e59f341ad2','ГАРМОНИЯ\r\n\r\n'),(129,'f16287c44386bd64b26dc681ab334806','ДЖЕК\r\n\r\n'),(130,'f1c722106a5562e8e4916fd12fab5612','САПСАН\r\n\r\n'),(86,'c272110840c5b5c6d7a8f7ae343938c4','ВИДЕО-ОБЗОР ТИПОВОГО ПРОЕКТА «УДАЧА +».\r\nВИДЕО-ОБЗОР ТИПОВОГО ПРОЕКТА «УДАЧА +». ЭТОТ ДЕРЕВЯННЫЙ ДОМ В 3 РАЗА ТЕПЛЕЕ И ТИШЕ, БЛАГОДАРЯ НАШЕЙ НОВЕЙШЕЙ ТЕХНОЛОГИИ\rУТЕПЛЕННОГО КЛЕЕНОГО БРУСА.\rДОМА С ВЫГОДОЙ ДО 30% ПО УНИКАЛЬНОЙ ТЕХНОЛОГИИ УТЕПЛЕННОГО БРУСА ТОЛЬКО С &QUOT;ИСКОНИ&QUOT;!\r\r\n\rЭТОТ ДЕРЕВЯННЫЙ ДОМ В 3 РАЗА ТЕПЛЕЕ И ТИШЕ, БЛАГОДАРЯ НАШЕЙ НОВЕЙШЕЙ ТЕХНОЛОГИИ УТЕПЛЕННОГО КЛЕЕНОГО БРУСА. ХОТИТЕ ТАКОЙ ЖЕ? НАПИШИТЕ НАМ!\r \rДОМ ДЛЯ МОЛОДОЙ СЕМЬИ В КОТТЕДЖНОМ ПОСЕЛКЕ \"ЭКО ОЗЕРО\" МЫ СТРОИЛИ РОВНО 1 ГОД.\rОБЩАЯ ПЛОЩАДЬ ДОМА – 130 КВ.М. \rИЗ НИХ 108 КВ.М. – ЖИЛАЯ И 22 КВ.М. – ТЕРРАСА И БАЛКОН.\rОБЩАЯ СТОИМОСТЬ ДОМА 4,5 МЛН. РУБЛЕЙ.\r \rСКОЛЬКО ПРЕИМУЩЕСТВ СРАЗУ ПОЛУЧИЛИ НАШИ ЗАКАЗЧИКИ ВМЕСТЕ С ЭТИМ ДОМОМ:\r- ЭКОЛОГИЧНОЕ ПРОСТРАНСТВО ДЛЯ ЖИЗНИ;\r- ОТСУТСТВИЕ СОСЕДЕЙ;\r- ОКНА НА ВСЕ СТОРОНЫ СВЕТА; \r- ОТДЕЛЬНОЕ ПАРКОВОЧНОЕ МЕСТО; \r- ПЕРСОНАЛЬНУЮ ДЕТСКУЮ ПЛОЩАДКУ И ЗОНУ БАРБЕКЮ ПРЯМ ПОД ОКНОМ. \r ДОМА С ВЫГОДОЙ ДО 30% ПО УНИКАЛЬНОЙ ТЕХНОЛОГИИ УТЕПЛЕННОГО БРУСА ТОЛЬКО С \"ИСКОНИ\"!\rВИДЕОКАНАЛ ISKONI, ПОДПИШИСЬ НА НОВОСТИ ТУТ: \rHTTPS://CLC.TO/ISKONITUBE\r\n'),(90,'9b387344f983f986b81a0cccdc43dac4','НОВОГОДНИЙ ПОДАРОК ОТ КОМПАНИИ ИСКОНИ\r\nНОВОГОДНИЙ ПОДАРОК ОТ КОМПАНИИ ИСКОНИ. В НАШЕЙ КОМПАНИИ ВЕСЬ ЯНВАРЬ ДЕЙСТВУЮТ ЦЕНЫ 2019 ГОДА.\r\nВЫГОДНАЯ ЦЕНА НА ВСЮ ЛИНЕЙКУ УТЕПЛЕННОГО КЛЕЕНОГО БРУСА. \r\n\r\n\rНОВОГОДНИЙ ПОДАРОК ОТ КОМПАНИИ ИСКОНИ. В НАШЕЙ КОМПАНИИ ВЕСЬ ЯНВАРЬ ДЕЙСТВУЮТ ЦЕНЫ 2019 ГОДА.\rВЫГОДНАЯ ЦЕНА НА ВСЮ ЛИНЕЙКУ УТЕПЛЕННОГО КЛЕЕНОГО БРУСА. \rДО 31.01.2020 МОЖНО СДЕЛАТЬ ЗАКАЗ НА ПОСТРОЙКУ ДОМА ИЗ УТЕПЛЕННОГО КЛЕЕНОГО БРУСА СО СКИДКОЙ 15%!\rИНФОРМАЦИЮ О ДЕЙСТВУЮЩИХ ЦЕНАХ СМОТРИТЕ В РАЗДЕЛЕ \r«УТЕПЛЕННЫЙ КЛЕЕНЫЙ БРУС».\r\n'),(92,'93847bb32ed8d67ca508fc4d61dd313f','ИСПЫТАНИЯ УТЕПЛЕННОГО БРУСА НА ПРОЧНОСТЬ\r\nДАННАЯ ТЕХНОЛОГИЯ ПОЗВОЛЯЕТ НАМ СТРОИТЬ ДОМА В 3 РАЗА ТЕПЛЕЕ И ТИШЕ, А ТАКЖЕ НА 10% ДЕШЕВЛЕ, ЧЕМ У КОНКУРЕНТОВ. ПОДРОБНЕЕ ОЗНАКОМИТЬСЯ С ДАННОЙ ТЕХНОЛОГИЕЙ ВЫ МОЖЕТЕ У НАС НА САЙТЕ В РАЗДЕЛЕ «УТЕПЛЕННЫЙ БРУС».\r\n\rПРЕДСТАВЛЯЕМ УНИКАЛЬНУЮ РАЗРАБОТКУ НАШЕГО ПРОИЗВОДСТВА - КОМПАНИИ КЕДР - УТЕПЛЕННЫЙ КЛЕЕНЫЙ БРУС.\r \rНАШИ ЗАКАЗЧИКИ СОМНЕВАЮТСЯ В ПРОЧНОСТИ ЭТОЙ ТЕХНОЛОГИЙ. В ДАННОМ ВИДЕО ВЫ МОЖЕТЕ ОЗНАКОМИТЬСЯ С ИСПЫТАНИЯМИ УТЕПЛЕННОГО КЛЕЕНОГО БРУСА. РАЗРУШЕНИЕ ПРОИСХОДИТ ПРИ ДАВЛЕНИИ 10 МПА. \rПРЕДСТАВЬТЕ СЕБЕ, ЧТО НА СТЕНУ ИЗ УТЕПЛЕННОГО КЛЕЕНОГО БРУСА ОДНОВРЕМЕННО ЗАБРАЛАСЬ СТАЯ ИЗ 240 СЛОНОВ, ... ИЛИ ПРИПАРКОВАЛИСЬ 40 ТАНКОВ Т-34. ДАЖЕ С ТАКОЙ НАГРУЗКОЙ У ЭТОЙ СТЕНЫ ОСТАЕТСЯ ЕЩЕ 10-КРАТНЫЙ ЗАПАС ПРОЧНОСТИ! ВОТ ЧТО ТАКОЕ ДАВЛЕНИЕ РАЗРУШЕНИЯ 10 МЕГА ПАСКАЛЕЙ.\r \rДАННАЯ ТЕХНОЛОГИЯ ПОЗВОЛЯЕТ НАМ СТРОИТЬ ДОМА В 3 РАЗА ТЕПЛЕЕ И ТИШЕ, А ТАКЖЕ НА 10% ДЕШЕВЛЕ, ЧЕМ У КОНКУРЕНТОВ. ПОДРОБНЕЕ ОЗНАКОМИТЬСЯ С ДАННОЙ ТЕХНОЛОГИЕЙ ВЫ МОЖЕТЕ У НАС НА САЙТЕ В РАЗДЕЛЕ «УТЕПЛЕННЫЙ БРУС».\r\n'),(95,'33f9f2fa06058b6fe71a2e1af63f5868','КУРС ВАЛЮТ\r\n\r\n'),(107,'af70b756e4cfc24164bed7ae0ed88278','ОПИСАНИЕ ТЕХНОЛОГИИ УТЕПЛЕННОГО КЛЕЕНОГО БРУСА\r\nОПИСАНИЕ ТЕХНОЛОГИИ УТЕПЛЕННОГО КЛЕЕНОГО БРУСА\r\n'),(91,'a21d8b004cd02a93bb858da7df959cba','ИСПЫТАНИЯ УТЕПЛЕННОГО КЛЕЕНОГО БРУСА\r\nДАННАЯ ТЕХНОЛОГИЯ ПОЗВОЛЯЕТ НАМ СТРОИТЬ ДОМА В 3 РАЗА ТЕПЛЕЕ И ТИШЕ, А ТАКЖЕ НА 10% ДЕШЕВЛЕ, ЧЕМ У КОНКУРЕНТОВ. ПОДРОБНЕЕ ОЗНАКОМИТЬСЯ С ДАННОЙ ТЕХНОЛОГИЕЙ ВЫ МОЖЕТЕ У НАС НА САЙТЕ В РАЗДЕЛЕ «УТЕПЛЕННЫЙ БРУС».\r\r\n\rУТЕПЛЕННЫЙ КЛЕЕНЫЙ БРУС - ЭТО ИННОВАЦИОННАЯ РАЗНОВИДНОСТЬ КЛЕЕНОГО БРУСА, ПРОСТРАНСТВО МЕЖДУ ВНЕШНИМИ ЛАМЕЛЯМИ КОТОРОГО ЗАПОЛНЯЕТСЯ СПЕЦИАЛЬНЫМ УТЕПЛИТЕЛЕМ.\r \rОДНИМ ИЗ СТРАХОВ НАШИХ ЗАКАЗЧИКОВ ЯВЛЯЕТСЯ ТО, ЧТО ТАКОЙ МАТЕРИАЛ ЯВЛЯЕТСЯ БОЛЕЕ ПОЖАРООПАСНЫМ, ЧЕМ ОБЫЧНЫЙ КЛЕЕНЫЙ БРУС. В ЭТОМ ВИДЕО МЫ ПРОВЕЛИ ПРАКТИЧЕСКИЕ ЭКСПЕРИМЕНТЫ НА ПОЖАРОУСТОЙЧИВОСТЬ, КОТОРЫЕ ДОКАЗЫВАЮТ, ЧТО УТЕПЛЕННЫЙ КЛЕЕНЫЙ БРУС НИЧЕМ НЕ УСТУПАЕТ ОБЫЧНОМУ КЛЕЕНОМУ БРУСУ. \r \rДАННАЯ ТЕХНОЛОГИЯ ПОЗВОЛЯЕТ НАМ СТРОИТЬ ДОМА В 3 РАЗА ТЕПЛЕЕ И ТИШЕ, А ТАКЖЕ НА 10% ДЕШЕВЛЕ, ЧЕМ У КОНКУРЕНТОВ. ПОДРОБНЕЕ ОЗНАКОМИТЬСЯ С ДАННОЙ ТЕХНОЛОГИЕЙ ВЫ МОЖЕТЕ У НАС НА САЙТЕ В РАЗДЕЛЕ «УТЕПЛЕННЫЙ БРУС».\r\n'),(108,'a340ee71643608dd60f9c43560646af4','ВЕЛИЙ-М\r\nДОМ, ВЫПОЛНЕННЫЙ ПО ПРОЕКТУ «ВЕЛИЙ-М», ПРЕДНАЗНАЧЕН ДЛЯ ПОСТОЯННОГО ИЛИ ВРЕМЕННОГО\rПРОЖИВАНИЯ БОЛЬШОЙ СЕМЬИ.\rВХОД В ДОМ ОСУЩЕСТВЛЯЕТСЯ ЧЕРЕЗ ВМЕСТИТЕЛЬНОЕ КРЫЛЬЦО, ЗАКРЫТОЕ НАВЕСОМ. НА ПЕРВОМ\rЭТАЖЕ ИМЕЕТСЯ ВМЕСТИТЕЛЬНЫЙ ТАМБУР С ГАРДЕРОБОМ ДЛЯ ВЕРХНЕЙ ОДЕЖДЫ И ВХОДОМ В ПОДСОБНОЕ\rПОМЕЩЕНИЕ.\rКУХНЯ, СОВМЕЩЕННАЯ С ГОСТИНОЙ, РАСПОЛАГАЮТСЯ В ПРОТИВОПОЛОЖНОЙ ОТ ВХОДА ЧАСТИ ДОМА. ИЗ\rЗОНЫ ГОСТИНОЙ ИМЕЕТСЯ ПРЯМОЙ ВЫХОД НА ШИРОКУЮ ТЕРРАСУ. ТЕРРАСА ИМЕЕТ НАВЕС, ПОЗВОЛЯЮЩИЙ\rУСТРАИВАТЬ ЛЕТНИЕ ЛАНЧИ ИЛИ ЛЮБЫЕ СЕМЕЙНЫЕ МЕРОПРИЯТИЯ, ПРОВОДИМЫЕ НА ОТКРЫТОМ ВОЗДУХЕ.\rНА ВТОРОМ ЭТАЖЕ РАСПОЛАГАЮТСЯ ЧЕТЫРЕ СПАЛЬНИ И САНУЗЕЛ. ПРИ НЕОБХОДИМОСТИ ПРОЕКТ\rИМЕЕТ ВОЗМОЖНОСТЬ СДЕЛАТЬ ПЕРЕПЛАНИРОВКУ ДО ТРЕХ СПАЛЕН, ПРИЧЕМ, ОДНУ СДЕЛАТЬ СОВМЕЩЕННУЮ\rС ОТДЕЛЬНЫМ САНУЗЛОМ И ГАРДЕРОБНОЙ КОМНАТОЙ.\rДОМ ПО ПРОЕКТУ «ВЕЛИЙ-М» МОЖЕТ БЫТЬ ВЫПОЛНЕН ИЗ УТЕПЛЕННОГО КЛЕЕНОГО БРУСА, КЛЕЕНОГО\rБРУСА ИЛИ В КАРКАСНОМ ВАРИАНТЕ.\r\nДОМ, ВЫПОЛНЕННЫЙ ПО ПРОЕКТУ «ВЕЛИЙ-М», ПРЕДНАЗНАЧЕН ДЛЯ ПОСТОЯННОГО ИЛИ ВРЕМЕННОГО\rПРОЖИВАНИЯ БОЛЬШОЙ СЕМЬИ.\rВХОД В ДОМ ОСУЩЕСТВЛЯЕТСЯ ЧЕРЕЗ ВМЕСТИТЕЛЬНОЕ КРЫЛЬЦО, ЗАКРЫТОЕ НАВЕСОМ. НА ПЕРВОМ\rЭТАЖЕ ИМЕЕТСЯ ВМЕСТИТЕЛЬНЫЙ ТАМБУР С ГАРДЕРОБОМ ДЛЯ ВЕРХНЕЙ ОДЕЖДЫ И ВХОДОМ В ПОДСОБНОЕ\rПОМЕЩЕНИЕ.\rКУХНЯ, СОВМЕЩЕННАЯ С ГОСТИНОЙ, РАСПОЛАГАЮТСЯ В ПРОТИВОПОЛОЖНОЙ ОТ ВХОДА ЧАСТИ ДОМА. ИЗ\rЗОНЫ ГОСТИНОЙ ИМЕЕТСЯ ПРЯМОЙ ВЫХОД НА ШИРОКУЮ ТЕРРАСУ. ТЕРРАСА ИМЕЕТ НАВЕС, ПОЗВОЛЯЮЩИЙ\rУСТРАИВАТЬ ЛЕТНИЕ ЛАНЧИ ИЛИ ЛЮБЫЕ СЕМЕЙНЫЕ МЕРОПРИЯТИЯ, ПРОВОДИМЫЕ НА ОТКРЫТОМ ВОЗДУХЕ.\rНА ВТОРОМ ЭТАЖЕ РАСПОЛАГАЮТСЯ ЧЕТЫРЕ СПАЛЬНИ И САНУЗЕЛ. ПРИ НЕОБХОДИМОСТИ ПРОЕКТ\rИМЕЕТ ВОЗМОЖНОСТЬ СДЕЛАТЬ ПЕРЕПЛАНИРОВКУ ДО ТРЕХ СПАЛЕН, ПРИЧЕМ, ОДНУ СДЕЛАТЬ СОВМЕЩЕННУЮ\rС ОТДЕЛЬНЫМ САНУЗЛОМ И ГАРДЕРОБНОЙ КОМНАТОЙ.\rДОМ ПО ПРОЕКТУ «ВЕЛИЙ-М» МОЖЕТ БЫТЬ ВЫПОЛНЕН ИЗ УТЕПЛЕННОГО КЛЕЕНОГО БРУСА, КЛЕЕНОГО\rБРУСА ИЛИ В КАРКАСНОМ ВАРИАНТЕ.\r\n'),(109,'1ca57763d19e4de3a6dec065ffd99495','КОНТАКТЫ\r\nКОНТАКТЫ\rГ. МОСКВА\rМЕТРО КОМСОМОЛЬСКАЯ\rАДРЕС: 1ЫЙ БАСМАННЫЙ ПЕРЕУЛОК, Д. 5/20, СТР. 2, ОФ. 35.\rОХРАНЯЕМАЯ ПАРКОВКА НА ТЕРРИТОРИИ.\r8-495-744-78-99\r8-800-222-35-77\rПН-ПТ С 10:00 ДО 19:00.\nМЫ ВСЕГДА РАДЫ ВАС ВИДЕТЬ В НАШЕМ ОФИСЕ, С УДОВОЛЬСТВИЕМ ПОКАЖЕМ ВАМ ВЫСТАВОЧНЫЕ ОБРАЗЦЫ И ПРОКОНСУЛЬТИРУЕМ ПО ТЕХНОЛОГИЯМ СТРОИТЕЛЬСТВА, А ТАКЖЕ УГОСТИМ САМЫМ ВКУСНЫМ ЧАЕМ И КОФЕ, НО УБЕДИТЕЛЬНО ПРОСИМ ПРЕДВАРИТЕЛЬНО СОГЛАСОВАТЬ ВАШ ВИЗИТ.\nТАТАРСТАН\rПРОИЗВОДСТВО КОМПАНИИ «КЕДР»\rАДРЕС: Г. КАЗАНЬ, УЛ. ГЛАДИЛОВА, Д. 27, ОФ. 426\r8-800-222-35-77\r\n'),(110,'59c06d69b490e62ed95e5da0aa373cc7','ПОСТРОЕННЫЕ ДОМА\r\nСТРОИМ ДОМА С 1997 ГОДА\rКОМПАНИЯ ИСКОНИ – МОСКОВСКОЕ ПРЕДСТАВИТЕЛЬСТВО КАЗАНСКОЙ СТРОИТЕЛЬНОЙ КОМПАНИИ КЕДР, РАБОТАЮЩЕЙ НА РЫНКЕ С 1997 ГОДА. ЗА ЭТО ВРЕМЯ НАМИ БЫЛО ПОСТРОЕНО БОЛЕЕ 500 ДОМОВ ИЗ БРУСА РАЗНЫХ ПОРОД ДЕРЕВА (КЕДР, ЕЛЬ, СОСНА). СЕГОДНЯ МЫ СТРОИМ ДОМА ИЗ КАЧЕСТВЕННОЙ ДРЕВЕСИНЫ СРЕДНЕЙ ПОЛОСЫ. ПРИ ПРОИЗВОДСТВЕ МЫ ИСПОЛЬЗУЕМ ТОЛЬКО ЭКОЛОГИЧЕСКИ ЧИСТЫЕ МАТЕРИАЛЫ, УНИКАЛЬНЫЕ СОБСТВЕННЫЕ И ПЕРЕДОВЫЕ НЕМЕЦКИЕ ТЕХНОЛОГИИ.\nПРЕДЛАГАЕМ ВАШЕМУ ВНИМАНИЮ НЕСКОЛЬКО ЛУЧШИХ ИНДИВИДУАЛЬНЫХ ПРОЕКТОВ, ПОСТРОЕННЫХ НАШЕЙ КОМПАНИЕЙ.\r\n'),(111,'3058880894d82bbc4776bd2792378545','ПОСТРОЕННЫЕ ДОМА\r\nСТРОИМ ДОМА С 1997 ГОДА\nКОМПАНИЯ ИСКОНИ – МОСКОВСКОЕ ПРЕДСТАВИТЕЛЬСТВО КАЗАНСКОЙ СТРОИТЕЛЬНОЙ КОМПАНИИ КЕДР, РАБОТАЮЩЕЙ НА РЫНКЕ С 1997 ГОДА. ЗА ЭТО ВРЕМЯ НАМИ БЫЛО ПОСТРОЕНО БОЛЕЕ 500 ДОМОВ ИЗ БРУСА РАЗНЫХ ПОРОД ДЕРЕВА (КЕДР, ЕЛЬ, СОСНА). СЕГОДНЯ МЫ СТРОИМ ДОМА ИЗ КАЧЕСТВЕННОЙ ДРЕВЕСИНЫ СРЕДНЕЙ ПОЛОСЫ. ПРИ ПРОИЗВОДСТВЕ МЫ ИСПОЛЬЗУЕМ ТОЛЬКО ЭКОЛОГИЧЕСКИ ЧИСТЫЕ МАТЕРИАЛЫ, УНИКАЛЬНЫЕ СОБСТВЕННЫЕ И ПЕРЕДОВЫЕ НЕМЕЦКИЕ ТЕХНОЛОГИИ.\nВИДЕО-ОБЗОР ТИПОВОГО ПРОЕКТА УДАЧА+\rОПИСАНИЕ ТЕХНОЛОГИИ УТЕПЛЕННОГО КЛЕЕНОГО БРУСА\rНАШИ ЛУЧШИЕ ИНДИВИДУАЛЬНЫЕ ПРОЕКТЫ\nИНДИВИДУАЛЬНЫЙ ПРОЕКТ ДОМА\nРЕСПУБЛИКА ТАТАРСТАН\nО ПРОЕКТЕ:\rДВУХЭТАЖНЫЙ ЖИЛОЙ ДОМ ПО ИНДИВИДУАЛЬНОМУ ПРОЕКТУ. ОБЩАЯ ПЛОЩАДЬ ДОМА: 232 М2. ГОД ПОСТРОЙКИ 2009.\rДОМ ВЫПОЛНЕН ИЗ КЛЕЕНОГО БРУСА 140 (ВЫСОТА) Х 200 ММ\rГОДОМ ПОЗЖЕ БЫЛИ ПОСТРОЕНЫ БАНЯ (КЛЕЕНЫЙ БРУС 140 Х 160 ММ)\rИ ЛЕТНЯЯ КУХНЯ (КЛЕЕНЫЙ БРУС 140 Х 129 ММ)\rДОМ БЫЛ ПОСТРОЕН ДЛЯ БОЛЬШОЙ СЕМЬИ. ВМЕСТИТЕЛЬНАЯ ГОСТИНАЯ ПОЗВОЛЯЕТ СОБИРАТЬСЯ НА СЕМЕЙНЫЕ ТОРЖЕСТВА БОЛЬШОМУ ЧИСЛУ ГОСТЕЙ. БЫЛИ ПРАЗДНИКИ, КОГДА СОБИРАЛОСЬ БОЛЕЕ 50 ЧЕЛОВЕК. В ДНИ ОБЫЧНЫЕ У КАЖДОГО ЧЛЕНА СЕМЬИ ЕСТЬ СВОЙ УЮТНЫЙ УГОЛОК, ДЕТЯМ МОЖНО СПОКОЙНО ДЕЛАТЬ УРОКИ, СТУДЕНТАМ – СВОИ ПРОЕКТЫ, А ГЛАВЕ СЕМЕЙСТВА – ОТДЫХАТЬ И НАСЛАЖДАТЬСЯ СЕМЕЙНЫМ СЧАСТЬЕМ.\nИНДИВИДУАЛЬНЫЙ ПРОЕКТ ДОМА\nРЕСПУБЛИКА ТАТАРСТАН\nО ПРОЕКТЕ:\rДВУХЭТАЖНЫЙ ЖИЛОЙ ДОМ ПО ИНДИВИДУАЛЬНОМУ ПРОЕКТУ. ОБЩАЯ ПЛОЩАДЬ ДОМА: 180 М2. ГОД ПОСТРОЙКИ 2018.\rДОМ ВЫПОЛНЕН ИЗ КЛЕЕНОГО БРУСА 180 (ВЫСОТА) Х 240 ММ\rОДНОВРЕМЕННО БЫЛИ ПОСТРОЕНЫ БАНЯ И ЛЕТНЯЯ КУХНЯ (КЛЕЕНЫЙ БРУС 180 Х 160 ММ)\rЭТОТ ДОМ ДЛЯ МОЛОДОЙ СЕМЬИ, НА МОМЕНТ ПОСТРОЙКИ БЫЛ ОДИН МАЛЕНЬКИЙ РЕБЕНОК, ДЛЯ КОТОРОГО РОДИТЕЛЯМИ СОЗДАНЫ ВСЕ УСЛОВИЯ ДЛЯ ГАРМОНИЧНОГО РАЗВИТИЯ. ОЧЕВИДНО, ЧТО НЕДАЛЕК ТОТ ДЕНЬ, КОГДА У ЭТОГО РЕБЕНКА ПОЯВЯТСЯ МЛАДШИЕ БРАТЬЯ ИЛИ СЕСТРЫ: ПРОСТРАНСТВА ВСЕМ ХВАТИТ.\nИНДИВИДУАЛЬНЫЙ ПРОЕКТ ДОМА\nРЕСПУБЛИКА ТАТАРСТАН\nО ПРОЕКТЕ:\rОПИСАНИЕ: ДВУХЭТАЖНЫЙ ЖИЛОЙ ДОМ ПО ИНДИВИДУАЛЬНОМУ ПРОЕКТУ. ОБЩАЯ ПЛОЩАДЬ ДОМА: 200 М2. ГОД ПОСТРОЙКИ 2017.\rДОМ ВЫПОЛНЕН ИЗ КЛЕЕНОГО БРУСА 180 (ВЫСОТА) Х 160 ММ\rДОМ ПОСТРОЕН НА ЖИВОПИСНОМ БЕРЕГУ ВОДОЕМА НА СКЛОНЕ. В ПРОЕКТЕ УДАЛОСЬ С МАКСИМАЛЬНОЙ ЭФФЕКТИВНОСТЬЮ ИСПОЛЬЗОВАТЬ ОСОБЕННОСТИ ПРИРОДНОГО ЛАНДШАФТА. \rПЛАНИРОВКА УЧАСТКА И СОПУТСТВУЮЩИЕ ПОСТРОЙКИ ДЕЛАЮТ ЭТОТ ДОМ ЦЕНТРОМ МАЛЕНЬКОГО УЮТНОГО МИРА НА БЕРЕГУ ВЕЛИКОЙ РУССКОЙ РЕКИ. ВСЕ ВОКРУГ КАК БЫ ГОВОРИТ О ТОМ, ЧТО НАШ МИР СОЗДАН ДЛЯ СЧАСТЬЯ.\nИНДИВИДУАЛЬНЫЙ ПРОЕКТ ДОМА\nРЕСПУБЛИКА ТАТАРСТАН\nО ПРОЕКТЕ:\rДВУХЭТАЖНЫЙ ЖИЛОЙ ДОМ ПО ИНДИВИДУАЛЬНОМУ ПРОЕКТУ. ОБЩАЯ ПЛОЩАДЬ ДОМА: 190 М2. ГОД ПОСТРОЙКИ 2018.\rДОМ ВЫПОЛНЕН ИЗ КЛЕЕНОГО БРУСА 180 (ВЫСОТА) Х 200 ММ\rСОВРЕМЕННЫЙ ПРОСТОРНЫЙ ДОМ. ШИКАРНАЯ ГОСТИНАЯ С КАМИНОМ И ВЫСОТОЙ ПОТОЛКОВ БОЛЕЕ 4-Х МЕТРОВ ИМЕЕТ БОЛЬШИЕ ПАНОРАМНЫЕ ОКНА.\rВ ЭТОМ ПРОЕКТЕ, В ЕГО РЕАЛИЗАЦИИ УДАЛОСЬ СОВМЕСТИТЬ СВЯЗЬ ВРЕМЕН, ГРАНДИОЗНЫЙ ЗАМЫСЕЛ И ЕДИНЕНИЕ ВЕЛИКОГО И МАЛОГО. МНОГО СВЕТА, МНОГО ТЕПЛА. ВСЕ ВОКРУГ ГОВОРИТ ОБ УСПЕШНОСТИ И УМЕНИИ ДОСТИГАТЬ ПОСТАВЛЕННЫХ РУБЕЖЕЙ. \nИНДИВИДУАЛЬНЫЙ ПРОЕКТ ДОМА\nРЕСПУБЛИКА ТАТАРСТАН\nО ПРОЕКТЕ:\rДВУХЭТАЖНЫЙ ЖИЛОЙ ДОМ ПО ПРОЕКТУ «АЗАЛИЯ-ЛЮКС». ОБЩАЯ ПЛОЩАДЬ ДОМА: 207 М2. \rГОД ПОСТРОЙКИ 2012.\rДОМ ВЫПОЛНЕН ИЗ КЛЕЕНОГО БРУСА 180 (ВЫСОТА) Х 220 ММ\rДОМ ПОСТРОЕН ДЛЯ МНОГОДЕТНОЙ СЕМЬИ. КОМНАТЫ ВЕРХНЕГО ЭТАЖА ИМЕЮТ МАНСАРДНЫЕ ОКНА. ВНУТРЕННИЕ ПЛАНИРОВКИ ОЧЕНЬ УДАЧНО И ГАРМОНИЧНО ПРОДУМАНЫ. НЕТ НИЧЕГО ЛИШЛЕГО, НИКАКИХ ИЗЛИШЕСТВ. ВСЕ ОЧЕНЬ ФУНКЦИОНАЛЬНО, ПРАВИЛЬНО. КАЖДАЯ ВЕЩЬ – НА СВОЕМ МЕСТЕ, КАЖДОЕ ПОМЕЩЕНИЕ ТАМ, ГДЕ ЕМУ СЛЕДУЕТ БЫТЬ, КАЖДОЕ ПРОСТРАНСТВО ТАКОЕ, КАК НУЖНО. ЭТОТ ДОМ – КАК РАЗ ТОТ СЛУЧАЙ, КОГДА ВСЕ ЕСТЕСТВЕННО И ПРОДУМАНО.\nИНДИВИДУАЛЬНЫЙ ПРОЕКТ ДОМА\nРЕСПУБЛИКА ТАТАРСТАН\nО ПРОЕКТЕ:\rДВУХЭТАЖНЫЙ ЖИЛОЙ ДОМ ПО ИНДИВИДУАЛЬНОМУ ПРОЕКТУ, ПЛОЩАДЬ ДОМА 187 М2.\rГОД ПОСТРОЙКИ 2019\rДОМ ВЫПОЛНЕН ИЗ КЛЕЕНОГО БРУСА 180 Х 200 ММ\rОРИГИНАЛЬНАЯ ПЛАНИРОВКА, НЕСТАНДАРТНОЕ РЕШЕНИЕ УГЛОВОЙ КОМНАТЫ, ВЫПОЛНЕННОЕ ПО ТИПУ КРЕМЛЕВСКОЙ ШЕСТИСТЕННОЙ БАШНИ ПРИДАЮТ ДОМУ ОСОБЫЙ КОЛОРИТ.\rЭТОТ ПРОЕКТ – ВОПЛОЩЕНИЕ ДЕТСКОЙ МЕЧТЫ ДЛЯ СВОИХ ДЕТЕЙ. А КОГДА ЧЕЛОВЕК ВЫРАСТАЕТ В АТМОСФЕРЕ СКАЗКИ, В АТМОСФЕРЕ СЧАСТЬЯ, ТОЛЬКО ТОГДА ОН СМОЖЕТ ПОКОРИТЬ ЛЮБЫЕ ВЕРШИНЫ.\nИНДИВИДУАЛЬНЫЙ ПРОЕКТ ДОМА\nРЕСПУБЛИКА ТАТАРСТАН\nО ПРОЕКТЕ:\rДВУХЭТАЖНЫЙ ДАЧНЫЙ ДОМ ПО ПРОЕКТУ «УДАЧА ПЛЮС». ОБЩАЯ ПЛОЩАДЬ ДОМА 113 М2.\rГОД ПОСТОРОЙКИ 2018.\rДОМ ВЫПОЛНЕН ИЗ УТЕПЛЕННОГО КЛЕЕНОГО БРУСА 180 Х 200 ММ.\rИДЕАЛЬНОЕ ДАЧНОЕ РЕШЕНИЕ ДЛЯ МОЛОДОЙ СЕМЬИ. ВСЕМ ЕСТЬ МЕСТО, И РОДИТЕЛЯМ И ДЕТЯМ, И ГОСТЯМ. В ЭТОМ ДОМЕ МОЖНО КОМФОРТНО ПРОВОДИТЬ И ТЕПЛЫЕ ЛЕТНИЕ ВЕЧЕРА, И ХОЛОДНЫЕ ЗИМНИЕ НОЧИ. ЛЕТОМ БУДЕТ ПРОХЛАДНО, ЗИМОЙ – ТЕПЛО И ТИХО.\r\n'),(112,'b3eddc0dd570c0bc8a4dfa1d02415319','ВИДЕОТРАНСЛЯЦИЯ\r\n\r\n'),(113,'53b23aad0fbbd4321fb85a070257d55d','ЭТАПЫ СТРОИТЕЛЬСТВА\r\n\r\n'),(114,'a5503f08ad83a4291da585d66a8589f6','НОВЫЕ УВЕДОМЛЕНИЯ\r\n\r\n'),(115,'6087aeb430584fd1b325bb7951cbafa8','ОПЛАТА\r\n\r\n'),(116,'fa5748f77d638bbd055a9ae71835c34c','ФОТООТЧЕТ СТРОИТЕЛЬСТВА ДОМА\r\n\r\n'),(117,'b6025fd20df8bb0a64ca460bfcde9d3d','ДОКУМЕНТЫ\r\n\r\n'),(118,'151ba8c3843fb002b2d7e4603b771f1d','ОЦИЛИНДРОВАННОЕ БРЕВНО\r\nОЦИЛИНДРОВАННОЕ БРЕВНО\rОЦИЛИНДРОВАННОЕ БРЕВНО\rСТРОИТЕЛЬСТВО ДОМА ИЗ ОЦИЛИНДРОВАННОГО БРЕВНА ПО СВОЕЙ ЦЕНЕ ВЫГОДНО ОТЛИЧАЕТСЯ СРЕДИ ДЕРЕВЯННЫХ ДОМОВ, ПОСТРОЕННЫХ ПО ДРУГИМ ТЕХНОЛОГИЯМ. ФИРМА КЕДР РЕАЛИЗУЕТ ОЦИЛИНДРОВАННОЕ БРЕВНО ПО ЦЕНЕ 8 900 РУБ/М3. ДЛЯ СРАВНЕНИЯ КЛЕЕНЫЙ БРУС В НАШЕЙ ФИРМЕ СТОИТ 26 000 РУБ/М3. В СРЕДНЕМ НА СТРОИТЕЛЬСТВО ОДНОГО ДОМА ИСПОЛЬЗУЕТСЯ 50 КУБОВ СТРОЙМАТЕРИАЛА. ПРИ ДАННОЙ КУБАТУРЕ ДОМ ИЗ БРЕВНА ЭКОНОМИТ 875 000 РУБЛЕЙ. ОДНАКО НУЖНО ПОНИМАТЬ, ЧТО ОЦИЛИНДРОВАННОЕ БРЕВНО — ЭТО МАТЕРИАЛ ЕСТЕСТВЕННОЙ ВЛАЖНОСТИ И В ОТЛИЧИЕ ОТ КЛЕЕНОГО БРУСА ПОДВЕРЖЕН ДЕФОРМАЦИИ И УСУШКЕ, ЧТО В ПОСЛЕДСТВИИ ВЫЗЫВАЕТ ТРЕЩИНЫ В БРЕВНЕ. СТОИТ ЛИ ЭТОГО БОЯТЬСЯ? ОТВЕТ НА ЭТОТ ВОПРОС У КАЖДОГО СВОЙ. КТО-ТО ИЩЕТ ВЫГОДУ В ЦЕНЕ ДОМА, ДЛЯ ДРУГИХ ВАЖНЕЕ КАЧЕСТВО И ЭСТЕТИКА, НО МЫ СМЕЛО МОЖЕМ СКАЗАТЬ, ЧТО ФИРМА КЕДР РАБОТАЕТ С ДАННЫМ МАТЕРИАЛОМ УЖЕ БОЛЕЕ 20 ЛЕТ. ЗА ЭТИ ГОДЫ МЫ ОТРАБОТАЛИ АЛГОРИТМЫ ПО НАРУЖНОЙ И ВНУТРЕННЕЙ ОТДЕЛКЕ. БОЛЕЕ ПОДРОБНУЮ ИНФОРМАЦИЮ ВЫ МОЖЕТЕ УЗНАТЬ У НАШИХ МЕНЕДЖЕРОВ. ВАМ РАССКАЖУТ КАКИЕ МАТЕРИАЛЫ НАША ФИРМА ИСПОЛЬЗУЕТ В СТРОИТЕЛЬСТВЕ ДЕРЕВЯННЫХ ДОМОВ. КАКИЕ РАБОТЫ ПРОВОДЯТСЯ ПО УСТРАНЕНИЮ ДЕФЕКТОВ И ПРИДАНИЮ ОЦИЛИНДРОВАННОМУ БРЕВНУ ЭСТЕТИЧЕСКОГО ВИДА.\nЦЕНЫ НА ОЦИЛИНДРОВАННОЕ БРЕВНО\r- СОСНА / ЕЛЬ\rДИАМЕТР\n18 СМ\rЗА М\n8 800 РУБ.\nЗА М\n1 440 РУБ.\nДИАМЕТР\n20 СМ\rЗА М\n8 800 РУБ.\nЗА М\n1 600 РУБ.\nДИАМЕТР\n22 СМ\rЗА М\n8 900 РУБ.\nЗА М\n1 780 РУБ.\nДИАМЕТР\n24 СМ\rЗА М\n9 200 РУБ.\nЗА М\n2 000 РУБ.\nДИАМЕТР\n26 СМ\rЗА М\n9 200 РУБ.\nЗА М\n2 170 РУБ.\nДИАМЕТР\n28 СМ\rЗА М\n9 300 РУБ.\nЗА М\n2 360 РУБ.\nДИАМЕТР\n30 СМ\rЗА М\n9 300 РУБ.\nЗА М\n2 535 РУБ.\nНЕСТАНДАРТНЫЕ ДЕТАЛИ СРУБА (ДЛИНА БОЛЕЕ 6М)- НАЦЕНКА 100%\rДЕТАЛИ ЭРКЕРОВ С ВЫПУСКОМ БРЕВНА – НАЦЕНКА 50%\rДЕТАЛИ ЭРКЕРОВ ВСТЫК – НАЦЕНКА 30%\r\n'),(119,'03df217536ea10975afea99011ca9f34','КЛЕЕНЫЙ БРУС\r\nКЛЕЕНЫЙ БРУС\rЦЕНЫ НА КЛЕЕНЫЙ БРУС\r- СОСНА / ЕЛЬ\r(200, 220, 240) Х 180 ММ.\rС НАРЕЗКОЙ ЧАШЕК ПОД ПРОЕКТ\rБЕЗ СРАЩИВАНИЯ ПО ДЛИНЕ (6 М.)\rШИРИНА\r200 ММ\r220 ММ\r240 ММ\nЗА М\n23 500 РУБ.\r23 500 РУБ.\r23 500 РУБ.\rЗА М\n4 976 РУБ.\r5 474 РУБ.\r5 972 РУБ.\r(200, 220, 240) Х 270 ММ.\rС НАРЕЗКОЙ ЧАШЕК ПОД ПРОЕКТ\rШИРИНА\r200 ММ\r220 ММ\r240 ММ\nЗА М\n29 000 РУБ.\r29 000 РУБ.\r29 000 РУБ.\rЗА М\n6 023 РУБ.\r6 625 РУБ.\r7 228 РУБ.\r120 Х 180 ММ.\rС НАРЕЗКОЙ ЧАШЕК ПОД ПРОЕКТ\rВЛАЖНОСТЬ (12%)\rЗА М\n26 000 РУБ.\rЗА М\n3 304 РУБ.\r120 Х 135 ММ.\rС НАРЕЗКОЙ ЧАШЕК ПОД ПРОЕКТ\rВЛАЖНОСТЬ (12%)\rЗА М\n26 000 РУБ.\rЗА М\n3 370 РУБ.\r160 Х 180 ММ.\rС НАРЕЗКОЙ ЧАШЕК ПОД ПРОЕКТ\rВЛАЖНОСТЬ (12%)\rЗА М\n26 000 РУБ.\rЗА М\n4 405 РУБ.\r160 Х 135 ММ.\rС НАРЕЗКОЙ ЧАШЕК ПОД ПРОЕКТ\rВЛАЖНОСТЬ (12%)\rЗА М\n26 000 РУБ.\rЗА М\n4 493 РУБ.\r200 Х 135 ММ.\rС НАРЕЗКОЙ ЧАШЕК ПОД ПРОЕКТ\rВЛАЖНОСТЬ (12%)\rЗА М\n26 000 РУБ.\rЗА М\n5 616 РУБ.\r(200, 220, 240) Х 180 ММ.\rС НАРЕЗКОЙ ЧАШЕК ПОД ПРОЕКТ\rШИРИНА\r200 ММ\r220 ММ\r240 ММ\nЗА М\n26 000 РУБ.\r26 000 РУБ.\r26 000 РУБ.\rЗА М\n5 506 РУБ.\r6 056 РУБ.\r6 607 РУБ.\rЦЕНЫ НА КЛЕЕНЫЙ БРУС\r- КЕДР\r(200, 220, 240) Х 270 ММ.\r(ПОЛНОСТЬЮ ИЗ КЕДРА)\rС НАРЕЗКОЙ ЧАШЕК ПОД ПРОЕКТ\rШИРИНА\r200 ММ\r220 ММ\r240 ММ\nЗА М³\n36 000 РУБ.\r36 000 РУБ.\r36 000 РУБ.\rЗА М²\n7 477 РУБ.\r8 225 РУБ.\r8 972 РУБ.\r(200, 220, 240) Х 270 ММ.\r(ЛИЦЕВЫЕ ЛАМЕЛИ ИЗ КЕДРА)\rС НАРЕЗКОЙ ЧАШЕК ПОД ПРОЕКТ\rШИРИНА\r200 ММ\r220 ММ\r240 ММ\nЗА М³\n34 000 РУБ.\r34 000 РУБ.\r34 000 РУБ.\rЗА М²\n7 062 РУБ.\r7 768 РУБ.\r8 474 РУБ.\r(200, 220, 240) Х 180 ММ.\r(ПОЛНОСТЬЮ ИЗ КЕДРА)\rС НАРЕЗКОЙ ЧАШЕК ПОД ПРОЕКТ\rШИРИНА\r200 ММ\r220 ММ\r240 ММ\nЗА М\n34 000 РУБ.\r34 000 РУБ.\r34 000 РУБ.\rЗА М\n7 200 РУБ.\r7 920 РУБ.\r8 640 РУБ.\r(200, 220, 240) Х 180 ММ.\r(ЛИЦЕВЫЕ ЛАМЕЛИ ИЗ КЕДРА)\rС НАРЕЗКОЙ ЧАШЕК ПОД ПРОЕКТ\rШИРИНА\r200 ММ\r220 ММ\r240 ММ\nЗА М\n32 000 РУБ.\r32 000 РУБ.\r32 000 РУБ.\rЗА М\n6 776 РУБ.\r7 454 РУБ.\r8 132 РУБ.\r\n'),(120,'96493abc55eaae6a08cc9c24971bce6f','УТЕПЛЕННЫЙ КЛЕЕНЫЙ БРУС\r\nПОЛЫЙ КЛЕЕНЫЙ БРУС\rУТЕПЛЕННЫЙ КЛЕЕНЫЙ БРУС\rНАША КОМПАНИЯ УЖЕ БОЛЕЕ 20 ЛЕТ ЗАНИМАЕТСЯ СТРОИТЕЛЬСТВОМ ДЕРЕВЯННЫХ ДОМОВ. ПЕРВЫЕ ДОМА МЫ СТРОИЛИ ИЗ ОЦИЛИНДРОВАННОГО БРЕВНА, НО ВСЕ МЕНЯЕТСЯ, ТЕХНОЛОГИИ НЕ СТОЯТ НА МЕСТЕ. ПОСЛЕ ОЦИЛИНДРОВАННОГО БРЕВНА, НАЧАЛИ ПРОИЗВОДИТЬ ДОМА ИЗ ПРОФИЛИРОВАННОГО БРУСА. СПУСТЯ МНОГО ЛЕТ НАШ ЗАВОД НАЧАЛ ПРОИЗВОДИТЬ КЛЕЕНЫЙ БРУС, А СЕГОДНЯ МЫ РАДЫ ВАМ ПРЕДСТАВИТЬ СОВЕРШЕННО НОВЫЙ МАТЕРИАЛ ДЛЯ СТЕН: КЛЕЕНЫЙ ПОЛЫЙ БРУС. МЫ ПРОИЗВЕЛИ МНОЖЕСТВО ИСПЫТАНИЙ: НА ТЕПЛОПРОВОДНОСТЬ, ИСПЫТАНИЯ ПО НАГРУЗКАМ, И С ПОЛНОЙ УВЕРЕННОСТЬЮ ГОТОВЫ СКАЗАТЬ, ЧТО ОН ТЕПЛЕЕ В 3 РАЗА И НИЧЕМ НЕ УСТУПАЕТ ПО НЕСУЩИМ СВОЙСТВАМ ЦЕЛЬНОМУ ПРОФИЛИРОВАННМУ БРУСУ.\nЦЕНЫ НА КЛЕЕНЫЙ ПОЛЫЙ БРУС\r- СОСНА / ЕЛЬ\rСЕЧЕНИЕ 220Х180 ММ.\r(СРОЩЕНИЕ ЛАМЕЛИ 40 ММ.)\r(УТЕПЛИТЕЛЬ \"ЭКОВАТА\" + 1600 ЗА М³)\rВЛАЖНОСТЬ (12%)\nЗА М³ \r18 500 РУБ.\rЗА М² \r4 309 РУБ.\nСЕЧЕНИЕ 220Х180 ММ.\r(СРОЩЕНИЕ ЛАМЕЛИ 60 ММ.)\r(УТЕПЛИТЕЛЬ \"ЭКОВАТА\" + 1400 ЗА М³)\rВЛАЖНОСТЬ (12%)\nЗА М³ \r21 500 РУБ.\nЗА М² \r5 008 РУБ.\nЧТО ТАКОЕ КЛЕЕНЫЙ ПОЛЫЙ БРУС?\rПОЛЫЙ БРУС ПРЕДСТАВЛЯЕТ СОБОЙ ДЕРЕВЯННУЮ КОНСТРУКЦИЮ, ОЧЕНЬ ПОХОЖУЮ ВНЕШНЕ НА КЛЕЕНЫЙ БРУС, ТО ЖЕ ВРЕМЯ НАПОМИНАЮЩУЮ ТЕХНОЛОГИЮ «ДВОЙНОГО БРУСА». ТЕХНОЛОГИЯ ИЗГОТОВЛЕНИЯ КЛЕЕНОГО ПОЛНОГО БРУСА ПРАКТИЧЕСКИ АНАЛОГИЧНА ТЕХНОЛОГИИ ИЗГОТОВЛЕНИЯ КЛЕЕНЫХ ДЕРЕВЯННЫХ КОНСТРУКЦИЙ (СОКРАЩЕННО КДК) И ПРЕДСТАВЛЯЕТ СОБОЙ ДВЕ СУХИЕ СТРОГАНЫЕ ДОСКИ (ЛАМЕЛИ) СОЕДИНЕННЫЕ (СКЛЕЕННЫЕ) МЕЖДУ СОБОЙ СОЕДИНИТЕЛЬНЫМИ ЭЛЕМЕНТАМИ ИЗ СКЛЕЕННЫХ ЛАМЕЛЕЙ ДЛИНОЙ 12-15 СМ.\nРАССТОЯНИЕ МЕЖДУ ЭЛЕМЕНТАМИ ПО ДЛИНЕ БРУСА СОСТАВЛЯЕТ 60 СМ. ВСЯ ЭТА СИСТЕМА ИЗ ДОСОК И СОЕДИНИТЕЛЬНЫХ ЭЛЕМЕНТОВ «БОБЫШЕК» ПОДВЕРГАЕТСЯ ФРЕЗЕРОВАНИЮ НА ЧЕТЫРЕХСТОРОННЕМ СТАНКЕ С ПОСЛЕДУЮЩЕЙ НАРЕЗКОЙ ЧАШЕК ПОД ПРОЕКТ ДОМА. В КОНСТРУКЦИИ СТЕНЫ, ФОРМИРУЕМОЙ ДЕТАЛЯМИ ИЗ ПОЛОГО БРУСА, СОЕДИНИТЕЛЬНЫЕ ЭЛЕМЕНТЫ РАСПОЛАГАЮТСЯ СТРОГО ДРУГ НАД ДРУГОМ, ОБРАЗУЯ ВЕРТИКАЛЬНЫЕ КОЛОННЫ В СТЕНЕ ОТ ФУНДАМЕНТА ДО САМОЙ КРЫШИ.\nОЧЕНЬ ВАЖНЫМ ЯВЛЯЕТСЯ ТОТ ФАКТ, ЧТО КАК И В ПРОФИЛИРОВАННОМ БРУСЕ И В ОЦИЛИНДРОВАННОМ БРЕВНЕ, ДА И В КЛЕЕНОМ БРУСЕ, МЫ ПРИМЕНЯЕМ ПРАКТИКУ МОНТАЖА ПОЛОГО БРУСА НА ШПИЛЬКИ ДИАМЕТРОМ 12-14 ММ С ШАГОМ 2М НА ВСЮ ВЫСОТУ СТЕНЫ. ЭТО ПРИДАЕТ СТЕНЕ ДОПОЛНИТЕЛЬНУЮ КОНСТРУКТИВНУЮ ЖЕСТКОСТЬ И ПРОЧНОСТЬ ВСЕЙ КОНСТРУКЦИИ ЗДАНИЯ. КОНСТРУКЦИЯ ВЕРХНЕГО БРЕВНА В СТЕНЕ «МАУЭРЛАТА» ПРЕДПОЛАГАЕТ ВОЗМОЖНОСТЬ ПОДТЯГИВАНИЯ ГАЕК НА ШПИЛЬКАХ ПО МЕРЕ УСАДКИ СРУБА. ОБРАЗУЮЩИЕСЯ В КОНСТРУКЦИИ СТЕНЫ ВЕРТИКАЛЬНЫЕ ПОЛОСТИ РАЗМЕРОМ 60 НА 14 СМ ЗАПОЛНЯЮТСЯ ЭФФЕКТИВНЫМ УТЕПЛИТЕЛЕМ - ЭКОВАТОЙ, ЧТО ДЕЛАЕТ СТЕНУ ИЗ КЛЕЕНОГО ПОЛОГО БРУСА ОЧЕНЬ ТЕПЛОЙ. КОЭФФИЦИЕНТ ТЕПЛОВОГО СОПРОТИВЛЕНИЯ СТЕНЫ R ДОСТИГАЕТ 3,1. ДЛЯ СРАВНЕНИЯ - R СТАНДАРТНОГО КЛЕЕНОГО БРУСА РАВНО 1,2 ПРИ ТАКОЙ ЖЕ ТОЛЩИНЕ СТЕНЫ (22 СМ).\nПРЕИМУЩЕСТВА КЛЕЕНОГО ПОЛОГО БРУСА\rПО СРАВНЕНИЮ С ДРУГИМИ ТЕХНОЛОГИЯМИ ДЕРЕВЯННОГО ДОМОСТРОЕНИЯ: \r1. СУЩЕСТВЕННО ЛУЧШИЕ ТЕПЛОИЗОЛЯЦИОННЫЕ ПОКАЗАТЕЛИ ПО СРАВНЕНИЮ С ЦЕЛЬНЫМ КЛЕЕНЫМ БРУСОМ.\n2. БОЛЬШАЯ ЭКОЛОГИЧНОСТЬ ПО СРАВНЕНИЮ С КАРКАСНОЙ СТЕНОЙ И СТЕНОЙ ИЗ ЦЕЛЬНОГО КЛЕЕНОГО БРУСА: В СТЕНЕ НЕ ИСПОЛЬЗУЮТСЯ ПЛЕНКИ, И В БОЛЬШЕЙ ЧАСТИ СТЕНЫ НЕТ КЛЕЕВЫХ ПРОСЛОЕК, СТЕНА «ДЫШИТ» БЛАГОДАРЯ ЭТОМУ ДАЖЕ ЛУЧШЕ, ЧЕМ СТЕНА ИЗ ЦЕЛЬНОГО ПРОФИЛИРОВАННОГО И КЛЕЕНОГО БРУСА.\n3. СУЩЕСТВЕННО БОЛЬШАЯ ПО СРАВНЕНИЮ С ДВОЙНЫМ БРУСОМ ПРОЧНОСТЬ, НАДЕЖНОСТЬ И ДОЛГОВЕЧНОСТЬ СТЕН ДОМА. ПРОЧНОСТЬ СТЕНЫ ИЗ ПОЛОГО БРУСА СРАВНИМА С ПРОЧНОСТЬЮ СТЕНЫ ИЗ ЦЕЛЬНОГО БРУСА, ЧТО ПОДТВЕРЖДАЕТ ИСПЫТАНИЕ ПРОВЕДЕННЫЕ СОТРУДНИКАМИ ООО «КЕДР» (ВИДЕО НА САЙТЕ).\n4. ТЕХНОЛОГИЧНОСТЬ И УДОБСТВО ПРОКЛАДКИ ВСЕХ КОММУНИКАЦИЙ В СТЕНЕ СРУБА. «СКРЫТАЯ» ЭЛЕКТРОПРОВОДКА, СЛАБОТОЧКА, ВОДОСНАБЖЕНИЕ И ДАЖЕ КАНАЛИЗАЦИЯ.\n5. ДЕШЕВИЗНА ТРАНСПОРТИРОВКИ И МОНТАЖА: ДОСТИГАЕТСЯ ЗА СЧЕТ МАЛОГО ВЕСА КЛЕЕНОГО ПОЛОГО БРУСА. В СТАНДАРТНУЮ ФУРУ МОЖНО ЗАГРУЗИТЬ ПО ОБЪЕМУ В 2 РАЗА БОЛЬШЕ ПОЛОГО БРУСА ПО СРАВНЕНИЮ С ОБЫЧНЫМ КЛЕЕНЫМ БРУСОМ. МОНТАЖ СТЕН ТАКЖЕ ОБЛЕГЧАЕТСЯ: ВСЕ ДЕТАЛИ ЛЕГКО ПЕРЕНОСЯТ ДВА МОНТАЖНИКА.\n6. И НАКОНЕЦ, НЕМАЛОВАЖНОЕ ПРЕИМУЩЕСТВО - ЭТО БОЛЬШАЯ ДЕШЕВИЗНА ПО СРАВНЕНИЮ С ОБЫЧНЫМ КЛЕЕНЫМ БРУСОМ.\nКЛЕЕНЫЙ ПОЛЫЙ БРУС (УТЕПЛИТЕЛЬ \"ЭКОВАТА\")\nКЛЕЕНЫЙ ПОЛЫЙ БРУС (УТЕПЛИТЕЛЬ \"GUTEX\")\nОБЫЧНЫЙ КЛЕЕНЫЙ БРУС\nЦЕНА БРУСА ЗА М³\n18 500\rРУБ.\n18 500\rРУБ.\n26 000\rРУБ.\nЗАКАЧКА УТЕПЛИТЕЛЯ НА 1 М³ БРУСА\n1 600\rРУБ.\n5 800\rРУБ.\n0\rРУБ.\nЗАСВЕРЛОВКА ЭЛЕКТРОКАНАЛОВ ДЛЯ СКРЫТОЙ ЭЛЕКТРОПРОВОДКИ (ПРИМЕРНАЯ СТОИМОСТЬ НА КУБ. БРУСА)\n0\rРУБ.\n0\rРУБ.\n500\rРУБ.\nЗАСВЕРЛОВКА КАНАЛОВ ПОД ШПИЛЬКИ ДЛЯ СТЯЖКИ БРУСА СНИЗУ ДО ВЕРХУ (ПРИМЕРНАЯ СТОИМОСТЬ)\n0\rРУБ.\n0\rРУБ.\n500\rРУБ.\nИТОГО ЗА М³\n20 100\rРУБ.\n24 300\rРУБ.\n27 000\rРУБ.\nИТОГО ЗА М²\n4 682\rРУБ.\n5 660\rРУБ.\n6 289\rРУБ.\r\n'),(121,'e6f366642713dc1989a2da1654eeabea','ПРОФИЛИРОВАННЫЙ БРУС\r\nПРОФИЛИРОВАННЫЙ СУХОЙ БРУС\rПРОФИЛИРОВАННЫЙ СУХОЙ БРУС\rВ ОТЛИЧИЕ ОТ КЛЕЕНОГО БРУСА ГДЕ ЗАГОТОВКА И СУШКА МАТЕРИАЛА ПРОИЗВОДИТСЯ ИЗ ДОСОК СРЕДНИМ СЕЧЕНИЕМ 45 НА 180 ММ ДЛЯ ПРОФИЛИРОВАННОГО БРУСА В СУШИЛЬНУЮ КАМЕРУ ЗАКЛАДЫВАЮТСЯ МАТЕРИАЛЫ БОЛЬШИМ СЕЧЕНИЕМ 190 НА 140 ММ. СУШКА МАТЕРИАЛА ТАКОГО СЕЧЕНИЯ ДЕЛО ХЛОПОТНОЕ, И ЧТОБЫ ПРОСУШИТЬ НЕ ТОЛЬКО ЕГО ПОВЕРХНОСТЬ, НО И ВЫСУШИТЬ ЕГО ИЗНУТРИ НЕОБХОДИМО СДЕЛАТЬ СПЕЦИАЛЬНЫЕ ПРОПИЛЫ, КОТОРЫЕ ПОМОГУТ СДЕЛАТЬ МАТЕРИАЛ ПОЛНОСТЬЮ СУХИМ.\rНА ПРОТЯЖЕНИИ ВСЕГО ВРЕМЕНИ СУШКИ МАТЕРИАЛА ДЛЯ ПРОФИЛИРОВАННОГО БРУСА, НЕОБХОДИМО ПОСТОЯННО ПРОИЗВОДИТЬ УВЛАЖНЕНИЕ, ТАК НАЗЫВАЕМУЮ ПРОПАРКУ.\rПРИ ВСЕХ НЮАНСАХ СУШКИ И ОТТОЧЕННЫХ ТЕХНОЛОГИЙ НЕЛЬЗЯ ДОБИТЬСЯ АБСОЛЮТНО ИДЕАЛЬНОЙ ГЕОМЕТРИИ МАТЕРИАЛА. НА НЕМ ОБРАЗУЮТСЯ НЕБОЛЬШИЕ ТРЕЩИНЫ И ИЗГИБЫ.\rЕСЛИ ПРОФИЛИРОВАННЫЙ БРУС СРАВНИВАТЬ С КЛЕЕНЫМ БРУСОМ, ТО БУДЕТ ЧЕСТНЫМ СРАВНИТЬ И ЦЕНУ ЭТИХ МАТЕРИАЛОВ. ЦЕНА ПРОФИЛИРОВАННОГО БРУСА ВЫГОДНО ОТЛИЧАЕТСЯ ОТ КЛЕЕНОГО БРУСА. ЭТО ЯВЛЯЕТСЯ ОСНОВНЫМ ФАКТОРОМ ДЛЯ КЛИЕНТОВ НАШЕЙ КОМПАНИИ, КОТОРЫЕ ОТДАЮТ ПРЕДПОЧТЕНИЕ ЭКОЛОГИЧНОСТИ МАТЕРИАЛА. МЫ ПРОИЗВОДИМ ЛЮБЫЕ ДЕРЕВЯННЫЕ ДОМА. ВЫБОР МАТЕРИАЛА ВСЕГДА ОСТАЕТСЯ ЗА НАШИМИ КЛИЕНТАМИ.\nЦЕНЫ НА ПРОФИЛИРОВАННЫЙ СУХОЙ БРУС\r- СОСНА / ЕЛЬ\r140 Х 140 ММ.\rС НАРЕЗКОЙ ЧАШЕК ПОД ПРОЕКТ\rВЛАЖНОСТЬ\r(18 - 20%)\rЗА М\n16 500\r РУБ.\nЗА М\n2 412\r РУБ.\n190 Х 140 ММ.\rС НАРЕЗКОЙ ЧАШЕК ПОД ПРОЕКТ\rВЛАЖНОСТЬ\r(18 - 20%)\rЗА М\n16 500\r РУБ.\nЗА М\n3 274\r РУБ.\nПРЕИМУЩЕСТВА И НЕДОСТАТКИ ПРОФИЛИРОВАННОГО БРУСА\rПОЛЫЙ КЛЕЕНЫЙ БРУС\rПЛЮСЫ\rВНЕШНЕ НИЧЕМ НЕ ОТЛИЧАЕТСЯ ОТ СТАНДАРТНОГО КЛЕЕНОГО БРУСА, НО В ТРИ РАЗА ТЕПЛЕЕ. УЛУЧШЕННАЯ ШУМОИЗОЛЯЦИЯ СТЕН, НЕ НУЖНО ВЫСВЕРЛИВАТЬ КАНАЛЫ В СТЕНЕ ПОД ЭЛЕКТРИКУ. ПРИВЛЕКАТЕЛЬНАЯ ЦЕНА.\nМИНУСЫ\rСЛОЖНОСТЬ ПРОИЗВОДСТВА, В ОСТАЛЬНОМ МИНУСОВ НЕТ.\nКЛЕЕНЫЙ БРУС\rПЛЮСЫ\rМИНИМАЛЬНАЯ УСАДКА, ПРАКТИЧЕСКИ ОТСУТСТВИЕ ТРЕЩИН, НЕ ТРЕБУЕТСЯ КОНОПАТКА. МИНИМАЛЬНОЕ ИЗМЕНЕНИЕ ГЕОМЕТРИИ МАТЕРИАЛА. ПОРОКИ ДРЕВЕСИНЫ ВЫРЕЗАЮТСЯ НА ЭТАПЕ ПРОИЗВОДСТВА.\nМИНУСЫ\rСТОИМОСТЬ 1 КУБ.М. КЛЕЕНОГО БРУСА ДОРОЖЕ ОСТАЛЬНЫХ МАТЕРИАЛОВ ДЛЯ ДЕРЕВЯННОГО ДОМА, ОДНАКО МИНИМАЛЬНЫЕ ЗАТРАТЫ ПРИ ПОСЛЕДУЮЩЕЙ ЭКСПЛУАТАЦИИ, КОМПЕНСИРУЮТ КАЖУЩУЮСЯ ДОРОГОВИЗНУ.\nПРОФИЛИРОВАННЫЙ СУХОЙ БРУС\rПЛЮСЫ\rВ ОТЛИЧИЕ ОТ БРЕВНА, НЕ ТРЕБУЕТ КОНОПАТКИ, БОЛЕЕ ЛЕГКАЯ ШЛИФОВКА, НЕ НУЖНО ОТКЛАДЫВАТЬ ОТДЕЛОЧНЫЕ РАБОТЫ НА ГОД ИЗ-ЗА УСАДКИ.\nМИНУСЫ\rПОДВЕРЖЕН ДЕФОРМАЦИЯМ, ВЕРТИКАЛЬНОСТЬ СТЕН НЕ ИДЕАЛЬНА. ГОРАЗДО БОЛЬШЕ ТРЕЩИН ПО СРАВНЕНИЮ С КЛЕЕНЫМ БРУСОМ. ПРИ ПРОИЗВОДСТВЕ НЕ ВЫРЕЗАЮТСЯ ВЫПАДНЫЕ СУЧКИ И ДЕФЕКТЫ ДРЕВЕСИНЫ.\nКАРКАСНЫЕ ДОМА\rПЛЮСЫ\rБЫСТРОВОЗВОДИМОЕ, ОТНОСИТЕЛЬНО ДЕШЕВОЕ ЖИЛЬЁ.\nМИНУСЫ\rВ БОРЬБЕ ЗА СНИЖЕНИЕМ СТОИМОСТИ СТРОИТЕЛЬСТВА ЗАЧАСТУЮ НЕ СОБЛЮДАЮТСЯ ТЕХНОЛОГИИ, И ПРИМЕНЯЮТСЯ ДЕШЕВЫЕ МАТЕРИАЛЫ. ДОСКУ ИСПОЛЬЗУЮТ НЕ СУХУЮ, НЕ ОБРАБОТАННУЮ АНТИСЕПТИКОМ, УМЕНЬШАЮТ КОЛИЧЕСТВО СТОЕК В СТЕНЕ, ИСПОЛЬЗУЮТ ПЛОХИЕ ПАРО И ГИДРОИЗОЛЯЦИОННЫЕ ПЛЕНКИ\nОЦИЛИНДРОВАННОЕ БРЕВНО\rПЛЮСЫ\rКАЖУЩАЯСЯ ДЕШЕВИЗНА НА НАЧАЛЬНОМ ЭТАПЕ.\nМИНУСЫ\rНЕЛЬЗЯ ПРИСТУПАТЬ К ОТДЕЛОЧНЫМ РАБОТАМ ОКОЛО ГОДА ПОКА СРУБ НЕ ДАСТ УСАДКУ, ОЧЕНЬ СИЛЬНО ТРЕСКАЕТСЯ, ТРЕБУЕТ В ДАЛЬНЕЙШЕМ СЕРЬЁЗНЫХ ЗАТРАТ НА КОНОПАТКУ И ШЛИФОВКУ, ТЕПЛОВОЙ ШОВ РАВЕН ТОЛЬКО ПОЛОВИНЕ ДИАМЕТРА БРЕВНА.\r\n'),(122,'16019836bacf9f61e421806861ff558d','ТЕХНОЛОГИИ И МАТЕРИАЛЫ\r\nТЕХНОЛОГИИ И МАТЕРИАЛЫ\rПРИ ПРОИЗВОДСТВЕ ДОМОВ ИЗ КЛЕЕНОГО БРУСА СТРОИТЕЛЬНАЯ КОМПАНИЯ ISKONI ИСПОЛЬЗУЕТ СОБСТВЕННЫЕ ЗАПАТЕНТОВАННЫЕ, А ТАКЖЕ САМЫЕ ПЕРЕДОВЫЕ НЕМЕЦКИЕ И АВСТРИЙСКИЕ ТЕХНОЛОГИИ, ЧТО ПОЗВОЛЯЕТ НАМ ГАРАНТИРОВАТЬ ВЫСОКОЕ КАЧЕСТВО ПО ВЫГОДНЫМ ЦЕНАМ.\nНИЖЕ ВЫ СМОЖЕТЕ БОЛЕЕ ПОДРОБНО ОЗНАКОМИТЬСЯ СО ВСЕМИ ЭТАПАМИ СТРОИТЕЛЬСТВА ДОМОВ ИЗ ДЕРЕВА И ПРИМЕНЯЕМЫМИ ТЕХНОЛОГИЧЕСКИМИ РЕШЕНИЯМИ КОМПАНИИ ISKONI.\nПРОИЗВОДСТВО КОМПАНИИ «КЕДР» В РЕСПУБЛИКЕ МАРИЙ ЭЛ:\nСЕМЕЙНАЯ ФАБРИКА С 25-ЛЕТНИМ ОПЫТОМ ДЕРЕВЯННОГО ДОМОСТРОИТЕЛЬСТВА;\rПОЛНЫЙ КОНТРОЛЬ И ОТБРАКОВКА ПОСТАВЛЯЕМЫХ ПИЛОМАТЕРИАЛОВ ВКЛЮЧАЕТ В СЕБЯ ПРОВЕРКУ ГЕОМЕТРИЧЕСКИХ ВЫПУСКОВ ПИЛОМАТЕРИАЛА, А ТАКЖЕ ПРОВЕРКУ ОТСУТСТВИЯ ГРИБНЫХ ПОРАЖЕНИЙ, ОКРАСОВ, ГНИЛИ И ДРУГИХ ДЕФЕКТОВ;\rЕСТЕСТВЕННАЯ СУШКА В ТЕЧЕНИЕ 2-4 НЕДЕЛЬ (В ЗАВИСИМОСТИ ОТ ВРЕМЕНИ ГОДА);\rКАМЕРНАЯ СУШКА В МЯГКОМ РЕЖИМЕ С 3-Х КРАТНОЙ ПРОПАРКОЙ ДЛЯ ИСКЛЮЧЕНИЯ ВНУТРЕННИХ НАПРЯЖЕНИЙ И ТРЕЩИН В ПИЛОМАТЕРИАЛЕ;\rПОШТУЧНЫЙ, А НЕ ВЫБОРОЧНЫЙ КОНТРОЛЬ ВЛАЖНОСТИ ДРЕВЕСИНЫ НА ВЫХОДЕ ИЗ СУШИЛЬНОЙ КАМЕРЫ. ТО ЕСТЬ ПРИ ПРОИЗВОДСТВЕ ДОМОКОМПЛЕКТА МЫ В ПРЯМОМ СМЫСЛЕ ПРОВЕРЯЕМ КАЖДУЮ ЗАГОТОВКУ (БРЕВНО ИЛИ ДОСКУ, В ЗАВИСИМОСТИ ОТ ТОГО, ИЗ ЧЕГО ВЫ РЕШИТЕ СТРОИТЬ ВАШ ДОМ);\rИСПОЛЬЗОВАНИЕ ЭКОЛОГИЧЕСКИ ЧИСТЫХ МАТЕРИАЛОВ ПРИ ПРОИЗВОДСТВЕ КЛЕЕНОГО БРУСА;\rПРЕДПОЧТИТЕЛЬНОЕ ИСПОЛЬЗОВАНИЕ «НЕМЕЦКОЙ» ГРЕБЕНКИ ПРИ ИЗГОТОВЛЕНИИ ЗАГОТОВОК КЛЕЕНОГО БРУСА. ДАННАЯ ТЕХНОЛОГИЯ ОЧЕНЬ ЭФФЕКТИВНА С ТОЧКИ ЗРЕНИЯ ТЕПЛОСБЕРЕЖЕНИЯ И ПОЖАРОСТОЙКОСТИ ГОТОВОГО СТРОЕНИЯ;\rАНТИСЕПТИЧЕСКАЯ ОБРАБОТКА МАТЕРИАЛА, ЧТО ГАРАНТИРУЕТ ОТСУТСТВИЕ ГРИБКА И ПАТОГЕННЫХ ОРГАНИЗМОВ;\r45 ДНЕЙ – СРОК ИЗГОТОВЛЕНИЯ ДОМОКОМПЛЕКТА «ПОД КЛЮЧ».\rУНИКАЛЬНЫЕ ТЕХНОЛОГИИ\nУТЕПЛЕННЫЙ КЛЕЕНЫЙ БРУС\rЗАПАТЕНТОВАННАЯ УНИКАЛЬНАЯ РАЗРАБОТКА\rКОМПАНИИ «КЕДР», НЕ ИМЕЮЩАЯ АНАЛОГОВ НА РЫНКЕ. УТЕПЛЕННЫЙ КЛЕЕНЫЙ БРУС ПРЕДСТАВЛЯЕТ СОБОЙ КЛЕЕНЫЙ БРУС С ПОЛОСТЯМИ ВНУТРИ, КОТОРЫЕ ЗАПОЛНЯЮТСЯ СПЕЦИАЛЬНЫМ МАТЕРИАЛОМ-УТЕПЛИТЕЛЕМ: ЭКОВАТОЙ ИЛИ АВСТРИЙСКИЙМ МАТЕРИАЛОМ GUTEX. УТЕПЛЕННЫЙ БРУС ОБЛАДАЕТ ГОРАЗДО БОЛЕЕ ВЫСОКИМИ ТЕПЛОИЗОЛЯЦИОННЫМИ ХАРАКТЕРИСТИКАМИ.\rТАКОЙ БРУС В 4,5 РАЗА ТЕПЛЕЕ ОБЫЧНОЙ ДРЕВИСИНЫ, А ТЕПЛОИЗОЛЯЦИЯ СТЕН ИЗ КЛЕЕНОГО БРУСА ПРОФИЛЯ 20 СМ ЭКВИВАЛЕНТНА ПОЛНОСТЬЮ ДЕРЕВЯННОЙ СТЕНЕ ТОЛЩИНОЙ В 60 СМ ИЛИ КИРПИЧНОЙ СТЕНЕ ТОЛЩИНОЙ В 3 МЕТРА! ТО ЕСТЬ ДОМ ИЗ УТЕПЛЕННОГО БРУСА ПОЛУЧАЕТСЯ В 3 РАЗА ТЕПЛЕЕ, ЧЕМ ДОМ ИЗ КЛАССИЧЕСКОГО КЛЕЕНОГО БРУСА И В ДЕСЯТКИ РАЗ ТЕПЛЕЕ, ЧЕМ КАМЕННЫЕ И КИРПИЧНЫЕ ДОМА.\rСТЕНА ИЗ КЛЕЕНОГО БРУСА С УТЕПЛИТЕЛЕМ ОБЛАДАЕТ ВСЕМИ ПРЕИМУЩЕСТВАМИ КЛЕЕНОГО БРУСА: ПРАКТИЧЕСКИ НЕТ УСАДКИ, НЕТ КОРОБЛЕНИЯ И РАСТРЕСКИВАЕТСЯ. ПРИ ЭТОМ УГЛОВОЙ ЗАМОК ПОЛУЧАЕТСЯ АБСОЛЮТНО НЕПРОДУВАЕМЫМ И ЗВУКОИЗОЛИРУЮЩИМ.\rУТЕПЛИТЕЛЬ ЭКОВАТА ИЛИ GUTEX ЯВЛЯЕТСЯ ХОРОШИМ АНТИСЕПТИКОМ, ЗАЩИЩАЮЩИМ БРУС ОТ БОЛЕЗНЕЙ И ГРЫЗУНОВ ИЗНУТРИ.\rНЕТ НЕОБХОДИМОСТИ СВЕРЛИТЬ ОТВЕРСТИЯ ДЛЯ КОММУНИКАЦИЙ: ВСЕ ТРУБЫ И ПРОВОДА МОЖНО СВОБОДНО МОНТИРОВАТЬ ДАЖЕ ПОСЛЕ ВОЗВЕДЕНИЯ СТЕН И КРЫШИ, ДАЖЕ ПОСЛЕ УКЛАДКИ УТЕПЛИТЕЛЯ.\rКРОМЕ ТОГО, ПРИ СТРОИТЕЛЬСТВЕ ДОМОВ ИЗ УТЕПЛЕННОГО БРУСА СТОИМОСТЬ ДОМОКОМПЛЕКТА ПО СРАВНЕНИЮ С КЛАССИЧЕСКИМ КЛЕЕНЫМ БРУСОМ ВЫГОДНЕЕ НА 30%, ЧТО ПРИВОДИТ К 10% ЭКОНОМИИ НА СТОИМОСТИ ВСЕГО ПРОЕКТА.\rИСПОЛЬЗУЕМЫЕ МАТЕРИАЛЫ В СТРОИТЕЛЬСТВЕ\nПРИ СТРОИТЕЛЬСТВЕ ДОМОВ ИЗ БРУСА МЫ ИСПОЛЬЗУЕМ СЛЕДУЮЩИЕ ВИДЫ МАТЕРИАЛОВ ИЗ СОСНЫ И ЕЛИ.\nВИД МАТЕРИАЛА\nПРЕИМУЩЕСТВА\nНЕДОСТАТКИ\nСТОИМОСТЬ ЗА М³\nУТЕПЛЕННЫЙ КЛЕЕНЫЙ БРУС\rТАКОЙ БРУС ТЕПЛЕЕ, ЛЕГЧЕ И НА 30% ДЕШЕВЛЕ ОБЫЧНОГО КЛЕЕНОГО БРУСА\nСУЩЕСТВЕННЫЕ НЕДОСТАТКИ ОТСУТСТВУЮТ\nОТ 18 500 Р\r*\nКЛЕЕНЫЙ БРУС\rВЫСОКОЕ КАЧЕСТВО, НАДЕЖНОСТЬ, МИНИМАЛЬНАЯ УСАДКА И ОТСУТСТВИЕ ТРЕЩИН\nСАМЫЙ ДОРОГОЙ ВИД СТРОИТЕЛЬНОГО МАТЕРИАЛА ДЕРЕВЯННЫХ ДОМОВ\nОТ 23 500 Р\r*\nПРОФИЛИРОВАННЫЙ БРУС\rОТНОСИТЕЛЬНО НИЗКАЯ СТОИМОСТЬ, ОТСУТСТВИЕ УСАДКИ\nПОДВЕРЖЕН ДЕФОРМАЦИЯМ И ТРЕЩИНАМ, МЕНЕЕ ТЕПЛОУСТОЙЧИВ ПО СРАВНЕНИЮ С ОБЫЧНЫМ БРУСОМ\nОТ 16 500 Р\r*\nЦИЛИНДРОВАННОЕ БРЕВНО\rСАМАЯ ПРИВЛЕКАТЕЛЬНАЯ ЦЕНА\nНЕОБХОДИМОСТЬ УСАДКИ ДОМОКОМЛПЕКТА В ТЕЧЕНИЕ ГОДА. БОЛЬШОЕ КОЛИЧЕСТВО ТРЕЩИН. ВЫСОКАЯ СТОИМОСТЬ ДОПОЛНИТЕЛЬНЫХ МАТЕРИАЛОВ И РАБОТ ПО ТЕПЛОИЗОЛЯЦИИ И ПОКРАСКЕ\nОТ 8 800 Р\r*\n*ЦЕНЫ УКАЗАНЫ ПРИ ЗАКАЗЕ ДОМОКОМПЛЕКТА И ВНЕСЕНИЯ ПРЕДОПЛАТЫ ДО 01.09.2019. \rТАКЖЕ ЕСТЬ ВОЗМОЖНОСТЬ ПОЛУЧИТЬ ИНДИВИДУАЛЬНЫЙ РАСЧЕТ ДЛЯ СТРОИТЕЛЬСТВА ДОМА ИЗ КЕДРА. ПРЕДЛАГАЕМ ВЫБРАТЬ КАКИМ БУДЕТ ВАШ БУДУЩИЙ ДОМ И ИЗ ЧЕГО МЫ ЕГО ПОСТРОИМ!\nСВЯЖИТЕСЬ С НАМИ И ПОЛУЧИТЕ РАСЧЕТ ПРОЕКТА СВОЕГО ДОМА\r\n'),(132,'d1b1f6f9a98bad187a948b5ec56556e9','АКЦИЯ ВО ВРЕМЯ КОРОНОВИРУСА\r\nСАМОИЗОЛЯЦИЯ С ИСКОНИ!\rТЕПЛО, УЮТНО, БЕЗОПАСНО. В НАШИХ ДОМА КОРОНАВИРУСА НЕТ!\rНАША КОМПАНИЯ ЖЕЛАЕТ ЗДОРОВЬЯ ВАМ И ВАШИМ БЛИЗКИМ И ПРИЗЫВАЕТ СОБЛЮДАТЬ РЕЖИМ САМОИЗОЛЯЦИИ В СВЯЗИ С ПАНДЕМИЕЙ КОРОНАВИРУСА. ПРЕДСТАВЬТЕ, НАСКОЛЬКО СЕЙЧАС КОМФОРТНО ТЕМ, КТО ПРОВОДИТ ВРЕМЯ В СВОЕМ ДОМЕ, А НЕ В КВАРТИРЕ В 4-Х СТЕНАХ. СЧИТАЕМ, ЧТО СЕЙЧАС ЛУЧШЕЕ ВРЕМЯ, ЧТОБЫ ЗАПЛАНИРОВАТЬ СТРОЙКУ СВОЕГО ДОМА!\rЧТОБЫ ВАМ БЫЛО ЛЕГКО И ИНТЕРЕСНО ПРОВОДИТЬ ВРЕМЯ ДОМА МЫ ПОДГОТОВИЛИ СПЕЦИАЛЬНУЮ АКЦИЮ - ИНДИВИДУАЛЬНЫЙ ПРОЕКТ ДОМА ИЗ КЛЕЕНОГО БРУСА ВСЕГО ЗА 10 000 РУБЛЕЙ!\rЗА ЭТУ СУММУ МЫ СОВМЕСТНО С ВАМИ:\r- ВЫБЕРЕМ АРХИТЕКТУРНЫЙ СТИЛЬ ВАШЕГО БУДУЩЕГО ДОМА;\r- ОТРИСУЕМ ВНЕШНИЙ ВИД;\r- СДЕЛАЕМ ОБЩИЕ ПЛАНИРОВКИ И ПЛАНИРОВКИ КАЖДОГО ПОМЕЩЕНИЯ;\r- СДЕЛАЕМ ПЛАН КРОВЛИ, НАВЕСА ДЛЯ АВТО И ВЕРАНДЫ;\r- ВЫБЕРЕМ МАТЕРИАЛЫ, КОТОРЫЕ БУДЕМ ИСПОЛЬЗОВАТЬ ПРИ СТРОИТЕЛЬСТВЕ;\r- ПРОВЕДЕМ ВСЕ НЕОБХОДИМЫЕ КОНСУЛЬТАЦИИ;\r- ВМЕСТЕ ПРОРАБОТАЕМ ВСЕ ТЕХНОЛОГИЧЕСКИЕ УЗЛЫ;\r- ПРОВЕДЕМ ВСЕ НЕОБХОДИМЫЕ ИНЖЕНЕРНЫЕ РАСЧЕТЫ;\r- ДАДИМ ОБЩИЕ РЕКОМЕНДАЦИИ ПО ВЫБОРУ ФУНДАМЕНТА, ВНУТРЕННИМ И ВНЕШНИМ ИНЖЕНЕРНЫМ СИСТЕМАМ.\rЭТА РАБОТА ЗАЙМЕТ ПРИМЕРНО МЕСЯЦ И ДАСТ ВАМ НЕ ТОЛЬКО ПРЕДСТАВЛЕНИЕ О ТОМ, КАК БУДЕТ ВЫГЛЯДЕТЬ ВАШ БУДУЩИЙ ДОМ, НО И СУЩЕСТВЕННО СОКРАТИТ ВРЕМЯ ПОДГОТОВКИ К СТРОЙКЕ: К МОМЕНТУ ВЫХОДА ИЗ КАРАНТИНА НАМ ОСТАНЕТСЯ ТОЛЬКО УДАРИТЬ ПО РУКАМ И ПРИСТУПИТЬ К СТРОИТЕЛЬСТВУ, ТАК КАК ОСНОВНЫЕ ВОПРОСЫ БУДУТ УЖЕ СОГЛАСОВАНЫ!\rПОДПИСЫВАЙТЕСЬ НА НАШ КАНАЛ, ИНСТАГРАМ @ISKONI_WOOD И ДАВАЙТЕ ПРОВЕДЕМ ВРЕМЯ САМОИЗОЛЯЦИИ С ПОЛЬЗОЙ!\r\n'),(167,'9b54f6d6f4014b7b0c3b7147e3ec9bea','НОВЫЙ КАТАЛОГ\r\n\r\n'),(177,'7df06b0634dc25c762a3d2a7f8e5e100','БЕГОНИЯ\r\n\r\n'),(172,'7f00a16a61c28841cd55feff99c164fb','/CATALOG_NEW/TEST/FILTER/PRICE-TO-4/FLOORS2-IS-DVUKHEHTAZHNYE/APPLY/\r\n\r\n'),(150,'12ae36aece14390a0afc77b7f55d5199','СВЕТЛЫЙ\r\n\r\n'),(155,'4232a178f96527c6fc7eedcca942a575','ИРИЙ\r\n\r\n'),(158,'dd98780d8719fde138b580a8c452a497','ФИНСКИЙ\r\n\r\n'),(175,'dd9f4d332b1585d719b1a61dc4f68c11','ПРОЕКТЫ ДОМОВ ИЗ БРУСА\r\n\r\n'),(176,'f1be51d60819b0cf213386a091660292','/CATALOG_NEW/DOMA-IZ-BRUSA/FILTER/FLOORS2-IS-ODNOEHTAZHNYE/APPLY/\r\n\r\n'),(52,'a903acfdfa22c30fcf7a13e858c7282b','ПОСТРОЕННЫЕ ДОМА\r\nСТРОИМ ДОМА С 1997 ГОДА\nКОМПАНИЯ ИСКОНИ – МОСКОВСКОЕ ПРЕДСТАВИТЕЛЬСТВО КАЗАНСКОЙ СТРОИТЕЛЬНОЙ КОМПАНИИ КЕДР, РАБОТАЮЩЕЙ НА РЫНКЕ С 1997 ГОДА. ЗА ЭТО ВРЕМЯ НАМИ БЫЛО ПОСТРОЕНО БОЛЕЕ 500 ДОМОВ ИЗ БРУСА РАЗНЫХ ПОРОД ДЕРЕВА (КЕДР, ЕЛЬ, СОСНА). СЕГОДНЯ МЫ СТРОИМ ДОМА ИЗ КАЧЕСТВЕННОЙ ДРЕВЕСИНЫ СРЕДНЕЙ ПОЛОСЫ. ПРИ ПРОИЗВОДСТВЕ МЫ ИСПОЛЬЗУЕМ ТОЛЬКО ЭКОЛОГИЧЕСКИ ЧИСТЫЕ МАТЕРИАЛЫ, УНИКАЛЬНЫЕ СОБСТВЕННЫЕ И ПЕРЕДОВЫЕ НЕМЕЦКИЕ ТЕХНОЛОГИИ.\nВИДЕО-ОБЗОР ТИПОВОГО ПРОЕКТА ДОМ УДАЧА+\rПЛИТНЫЙ ФУНДАМЕНТ ДЛЯ ДЕРЕВЯННОГО ДОМА\rНАШИ ЛУЧШИЕ ИНДИВИДУАЛЬНЫЕ ПРОЕКТЫ\nИНДИВИДУАЛЬНЫЙ ПРОЕКТ ДОМА\nРЕСПУБЛИКА ТАТАРСТАН\nО ПРОЕКТЕ:\rДВУХЭТАЖНЫЙ ЖИЛОЙ ДОМ ПО ИНДИВИДУАЛЬНОМУ ПРОЕКТУ. ОБЩАЯ ПЛОЩАДЬ ДОМА: 190 М2. ГОД ПОСТРОЙКИ 2018.\rДОМ ВЫПОЛНЕН ИЗ КЛЕЕНОГО БРУСА 180 (ВЫСОТА) Х 200 ММ\rСОВРЕМЕННЫЙ ПРОСТОРНЫЙ ДОМ. ШИКАРНАЯ ГОСТИНАЯ С КАМИНОМ И ВЫСОТОЙ ПОТОЛКОВ БОЛЕЕ 4-Х МЕТРОВ ИМЕЕТ БОЛЬШИЕ ПАНОРАМНЫЕ ОКНА.\rВ ЭТОМ ПРОЕКТЕ, В ЕГО РЕАЛИЗАЦИИ УДАЛОСЬ СОВМЕСТИТЬ СВЯЗЬ ВРЕМЕН, ГРАНДИОЗНЫЙ ЗАМЫСЕЛ И ЕДИНЕНИЕ ВЕЛИКОГО И МАЛОГО. МНОГО СВЕТА, МНОГО ТЕПЛА. ВСЕ ВОКРУГ ГОВОРИТ ОБ УСПЕШНОСТИ И УМЕНИИ ДОСТИГАТЬ ПОСТАВЛЕННЫХ РУБЕЖЕЙ. \nИНДИВИДУАЛЬНЫЙ ПРОЕКТ ДОМА\nРЕСПУБЛИКА ТАТАРСТАН\nО ПРОЕКТЕ:\rДВУХЭТАЖНЫЙ ЖИЛОЙ ДОМ ПО ИНДИВИДУАЛЬНОМУ ПРОЕКТУ, ПЛОЩАДЬ ДОМА 187 М2.\rГОД ПОСТРОЙКИ 2019\rДОМ ВЫПОЛНЕН ИЗ КЛЕЕНОГО БРУСА 180 Х 200 ММ\rОРИГИНАЛЬНАЯ ПЛАНИРОВКА, НЕСТАНДАРТНОЕ РЕШЕНИЕ УГЛОВОЙ КОМНАТЫ, ВЫПОЛНЕННОЕ ПО ТИПУ КРЕМЛЕВСКОЙ ШЕСТИСТЕННОЙ БАШНИ ПРИДАЮТ ДОМУ ОСОБЫЙ КОЛОРИТ.\rЭТОТ ПРОЕКТ – ВОПЛОЩЕНИЕ ДЕТСКОЙ МЕЧТЫ ДЛЯ СВОИХ ДЕТЕЙ. А КОГДА ЧЕЛОВЕК ВЫРАСТАЕТ В АТМОСФЕРЕ СКАЗКИ, В АТМОСФЕРЕ СЧАСТЬЯ, ТОЛЬКО ТОГДА ОН СМОЖЕТ ПОКОРИТЬ ЛЮБЫЕ ВЕРШИНЫ.\nИНДИВИДУАЛЬНЫЙ ПРОЕКТ ДОМА\nРЕСПУБЛИКА ТАТАРСТАН\nО ПРОЕКТЕ:\rДВУХЭТАЖНЫЙ ЖИЛОЙ ДОМ ПО ПРОЕКТУ «АЗАЛИЯ-ЛЮКС». ОБЩАЯ ПЛОЩАДЬ ДОМА: 207 М2. \rГОД ПОСТРОЙКИ 2012.\rДОМ ВЫПОЛНЕН ИЗ КЛЕЕНОГО БРУСА 180 (ВЫСОТА) Х 220 ММ\rДОМ ПОСТРОЕН ДЛЯ МНОГОДЕТНОЙ СЕМЬИ. КОМНАТЫ ВЕРХНЕГО ЭТАЖА ИМЕЮТ МАНСАРДНЫЕ ОКНА. ВНУТРЕННИЕ ПЛАНИРОВКИ ОЧЕНЬ УДАЧНО И ГАРМОНИЧНО ПРОДУМАНЫ. НЕТ НИЧЕГО ЛИШЛЕГО, НИКАКИХ ИЗЛИШЕСТВ. ВСЕ ОЧЕНЬ ФУНКЦИОНАЛЬНО, ПРАВИЛЬНО. КАЖДАЯ ВЕЩЬ – НА СВОЕМ МЕСТЕ, КАЖДОЕ ПОМЕЩЕНИЕ ТАМ, ГДЕ ЕМУ СЛЕДУЕТ БЫТЬ, КАЖДОЕ ПРОСТРАНСТВО ТАКОЕ, КАК НУЖНО. ЭТОТ ДОМ – КАК РАЗ ТОТ СЛУЧАЙ, КОГДА ВСЕ ЕСТЕСТВЕННО И ПРОДУМАНО.\nИНДИВИДУАЛЬНЫЙ ПРОЕКТ ДОМА\nМОСКОВСКАЯ ОБЛАСТЬ\nО ПРОЕКТЕ:\rДВУХЭТАЖНЫЙ ДАЧНЫЙ ДОМ ПО ПРОЕКТУ «УДАЧА ПЛЮС». ОБЩАЯ ПЛОЩАДЬ ДОМА 113 М2.\rГОД ПОСТОРОЙКИ 2018.\rДОМ ВЫПОЛНЕН ИЗ УТЕПЛЕННОГО КЛЕЕНОГО БРУСА 180 Х 200 ММ.\rИДЕАЛЬНОЕ ДАЧНОЕ РЕШЕНИЕ ДЛЯ МОЛОДОЙ СЕМЬИ. ВСЕМ ЕСТЬ МЕСТО, И РОДИТЕЛЯМ И ДЕТЯМ, И ГОСТЯМ. В ЭТОМ ДОМЕ МОЖНО КОМФОРТНО ПРОВОДИТЬ И ТЕПЛЫЕ ЛЕТНИЕ ВЕЧЕРА, И ХОЛОДНЫЕ ЗИМНИЕ НОЧИ. ЛЕТОМ БУДЕТ ПРОХЛАДНО, ЗИМОЙ – ТЕПЛО И ТИХО.\nИНДИВИДУАЛЬНЫЙ ПРОЕКТ ДОМА\nРЕСПУБЛИКА ТАТАРСТАН\nО ПРОЕКТЕ:\rДВУХЭТАЖНЫЙ ЖИЛОЙ ДОМ ПО ИНДИВИДУАЛЬНОМУ ПРОЕКТУ. ОБЩАЯ ПЛОЩАДЬ ДОМА: 180 М2. ГОД ПОСТРОЙКИ 2018.\rДОМ ВЫПОЛНЕН ИЗ КЛЕЕНОГО БРУСА 180 (ВЫСОТА) Х 240 ММ\rОДНОВРЕМЕННО БЫЛИ ПОСТРОЕНЫ БАНЯ И ЛЕТНЯЯ КУХНЯ (КЛЕЕНЫЙ БРУС 180 Х 160 ММ)\rЭТОТ ДОМ ДЛЯ МОЛОДОЙ СЕМЬИ, НА МОМЕНТ ПОСТРОЙКИ БЫЛ ОДИН МАЛЕНЬКИЙ РЕБЕНОК, ДЛЯ КОТОРОГО РОДИТЕЛЯМИ СОЗДАНЫ ВСЕ УСЛОВИЯ ДЛЯ ГАРМОНИЧНОГО РАЗВИТИЯ. ОЧЕВИДНО, ЧТО НЕДАЛЕК ТОТ ДЕНЬ, КОГДА У ЭТОГО РЕБЕНКА ПОЯВЯТСЯ МЛАДШИЕ БРАТЬЯ ИЛИ СЕСТРЫ: ПРОСТРАНСТВА ВСЕМ ХВАТИТ.\nИНДИВИДУАЛЬНЫЙ ПРОЕКТ ДОМА\nРЕСПУБЛИКА ТАТАРСТАН\nО ПРОЕКТЕ:\rОПИСАНИЕ: ДВУХЭТАЖНЫЙ ЖИЛОЙ ДОМ ПО ИНДИВИДУАЛЬНОМУ ПРОЕКТУ. ОБЩАЯ ПЛОЩАДЬ ДОМА: 200 М2. ГОД ПОСТРОЙКИ 2017.\rДОМ ВЫПОЛНЕН ИЗ КЛЕЕНОГО БРУСА 180 (ВЫСОТА) Х 160 ММ\rДОМ ПОСТРОЕН НА ЖИВОПИСНОМ БЕРЕГУ ВОДОЕМА НА СКЛОНЕ. В ПРОЕКТЕ УДАЛОСЬ С МАКСИМАЛЬНОЙ ЭФФЕКТИВНОСТЬЮ ИСПОЛЬЗОВАТЬ ОСОБЕННОСТИ ПРИРОДНОГО ЛАНДШАФТА. \rПЛАНИРОВКА УЧАСТКА И СОПУТСТВУЮЩИЕ ПОСТРОЙКИ ДЕЛАЮТ ЭТОТ ДОМ ЦЕНТРОМ МАЛЕНЬКОГО УЮТНОГО МИРА НА БЕРЕГУ ВЕЛИКОЙ РУССКОЙ РЕКИ. ВСЕ ВОКРУГ КАК БЫ ГОВОРИТ О ТОМ, ЧТО НАШ МИР СОЗДАН ДЛЯ СЧАСТЬЯ.\nИНДИВИДУАЛЬНЫЙ ПРОЕКТ ДОМА\nРЕСПУБЛИКА ТАТАРСТАН\nО ПРОЕКТЕ:\rДВУХЭТАЖНЫЙ ЖИЛОЙ ДОМ ПО ИНДИВИДУАЛЬНОМУ ПРОЕКТУ. ОБЩАЯ ПЛОЩАДЬ ДОМА: 232 М2. ГОД ПОСТРОЙКИ 2009.\rДОМ ВЫПОЛНЕН ИЗ КЛЕЕНОГО БРУСА 140 (ВЫСОТА) Х 200 ММ\rГОДОМ ПОЗЖЕ БЫЛИ ПОСТРОЕНЫ БАНЯ (КЛЕЕНЫЙ БРУС 140 Х 160 ММ)\rИ ЛЕТНЯЯ КУХНЯ (КЛЕЕНЫЙ БРУС 140 Х 129 ММ)\rДОМ БЫЛ ПОСТРОЕН ДЛЯ БОЛЬШОЙ СЕМЬИ. ВМЕСТИТЕЛЬНАЯ ГОСТИНАЯ ПОЗВОЛЯЕТ СОБИРАТЬСЯ НА СЕМЕЙНЫЕ ТОРЖЕСТВА БОЛЬШОМУ ЧИСЛУ ГОСТЕЙ. БЫЛИ ПРАЗДНИКИ, КОГДА СОБИРАЛОСЬ БОЛЕЕ 50 ЧЕЛОВЕК. В ДНИ ОБЫЧНЫЕ У КАЖДОГО ЧЛЕНА СЕМЬИ ЕСТЬ СВОЙ УЮТНЫЙ УГОЛОК, ДЕТЯМ МОЖНО СПОКОЙНО ДЕЛАТЬ УРОКИ, СТУДЕНТАМ – СВОИ ПРОЕКТЫ, А ГЛАВЕ СЕМЕЙСТВА – ОТДЫХАТЬ И НАСЛАЖДАТЬСЯ СЕМЕЙНЫМ СЧАСТЬЕМ.\r\n'),(134,'37c6d450a2132f710bbd6c318d2aa722','ПОСТРОЕННЫЕ ДОМА\r\nСТРОИМ ДОМА С 1997 ГОДА \rКОМПАНИЯ ИСКОНИ – МОСКОВСКОЕ ПРЕДСТАВИТЕЛЬСТВО КАЗАНСКОЙ СТРОИТЕЛЬНОЙ КОМПАНИИ КЕДР, РАБОТАЮЩЕЙ НА РЫНКЕ С 1997 ГОДА. ЗА ЭТО ВРЕМЯ НАМИ БЫЛО ПОСТРОЕНО БОЛЕЕ 500 ДОМОВ ИЗ БРУСА РАЗНЫХ ПОРОД ДЕРЕВА (КЕДР, ЕЛЬ, СОСНА). СЕГОДНЯ МЫ СТРОИМ ДОМА ИЗ КАЧЕСТВЕННОЙ ДРЕВЕСИНЫ СРЕДНЕЙ ПОЛОСЫ. ПРИ ПРОИЗВОДСТВЕ МЫ ИСПОЛЬЗУЕМ ТОЛЬКО ЭКОЛОГИЧЕСКИ ЧИСТЫЕ МАТЕРИАЛЫ, УНИКАЛЬНЫЕ СОБСТВЕННЫЕ И ПЕРЕДОВЫЕ НЕМЕЦКИЕ ТЕХНОЛОГИИ.\nВИДЕО-ОБЗОР ТИПОВОГО ПРОЕКТА ДОМ УДАЧА+\nПЛИТНЫЙ ФУНДАМЕНТ ДЛЯ ДЕРЕВЯННОГО ДОМА\nНАШИ ЛУЧШИЕ ИНДИВИДУАЛЬНЫЕ ПРОЕКТЫ \rКОД PHP\r\"> \nИНДИВИДУАЛЬНЫЙ ПРОЕКТ ДОМА \rРЕСПУБЛИКА ТАТАРСТАН \rО ПРОЕКТЕ:\rДВУХЭТАЖНЫЙ ЖИЛОЙ ДОМ ПО ИНДИВИДУАЛЬНОМУ ПРОЕКТУ. ОБЩАЯ ПЛОЩАДЬ ДОМА: 190 М2. ГОД ПОСТРОЙКИ 2018.\rДОМ ВЫПОЛНЕН ИЗ КЛЕЕНОГО БРУСА 180 (ВЫСОТА) Х 200 ММ\rСОВРЕМЕННЫЙ ПРОСТОРНЫЙ ДОМ. ШИКАРНАЯ ГОСТИНАЯ С КАМИНОМ И ВЫСОТОЙ ПОТОЛКОВ БОЛЕЕ 4-Х МЕТРОВ ИМЕЕТ БОЛЬШИЕ ПАНОРАМНЫЕ ОКНА.\rВ ЭТОМ ПРОЕКТЕ, В ЕГО РЕАЛИЗАЦИИ УДАЛОСЬ СОВМЕСТИТЬ СВЯЗЬ ВРЕМЕН, ГРАНДИОЗНЫЙ ЗАМЫСЕЛ И ЕДИНЕНИЕ ВЕЛИКОГО И МАЛОГО. МНОГО СВЕТА, МНОГО ТЕПЛА. ВСЕ ВОКРУГ ГОВОРИТ ОБ УСПЕШНОСТИ И УМЕНИИ ДОСТИГАТЬ ПОСТАВЛЕННЫХ РУБЕЖЕЙ.\nКОД PHP\r\"> \nИНДИВИДУАЛЬНЫЙ ПРОЕКТ ДОМА \rРЕСПУБЛИКА ТАТАРСТАН \rО ПРОЕКТЕ:\rДВУХЭТАЖНЫЙ ЖИЛОЙ ДОМ ПО ИНДИВИДУАЛЬНОМУ ПРОЕКТУ, ПЛОЩАДЬ ДОМА 187 М2.\rГОД ПОСТРОЙКИ 2019\rДОМ ВЫПОЛНЕН ИЗ КЛЕЕНОГО БРУСА 180 Х 200 ММ\rОРИГИНАЛЬНАЯ ПЛАНИРОВКА, НЕСТАНДАРТНОЕ РЕШЕНИЕ УГЛОВОЙ КОМНАТЫ, ВЫПОЛНЕННОЕ ПО ТИПУ КРЕМЛЕВСКОЙ ШЕСТИСТЕННОЙ БАШНИ ПРИДАЮТ ДОМУ ОСОБЫЙ КОЛОРИТ.\rЭТОТ ПРОЕКТ – ВОПЛОЩЕНИЕ ДЕТСКОЙ МЕЧТЫ ДЛЯ СВОИХ ДЕТЕЙ. А КОГДА ЧЕЛОВЕК ВЫРАСТАЕТ В АТМОСФЕРЕ СКАЗКИ, В АТМОСФЕРЕ СЧАСТЬЯ, ТОЛЬКО ТОГДА ОН СМОЖЕТ ПОКОРИТЬ ЛЮБЫЕ ВЕРШИНЫ.\nИНДИВИДУАЛЬНЫЙ ПРОЕКТ ДОМА \rРЕСПУБЛИКА ТАТАРСТАН \rО ПРОЕКТЕ:\rДВУХЭТАЖНЫЙ ЖИЛОЙ ДОМ ПО ПРОЕКТУ «АЗАЛИЯ-ЛЮКС». ОБЩАЯ ПЛОЩАДЬ ДОМА: 207 М2. \rГОД ПОСТРОЙКИ 2012.\rДОМ ВЫПОЛНЕН ИЗ КЛЕЕНОГО БРУСА 180 (ВЫСОТА) Х 220 ММ\rДОМ ПОСТРОЕН ДЛЯ МНОГОДЕТНОЙ СЕМЬИ. КОМНАТЫ ВЕРХНЕГО ЭТАЖА ИМЕЮТ МАНСАРДНЫЕ ОКНА. ВНУТРЕННИЕ ПЛАНИРОВКИ ОЧЕНЬ УДАЧНО И ГАРМОНИЧНО ПРОДУМАНЫ. НЕТ НИЧЕГО ЛИШЛЕГО, НИКАКИХ ИЗЛИШЕСТВ. ВСЕ ОЧЕНЬ ФУНКЦИОНАЛЬНО, ПРАВИЛЬНО. КАЖДАЯ ВЕЩЬ – НА СВОЕМ МЕСТЕ, КАЖДОЕ ПОМЕЩЕНИЕ ТАМ, ГДЕ ЕМУ СЛЕДУЕТ БЫТЬ, КАЖДОЕ ПРОСТРАНСТВО ТАКОЕ, КАК НУЖНО. ЭТОТ ДОМ – КАК РАЗ ТОТ СЛУЧАЙ, КОГДА ВСЕ ЕСТЕСТВЕННО И ПРОДУМАНО.\nИНДИВИДУАЛЬНЫЙ ПРОЕКТ ДОМА \rМОСКОВСКАЯ ОБЛАСТЬ \rО ПРОЕКТЕ:\rДВУХЭТАЖНЫЙ ДАЧНЫЙ ДОМ ПО ПРОЕКТУ «УДАЧА ПЛЮС». ОБЩАЯ ПЛОЩАДЬ ДОМА 113 М2.\rГОД ПОСТОРОЙКИ 2018.\rДОМ ВЫПОЛНЕН ИЗ УТЕПЛЕННОГО КЛЕЕНОГО БРУСА 180 Х 200 ММ.\rИДЕАЛЬНОЕ ДАЧНОЕ РЕШЕНИЕ ДЛЯ МОЛОДОЙ СЕМЬИ. ВСЕМ ЕСТЬ МЕСТО, И РОДИТЕЛЯМ И ДЕТЯМ, И ГОСТЯМ. В ЭТОМ ДОМЕ МОЖНО КОМФОРТНО ПРОВОДИТЬ И ТЕПЛЫЕ ЛЕТНИЕ ВЕЧЕРА, И ХОЛОДНЫЕ ЗИМНИЕ НОЧИ. ЛЕТОМ БУДЕТ ПРОХЛАДНО, ЗИМОЙ – ТЕПЛО И ТИХО.\nКОД PHP\r\"> \nИНДИВИДУАЛЬНЫЙ ПРОЕКТ ДОМА \rРЕСПУБЛИКА ТАТАРСТАН \rО ПРОЕКТЕ:\rДВУХЭТАЖНЫЙ ЖИЛОЙ ДОМ ПО ИНДИВИДУАЛЬНОМУ ПРОЕКТУ. ОБЩАЯ ПЛОЩАДЬ ДОМА: 180 М2. ГОД ПОСТРОЙКИ 2018.\rДОМ ВЫПОЛНЕН ИЗ КЛЕЕНОГО БРУСА 180 (ВЫСОТА) Х 240 ММ\rОДНОВРЕМЕННО БЫЛИ ПОСТРОЕНЫ БАНЯ И ЛЕТНЯЯ КУХНЯ (КЛЕЕНЫЙ БРУС 180 Х 160 ММ)\rЭТОТ ДОМ ДЛЯ МОЛОДОЙ СЕМЬИ, НА МОМЕНТ ПОСТРОЙКИ БЫЛ ОДИН МАЛЕНЬКИЙ РЕБЕНОК, ДЛЯ КОТОРОГО РОДИТЕЛЯМИ СОЗДАНЫ ВСЕ УСЛОВИЯ ДЛЯ ГАРМОНИЧНОГО РАЗВИТИЯ. ОЧЕВИДНО, ЧТО НЕДАЛЕК ТОТ ДЕНЬ, КОГДА У ЭТОГО РЕБЕНКА ПОЯВЯТСЯ МЛАДШИЕ БРАТЬЯ ИЛИ СЕСТРЫ: ПРОСТРАНСТВА ВСЕМ ХВАТИТ.\nИНДИВИДУАЛЬНЫЙ ПРОЕКТ ДОМА \rРЕСПУБЛИКА ТАТАРСТАН \rО ПРОЕКТЕ:\rОПИСАНИЕ: ДВУХЭТАЖНЫЙ ЖИЛОЙ ДОМ ПО ИНДИВИДУАЛЬНОМУ ПРОЕКТУ. ОБЩАЯ ПЛОЩАДЬ ДОМА: 200 М2. ГОД ПОСТРОЙКИ 2017.\rДОМ ВЫПОЛНЕН ИЗ КЛЕЕНОГО БРУСА 180 (ВЫСОТА) Х 160 ММ\rДОМ ПОСТРОЕН НА ЖИВОПИСНОМ БЕРЕГУ ВОДОЕМА НА СКЛОНЕ. В ПРОЕКТЕ УДАЛОСЬ С МАКСИМАЛЬНОЙ ЭФФЕКТИВНОСТЬЮ ИСПОЛЬЗОВАТЬ ОСОБЕННОСТИ ПРИРОДНОГО ЛАНДШАФТА. \rПЛАНИРОВКА УЧАСТКА И СОПУТСТВУЮЩИЕ ПОСТРОЙКИ ДЕЛАЮТ ЭТОТ ДОМ ЦЕНТРОМ МАЛЕНЬКОГО УЮТНОГО МИРА НА БЕРЕГУ ВЕЛИКОЙ РУССКОЙ РЕКИ. ВСЕ ВОКРУГ КАК БЫ ГОВОРИТ О ТОМ, ЧТО НАШ МИР СОЗДАН ДЛЯ СЧАСТЬЯ.\nИНДИВИДУАЛЬНЫЙ ПРОЕКТ ДОМА \rРЕСПУБЛИКА ТАТАРСТАН \rО ПРОЕКТЕ:\rДВУХЭТАЖНЫЙ ЖИЛОЙ ДОМ ПО ИНДИВИДУАЛЬНОМУ ПРОЕКТУ. ОБЩАЯ ПЛОЩАДЬ ДОМА: 232 М2. ГОД ПОСТРОЙКИ 2009.\rДОМ ВЫПОЛНЕН ИЗ КЛЕЕНОГО БРУСА 140 (ВЫСОТА) Х 200 ММ\rГОДОМ ПОЗЖЕ БЫЛИ ПОСТРОЕНЫ БАНЯ (КЛЕЕНЫЙ БРУС 140 Х 160 ММ)\rИ ЛЕТНЯЯ КУХНЯ (КЛЕЕНЫЙ БРУС 140 Х 129 ММ)\rДОМ БЫЛ ПОСТРОЕН ДЛЯ БОЛЬШОЙ СЕМЬИ. ВМЕСТИТЕЛЬНАЯ ГОСТИНАЯ ПОЗВОЛЯЕТ СОБИРАТЬСЯ НА СЕМЕЙНЫЕ ТОРЖЕСТВА БОЛЬШОМУ ЧИСЛУ ГОСТЕЙ. БЫЛИ ПРАЗДНИКИ, КОГДА СОБИРАЛОСЬ БОЛЕЕ 50 ЧЕЛОВЕК. В ДНИ ОБЫЧНЫЕ У КАЖДОГО ЧЛЕНА СЕМЬИ ЕСТЬ СВОЙ УЮТНЫЙ УГОЛОК, ДЕТЯМ МОЖНО СПОКОЙНО ДЕЛАТЬ УРОКИ, СТУДЕНТАМ – СВОИ ПРОЕКТЫ, А ГЛАВЕ СЕМЕЙСТВА – ОТДЫХАТЬ И НАСЛАЖДАТЬСЯ СЕМЕЙНЫМ СЧАСТЬЕМ.\r\n'),(166,'ad8ebf8a287cb85eec4d643a9672cd45','ДОМА ИЗ БРУСА ПОД КЛЮЧ\r\nНАША КОМПАНИЯ ЗАНИМАЕТСЯ СТРОИТЕЛЬСТВОМ ДОМОВ ИЗ БРУСА ПОД КЛЮЧ. У НАС СВОЕ ПРОИЗВОДСТВО КЛЕЕНОГО БРУСА И МЫ ЗАНИМАЕМСЯ ЭТИМ УЖЕ БОЛЬШЕ 20 ЛЕТ.\nМНОГИЕ СЧИТАЮТ, ЧТО ПОСТРОИТЬ СВОЙ ДОМ – ЭТО СЛОЖНАЯ И ТРУДНОВЫПОЛНИМАЯ ЗАДАЧА. БОЛЬШИМ ОПАСЕНИЕМ БОЛЬШИНСТВА ЗАКАЗЧИКОВ ЯВЛЯЕТСЯ ТО, ЧТО БОЛЬШИНСТВО КОМПАНИЙ ИСПОЛЬЗУЮТ ПРИ СТРОИТЕЛЬСТВЕ САМЫЕ ДЕШЕВЫЕ МАТЕРИАЛЫ И НЕ ПОКАЗЫВАЮТ ОКОНЧАТЕЛЬНУЮ СМЕТУ.\nМЫ СРАЗУ ПОШЛИ ПО ДРУГОМУ ПУТИ И УКАЗЫВАЕМ СТОИМОСТЬ СТРОИТЕЛЬСТВА ПОД КЛЮЧ. НАШИ СМЕТЫ И ЦЕНЫ НА ДЕРЕВЯННЫЕ ДОМА ОТЛИЧАЮТСЯ ПРОЗРАЧНОСТЬЮ И ТОЧНОСТЬЮ РАСЧЕТОВ. В СМЕТАХ МЫ ПОКАЗЫВАЕМ ТИПЫ ПРИМЕНЯЕМЫХ МАТЕРИАЛОВ И ИХ ОБЪЕМОВ. ПРИ ЭТОМ ПО ЧАСТИ ПОЗИЦИЙ МЫ МОЖЕМ РАБОТАТЬ НА ВАШИХ СТРОИТЕЛЬНЫХ МАТЕРИАЛАХ, НА ЧТО МНОГИЕ СТРОИТЕЛЬНЫХ КОМПАНИИ ИДУТ С БОЛЬШОЙ НЕОХОТОЙ!\nТАКЖЕ ВАЖНЫМ НАШИМ ПРЕИМУЩЕСТВОМ ЯВЛЯЕТСЯ СОБСТВЕННОЕ ПРОИЗВОДСТВО, ТО ЕСТЬ ЗАКЛЮЧАЯ С НАМИ КОНТРАКТ НА СТРОИТЕЛЬСТВО ДЕРЕВЯННОГО ДОМА ВЫ МОЖЕТЕ КУПИТЬ КЛЕЕНЫЙ БРУС ОТ ПРОИЗВОДИТЕЛЯ, МЫ ОТВЕЧАЕМ ЗА КАЧЕСТВО И ДАЕМ РАСШИРЕННЫЕ ГАРАНТИИ.\nВИДЕО-ОБЗОР ТИПОВОГО ПРОЕКТА УДАЧА+\nОПИСАНИЕ ТЕХНОЛОГИИ УТЕПЛЕННОГО КЛЕЕНОГО БРУСА\nПРОЕКТЫ ДОМОВ \rСТРОИТЕЛЬСТВО ДЕРЕВЯНННОГО ДОМА СОСТОИТ ИЗ НЕСКОЛЬКИХ ЭТАПОВ:\r- ВЫБОР И УТВЕРЖДЕНИЕ ПРОЕКТА ДОМА;\r- ВЫБОР И МОТАЖ ФУНДАМЕНТА;\r- ВОЗВЕДЕНИЕ ДОМОКОМПЛЕКТА;\r- МОНТАЖ БАЛОК ПЕРЕКРЫТИЙ ПОЛА И ПОТОЛКА;\r- МОНТАЖ СТРОПИЛЬНОЙ СИСТЕМЫ, УТЕПЛЕНИЕ КРОВЛИ;\r- ВНУТРЕННЯЯ И ВНЕШНЯЯ ОТДЕЛКА;\r- ВНУТРЕННИЕ И ВНЕШНИЕ ИНЖЕНЕРНЫЕ КОММУНИКАЦИИ.\rНАША КОМПАНИЯ ОСУЩЕСТВЛЯЕТ ПОЛНЫЙ ЦИКЛ РАБОТ ПО СТРОИТЕЛЬСТВУ БРУСОВОГО ДОМА ПОД КЛЮЧ.\nДОМА ИЗ БРУСА МОЖНО РАЗДЕЛИТЬ ПО СЛЕДУЮЩИМ КРИТЕРИЯМ:\r- РАЗМЕР ДОМА И ПЛОЩАДЬ ПЯТНА ЗАСТРОЙКИ;\r- ЭТАЖНОСТЬ;\r- НАЛИЧИЯ ЦОКОЛЯ И МАНСАРДНОГО ЭТАЖА;\r- НАЗНАЧЕНИЕ ДОМА (ДАЧНЫЙ ДЕРЕВЯННЫЙ ДОМ ИЛИ ДОМ ДЛЯ ПОСТОЯННОГО ПРОЖИВАНИЯ).\rПРЕИМУЩЕСТВОМ ДЕРЕВЯННОГО ДОМА ИЗ КЛЕЕНОГО БРУСА ЯВЛЯЕТСЯ ВЫСОКАЯ ЭКОЛОГИЧНОСТЬ ТАКОГО ДОМА. ИСКОНИ ЛЮДИ СТРОИЛИ И ЖИЛИ В ДОМАХ ИЗ ДЕРЕВА. ТАКЖЕ ДЕРЕВЯННЫЕ ДОМА ОЧЕНЬ ТЕПЛЫЕ И УЮТНЫЕ. ПРЕДСТАВЬТЕ СЕБЕ СВЕЖИЙ ЗАПАХ ВЫПЕЧКИ, КАК ОН СОЧЕТАЕТСЯ С ПРИЯТНЫМИ ХВОЙНЫМИ АРОМАТАМИ ДОМА ИЗ СОСНЫ ИЛИ КЕДРА! ВЫ ПОЛЮБИТЕ ТАКОЙ ДОМ С ПЕРВОГО ВЗГЛЯДА, С ПЕРВОГО ВЗДОХА, ОБРЕТЕТЕ ЗДЕСЬ УЮТ, СПОКОЙСТВИЕ И КРЕПКОЕ СЕМЕЙНОЕ СЧАСТЬЕ!\nТАК В НАШЕЙ КОМПАНИИ ВЫ МОЖЕТЕ ПОСТРОИТЬ ДОМ ИЗ УТЕПЛЕННОГО КЛЕЕНОГО БРУСА (ЕГО ЕЩЕ НАЗЫВАЮТ «ПОЛЫЙ» КЛЕЕНЫЙ БРУС). В КАЧЕСТВЕ УТЕПЛИТЕЛЯ МЫ ИСПОЛЬЗУЕМ ЭКОЛОГИЧЕСКИ ЧИСТЫЕ МАТЕРИАЛЫ, КОТОРЫЕ ПОВТОРЯЮТ СТРУКТУРУ ДЕРЕВА. ТАКОЙ ДОМ В 3 РАЗА ТЕПЛЕЕ И ТИШЕ ДОМА ИЗ ОБЫЧНОГО БРУСА. У НАС ТАКЖЕ ЕСТЬ ПРЕМИАЛЬНЫЙ УТЕПЛИТЕЛЬ – КОРА ПРОБКОВОГО ДОМА. НАМ ЕСТЬ ЧЕМ ВАС УДИВИТЬ! УВЕРЕНЫ, ЧТО ВЫ ЕЩЕ НИКОГДА НЕ БЫЛИ В ДОМЕ ИЗ ПРОБКОВОГО БРУСА!\nНАПИШИТЕ ИЛИ ПОЗВОНИТЕ НАМ И МЫ С УДОВОЛЬСТВИЕМ ОТВЕТИМ НА ВСЕ ВОПРОСЫ О ДЕРЕВЯННОМ ДОМЕ И ПОМОЖЕМ ПОДОБРАТЬ ПОДХОДЯЩИЙ ПРОЕКТ ДОМА ИЗ БРУСА.\r\n'),(32,'e2302d38568747722cfae5bf695030d5','КАТАЛОГ ДОМОВ\r\nПРОЕКТЫ ДОМОВ ИЗ БРУСА\rНА ДАННОЙ СТРАНИЦЕ ВЫ МОЖЕТЕ ОЗНАКОМИТЬСЯ С НАШИМИ ТИПОВЫМИ ПРОЕКТАМИ ДОМОВ ИЗ БРУСА.\nЦЕНА ДЕРЕВЯННОГО ДОМА ИЗ БРУСА ЗАВИСИТ ОТ НЕСКОЛЬКИХ ФАКТОРОВ:\n- \rПЛОЩАДЬ ДОМА\r. КАК ПРАВИЛО ОПРЕДЕЛЯЕТСЯ ВОЗМОЖНОСТЯМИ ВАШЕГО УЧАСТКА. ЧАСТО ТАКЖЕ ЗАКАЗЧИКИ ВЫБИРАЮТ КОНКРЕТНЫЙ ПРОЕКТ ДОМА, КОТОРЫЙ ИМ НРАВИТСЯ И УЖЕ С УЧЕТОМ ЭТОГО ПОКУПАЮТ УЧАСТОК;\n- \rТИП ФУНДАМЕНТА\r. МЫ РЕКОМЕНДУЕМ ПЕРЕД СТРОИТЕЛЬСТВОМ ДОМА ОСУЩЕСТВЛЯТЬ ГЕОЛОГИЧЕСКИЕ ИССЛЕДОВАНИЯ ПОЧВЫ, НА ОСНОВАНИИ КОТОРЫХ ДАЮТСЯ РЕКОМЕНДАЦИЮ К ТИПУ ФУНДАМЕНТА;\n- \rКОЛИЧЕСТВО ЭТАЖЕЙ\r, НАЛИЧИЯ ЦОКОЛЬНОГО ЭТАЖА. ЗДЕСЬ СЛЕДУЕТ ОТМЕТИТЬ, ЧТО СТОИМОСТЬ КВАДРАТНОГО МЕТРА ОДНОЭТАЖНОГО ДОМА БУДЕТ ВЫШЕ, ЧЕМ СТОИМОСТЬ ДВУХЭТАЖНОГО ДОМА ИЛИ ДОМА С ЦОКОЛЕМ;\n- \rТИП БРУСА\r. В КАЧЕСТВЕ ДОМОКОМПЛЕКТА МОЖНО ИСПОЛЬЗОВАТЬ ОЦИЛИНДРОВАННОЕ БРЕВНО, ПРОФИЛИРОВАННЫЙ БРУС ИЛИ КЛЕЕНЫЙ БРУС. ТАКЖЕ МЫ МОЖЕМ ПРЕДЛОЖИТЬ ДЛЯ ВАС НАШУ ИННОВАЦИОННУЮ ТЕХНОЛОГИЙ – УТЕПЛЕННЫЙ БРУС С ЭКОЛОГИЧЕСКИ ЧИСТЫМИ УТЕПЛИТЕЛЯМИ. ТАКОЙ БРУС В 3 РАЗА ТЕПЛЕЕ И ТИШЕ ОБЫЧНОГО КЛЕЕНОГО БРУСА;\n- \rПРИМЕНЯЕМЫХ МАТЕРИАЛОВ КРОВЛИ, ВНУТРЕННЕЙ ОТДЕЛКИ, ИНЖЕНЕРНЫХ КОММУНИКАЦИЙ\r. МЫ СОТРУДНИЧАЕМ С САМЫМИ КРУПНЫМИ И ПЕРЕДОВЫМИ КОМПАНИЯМИ ПО ПРОИЗВОДСТВУ СТРОИТЕЛЬНЫХ МАТЕРИАЛОВ ДЛЯ ДОМОВ ИЖС;\n- \rНАЛИЧИЯ МАНСАРДЫ, НАВЕСА ДЛЯ АВТО, ВЕРАНДЫ\rИ ДРУГИХ ВЕЩЕЙ.\nУ НАС В НАЛИЧИИ БОЛЕЕ 30 ПРОЕКТОВ ДЕРЕВЯННЫХ ДОМОВ С РАЗЛИЧНЫМИ РЕШЕНИЯМИ, КОТОРЫЕ МОГУТ УДОВЛЕТВОРИТЬ БОЛЬШИНСТВО ВАШИХ ПОЖЕЛАНИЙ. ТАКЖЕ МЫ МОЖЕМ ПОСТРОИТЬ ДОМ ПО ВАШЕМУ ПРОЕКТУ.\nНАША ПОЛИТИКА – ЭТО ОТКРЫТЫЕ СМЕТЫ И КОСМИЧЕСКАЯ ТОЧНОСТЬ РАСЧЕТОВ. ЗДЕСЬ ВЫ МОЖЕТЕ УЗНАТЬ СТОИМОСТЬ ДЕРЕВЯННОГО ДОМА ИЗ БРУСА ПОД КЛЮЧ, А ТАКЖЕ ОЦЕНИТЬ ЦЕНУ КАЖДОГО ЭТАПА СТРОИТЕЛЬСТВА. СТОИТ ОТМЕТИТЬ, ЧТО НАШИМ ПРЕИМУЩЕСТВОМ ЯВЛЯЕТСЯ ГАРАНТИЯ, ЧТО СТОИМОСТЬ ПРОЕКТА НЕ ВЫРАСТЕТ НА ЭТАПЕ СТРОЙКИ. ЗАКЛЮЧАЯ С НАМИ ДОГОВОР, ВЫ МОЖЕТЕ БЫТЬ СПОКОЙНЫ И УВЕРЕНЫ, ЧТО ЦЕНА ВАШЕГО ДОМА НЕ ВЫРАСТЕТ НИ НА РУБЛЬ ПОСЛЕ ЗАКЛЮЧЕНИЯ С НАМИ ДОГОВОРА НА СТРОИТЕЛЬСТВА ДОМА ВАШЕЙ МЕЧТЫ!\nБРУСОВЫЕ ДОМА ОТЛИЧАЮТСЯ СВОЕЙ УНИВЕРСАЛЬНОСТЬЮ. ТАКОМ ДОМ НАША КОМПАНИЯ МОЖЕТ ВЫПОЛНИТЬ В ЛЮБОМ СТИЛЕ:\r- АЛЬПИЙСКИЙ ДОМ В СТИЛЕ ШАЛЕ, В КОТОРОМ ВЫ МОЖЕТЕ ОКУНУТЬСЯ В АТМОСФЕРУ И УЮТ ГОРНЫХ КУРОРТОВ;\r- КЛАССИЧЕСКИЙ СКАНДИНАВСКИЙ СТИЛЬ, КАК ПРАВИЛО ЭТО ОЧЕНЬ ТЕПЛЫЕ И УЮТНЫЕ ДОМА, В КОТОРЫХ ЭФФЕКТИВНО ИСПОЛЬЗУЕТСЯ КАЖДЫЙ КВАДРАТНЫЙ МЕТР ПЛОЩАДИ;\r- СОВРЕМЕННЫЙ ШВЕДСКИЙ СТИЛЬ – БАРНХАУС ИДЕАЛЬНО ПОДОЙДЕТ ДЛЯ ТЕХ, КТО СЛЕДИТ ЗА МОДОЙ И СЛЕДИТ ЗА ТЕНДЕНЦИЯМИ;\r- МОДЕРН И ХАЙТЕК ДЛЯ ТЕХ, КТО УЖЕ СЕГОДНЯ СМОТРИТ В ЗАВТРАШНИЙ ДЕНЬ ИМЕННО МЫ ПОМОЖЕМ ВНЕДРИТЬ КЛАССИЧЕСКИЕ ТЕХНОЛОГИИ ДЕРЕВЯННОГО ДОМОСТРОЕНИЕ В ИННОВАЦИОННЫЕ РЕШЕНИЯ И СДЕЛАТЬ ПРОЕКТ ВАШЕГО ДОМА ИЗ БРУСА УНИКАЛЬНЫМ!\r- КЛАССИЧЕСКИЙ РУССКИЙ СТИЛЬ, ДОМ, В КОТОРОМ ВЫ ПРОВЕЛИ ДЕТСКИЕ ГОДЫ И СМОЖЕТЕ ОКУНУТЬСЯ В ПРИЯТНЫЕ ВОСПОМИНАНИЯ.\rСТОИМОСТЬ ПРОЕКТА ДЕРЕВЯННОГО ДОМА ТАКЖЕ ЗАВИСИТ ОТ ИНДИВИДУАЛЬНЫХ ПОЖЕЛАНИЙ ЗАКАЗЧИКОВ, ЧАСТО НЕТРИВИАЛЬНЫХ И ИНТЕРЕСНЫХ РЕШЕНИЙ.\rПОЗВОНИТЕ НАМ ИЛИ ОСТАВЬТЕ ЗАЯВКУ И НАШ МЕНЕДЖЕР ПОМОЖЕТ ПОДОБРАТЬ ЛУЧШИЙ ДОМ ВАШЕЙ МЕЧТЫ, ПРОКОНСУЛЬТИРУЕТ ПО ТЕХНОЛОГИЯМ СТРОИТЕЛЬСТВА И СОРИЕНТИРУЕТ ПО ЦЕНЕ ТИПОВЫХ И ИНДИВИДУАЛЬНЫХ ПРОЕКТОВ ДОМОВ ИЗ БРУСА.\r\n'),(173,'50916e7046e11bd27ef3dd86c09a3831','ТИПЫ ФУНДАМЕНТОВ И ИХ ОТЛИЧИЯ\r\nСЕГОДНЯ МЫ ХОТИМ РАССКАЗАТЬ КАКИЕ ТИПЫ ФУНДАМЕНТОВ ИСПОЛЬЗУЕМ ПРИ СТРОИТЕЛЬСТВЕ ДОМОВ ИЗ КЛЕЕНОГО БРУСА СВОЕГО ПРОИЗВОДСТВА.\r\n\rЗДРАВСТВУЙТЕ! СЕГОДНЯ МЫ ХОТИМ РАССКАЗАТЬ КАКИЕ ТИПЫ ФУНДАМЕНТОВ ИСПОЛЬЗУЕМ ПРИ СТРОИТЕЛЬСТВЕ ДОМОВ ИЗ КЛЕЕНОГО БРУСА СВОЕГО ПРОИЗВОДСТВА.\rФУНКЦИИ ФУНДАМЕНТА.\rДЕЙСТВИТЕЛЬНО, ЗАЧЕМ ВООБЩЕ НУЖЕН ФУНДАМЕНТ?\rФУНДАМЕНТ – ЭТО ОСНОВА ЛЮБОГО ЗДАНИЯ И СООРУЖЕНИЯ. ФУНДАМЕНТ ПЕРЕДАЕТ НАГРУЗКУ СТЕН И ВСЕГО ЗДАНИЯ И РАВНОМЕРНО РАСПРЕДЕЛЯЕТ ЕЕ НА ГРУНТ ТАКИМ ОБРАЗОМ, ЧТОБЫ СТРОЕНИЕ НАХОДИЛОСЬ НА ПРОЕКТНОМ УРОВНЕ НАД ЗЕМЛЕЙ, ТО ЕСТЬ, ПРОЩЕ ГОВОРЯ, НЕ ПРОВАЛИЛОСЬ В ПОЧВУ (НЕ КАСАЕТСЯ ЦОКОЛЬНОГО ЭТАЖА). \rОСНОВНЫЕ ФУНКЦИИ ФУНДАМЕНТА:\r1) ЯВЛЯЕТСЯ ОСНОВОЙ, ОДНИМ ИЗ САМЫХ ВАЖНЫХ ЭЛЕМЕНТОВ ЛЮБОГО ДОМА;\r2) НЕСЕТ НАГРУЗКУ ОТ СТЕН, ОТВЕЧАЕТ ЗА УСТОЙЧИВОСТЬ И НЕПОДВИЖНОСТЬ ДОМА;\r3) ТЕПЛОЗАЩИТА (СПРОСИТЬ У ТАГИРА ФОРМУЛИРОВКУ)\r4) ЗАЩИЩАЕТ СТЕНЫ ДОМА ОТ ВЛАГИ И ПЕРЕМЕН ТЕМПЕРАТУРЫ, УВЕЛИЧИВАЕТ СРОК СЛУЖБЫ ВАШЕГО ДОМА;\r5) ПРЕДОХРАНЯЕТ ВАШ ДОМ ОТ ПРОНИКНОВЕНИЯ ЖИВОТНЫХ И НАСЕКОМЫХ;\r6) ЗАЩИЩАЕТ ДОМ ОТ КОЛЕБАНИЙ ПОЧВЫ (НЕАКТУАЛЬНО ДЛЯ СРЕДНЕЙ ПОЛОСЫ);\r7) ПРЕДОХРАНЯЕТ ДОМ ОТ ВРЕДНЫХ ИСПАРЕНИЙ ПОЧВЫ.\rОСНОВНЫЕ ТИПЫ ФУНДАМЕНТОВ.\rДАВАЙТЕ РАССМОТРИМ ОСНОВНЫЕ ТИПЫ ФУНДАМЕНТОВ, ИХ ПРЕИМУЩЕСТВА И НЕДОСТАТКИ КОТОРЫЕ НАША КОМПАНИЯ РЕКОМЕНДУЕТ ПРИ СТРОИТЕЛЬСТВЕ СВОЕГО ДОМА:\r1) СВАЙНЫЙ ФУНДАМЕНТ. ПРИ ЭТОМ ЕСТЬ 2 ТИПА СВАЙ: ЖЕЛЕЗНЫЕ ВИНТОВЫЕ СВАИ И ЖЕЛЕЗОБЕТОННЫЕ ЗАБИВНЫЕ СВАИ. ПРЕИМУЩЕСТВОМ ТАКОГО ФУНДАМЕНТА ЯВЛЯЕТСЯ СТОИМОСТЬ, ЭТО НАИБОЛЕЕ БЮДЖЕТНЫЙ ВАРИАНТ. ХОРОШО ПОДХОДИТ ДЛЯ ДОМОВ НЕПОСТОЯННОГО ПРОЖИВАНИЯ (ДАЧИ, ЛЕТНИХ ДОМИКОВ), ДЛЯ НЕБОЛЬШИХ ПОСТРОЕК, ТАКИХ КАК БАНЯ, НАВЕСЫ, ГАРАЖ ДЛЯ АВТО И ПР. ОСНОВНЫМ НЕДОСТАТКОМ ТАКОГО ФУНДАМЕНТА ЯВЛЯЕТСЯ ТО, ЧТО ПРИ СТРОИТЕЛЬСТВЕ ДОМА ПОТРЕБУЕТСЯ ДЕЛАТЬ РАБОТЫ ПО МОНТАЖУ БАЛОК ПЕРЕКРЫТИЙ ПЕРВОГО ЭТАЖА, А ТАКЖЕ РАБОТЫ ПО УТЕПЛЕНИЮ ПОЛА ПЕРВОГО ЭТАЖА И ЗАКРЫВАТЬ ЦОКОЛЬ. ТАКЖЕ ЖИТЕЛИ ДОМОВ НА ТАКОМ ФУНДАМЕНТЕ ЧАСТО ЖАЛУЮТСЯ НА ГРЫЗУНОВ. НА ПЕРВЫЙ ВЗГЛЯД ТАКОЙ ТИП ФУНДАМЕНТА (ОСОБЕННО ДЛЯ ВИНТОВЫХ СВАЙ) ПРЕДСТАВЛЯЕТСЯ НЕ САМЫМ НАДЕЖНЫМ (МОЙ ОТЕЦ НАЗЫВАЕТ ТАКИЕ ДОМА «ДОМИК НА КУРЬИХ НОЖКАХ), НО ТАКИЕ ФУНДАМЕНТЫ СЕЙЧАС АКТИВНО ПРИМЕНЯЮТСЯ ПРИ МАЛОЭТАЖНОМ СТРОИТЕЛЬСТВЕ. СТОИТ ОТМЕТИТЬ, ЧТО В ОПРЕДЕЛЕННЫХ СЛУЧАЯХ ПРИ БОЛОТИСТЫХ ПОЧВАХ И ВЫСОКИХ ГРУНТОВЫХ ВОДАХ СВАИ – ЭТО ЕДИНСТВЕННЫЙ ФУНДАМЕНТ, КОТОРЫЙ МОЖНО ПРИМЕНЯТЬ ДЛЯ СТРОИТЕЛЬСТВА СВОЕГО ДОМА, ПРИ ЭТОМ ГЛУБИНА СВАЙ МОЖЕТ ДОХОДИТЬ ДО 10-12 МЕТРОВ.\r2) МЕЛКОЗАГЛУБЛЕННЫЙ ЛЕНТОЧНЫЙ ФУНДАМЕНТ. ПРЕДСТАВЛЯЕТ СОБОЙ БЕТОННУЮ ЛЕНТУ, КОТОРАЯ ПРОКЛАДЫВАЕТСЯ ПО ВСЕМУ ПЕРИМЕТРУ ЗДАНИЯ И ПОД НЕСУЩИМИ СТЕНАМИ. ГЛУБИНА ЗАЛЕГАНИЯ ЛЕНТЫ СОСТАВЛЯЕТ 15-20 СМ. ЛЕНТА – ХОРОШЕЕ РЕШЕНИЕ ДЛЯ НЕБОЛЬШИХ ДОМОВ. ЧТО НАЗЫВАЕТСЯ «ЦЕНА/КАЧЕСТВО». НЕДОСТАТКОМ ЛЕНТОЧНОГО ФУНДАМЕНТА ЯВЛЯЕТСЯ НЕОБХОДИМОСТЬ МОНТАЖА БАЛОК ПЕРЕКРЫТИЙ И УТЕПЛЕНИЯ ПЕРВОГО ЭТАЖА.\r3) СВАЙНО-РОСТВЕРКОВЫЙ ФУНДАМЕНТ. ЭТО КОМБИНАЦИЯ СВАЙНОГО И ЛЕНТОЧНОГО ФУНДАМЕНТА. ПРЕДСТАВЛЯЕТ СОБОЙ БЕТОННЫЕ БУРОЗАБИВНЫЕ СВАИ, СОЕДИНЕННЫЕ БЕТОННЫМ РОСТВРЕКОМ. НАДЕЖНЫЙ ТИП ФУНДАМЕНТА. БЮДЖЕТ ТАКОГО ФУНДАМЕНТА ВЫШЕ, ЧЕМ ПРОСТО СВАЙНЫЙ ИЛИ ЛЕНТОЧНЫЙ ФУНДАМЕНТ, ПРИ ЭТОМ ТАКЖЕ НЕОБХОДИМО ОСУЩЕСТВЛЯТЬ МОНТАЖ БАЛОК ПЕРЕКРЫТИЙ ПЕРВОГО ЭТАЖА И ОСУЩЕСТВЛЯТЬ РАБОТЫ ПО УТЕПЛЕНИЮ ПОЛА И ЗАЩИТЕ ОТ ГРЫЗУНОВ.\r4) МОНОЛИТНЫЙ ПЛИТНЫЙ ФУНДАМЕНТ. САМЫЙ НАДЕЖНЫЙ ВИД ФУНДАМЕНТА. ЕЩЕ ДРЕВНИЕ ЕГИПТЯНЕ ИСПОЛЬЗОВАЛИ ТАКОЙ ТИП ФУНДАМЕНТА – СТРОИТЕЛИ ПИРАМИДЫ ХЕОПСА ИСПОЛЬЗОВАЛИ В КАЧЕСТВЕ ОСНОВАНИЯ НЕВЫСОКИЙ ХОЛМ, НАВЕРХУ КОТОРОГО БЫЛА ПОЛНОСТЬЮ ОБНАЖИВШАЯСЯ СКАЛА. ОНИ ВЫРОВНЯЛИ ПОВЕРХНОСТЬ СКАЛЫ И УЛОЖИЛИ НА НЕЙ СПЛОШНОЙ СЛООЙ ИЗ ТРЕХТОННЫХ БЛОКОВ ИЗВЕСТНЯКА В ФОРМЕ КВАДРАТА СО СТОРОНОЙ 225 М. НА ЭТОЙ ПОДУШКЕ БЫЛА ВОЗВЕДЕНА ПИРАМИДА ВЕСОМ 7 МЛН. Т И ВЫСОТОЙ 144 М, ПРОСТОЯВШАЯ В ТЕЧЕНИЕ 5000 ЛЕТ БЕЗ КАКОЙ-ЛИБО ДЕФОРМАЦИИ!\rМОНОЛИТНЫЙ ФУНДАМЕНТ ПРЕДСТАВЛЯЕТ НЕ ГОТОВЫЕ БЕТОННЫЕ ПЛИТЫ, КОТОРЫЕ ПРИВОЗЯТСЯ НА УЧАСТОК (КАК МНОГИЕ ЧАСТО СПРАШИВАЮТ), А ЗАЛИВАЕТСЯ БЕТОННЫМ РАСТВОРОМ НЕПОСРЕДСТВЕННО НА МЕСТЕ ПО ИНДИВИДУАЛЬНОМУ ПРОЕКТУ С УЧЕТОМ ОСОБЕННОСТЕЙ ГРУНТА НЕПОСРЕДСТВЕННО ПОД ВАШ ДОМ.\r\n'),(174,'a71c8c59177876df18492e9530220f63','УШП С ОСТРЫМ РОСТВЕРКОМ С КОМПОЗИТНОЙ АРМАТУРОЙ\r\nДАННЫЙ ФУНДАМЕНТ ВЫПОЛНЕН ПО ДОРАБОТАННОЙ ТЕХНОЛОГИИ \"УТЕПЛЕННАЯ ШВЕДСКАЯ ПЛИТА\" И ПРЕДСТАВЛЯЕТ СОБОЙ ФУНДАМЕНТНУЮ ПЛИТУ С ОСТРЫМ УСЕЧЕННЫМ РОСТВЕРКОМ (С РЕБРАМИ ЖЕСТКОСТИ). \r\n\rЗАКАЗЧИК ВЫБРАЛ НАШ ТИПОВОЙ ПРОЕКТ ЛИЛИЯ – ЭТО ДВУХЭТАЖНЫЙ ДОМ СО ВТОРЫМ СВЕТОМ. У ЗАКАЗЧИКА БОЛЬШАЯ СЕМЬЯ И МНОГО ДРУЗЕЙ, ПОЭТОМУ ОН ВЫБРАЛ БОЛЬШОЙ ДОМ. ПЛОЩАДЬ ДОМА 194 КВАДРАТНЫХ МЕТРА + БОЛЬШАЯ ВЕРАНДА НА 35 КВАДРАТНЫХ МЕТРОВ. В ДОМЕ 6 СПАЛЕН, КУХНЯ СОВМЕЩЕННАЯ С ГОСТИНОЙ СО ВТОРЫМ СВЕТОМ И КАМИНОМ И БОЛЬШОЙ ВЕРАНДОЙ С ВИДОМ НА ЛЕС.\rВ КАЧЕСТВЕ МАТЕРИАЛА ДОМОКОМПЛЕКТА БЫЛ ВЫБРАН НАШ ИННОВАЦИОННЫЙ МАТЕРИАЛ – УТЕПЛЕННЫЙ КЛЕЕНЫЙ БРУС. В ПРОШЛОМ ВИДЕО МЫ ВАМ ПОКАЗЫВАЛИ УТЕПЛЕННЫЙ БРУС, ГДЕ В КАЧЕСТВЕ УТЕПЛИТЕЛЯ ИСПОЛЬЗУЕТСЯ ЭКОВАТА, ЗДЕСЬ ИСПОЛЬЗУЕТСЯ УТЕПЛЕННЫЙ БРУС, ГДЕ В КАЧЕСТВЕ УТЕПЛИТЕЛЯ ИСПОЛЬЗУЕТСЯ БЕЛТЕРМО – ЭТО СПРЕСОВАННАЯ РАСПУШАНЯ ДЕРЕВЕСИНА (БЕЛОРУССКИЙ УТЕПЛИТЕЛЬ).\rТАКОЙ БРУС В 3 РАЗА ТЕПЛЕЕ ОБЫЧНОГО БРУСА, А ПРИ ИСПОЛЬЗОВАНИИ ТВЕРДОЙ ФРАКЦИИ УТЕПЛИТЕЛЯ ОТСУТСТВУЮТ КАКИЕ-ЛИБО РИСКИ ПО ЕГО УСАДКЕ (ОДИН ИЗ ОСНОВНЫХ ОПАСЕНИЙ НАШИХ ЗАКАЗЧИКОВ).\rДАННЫЙ ФУНДАМЕНТ ВЫПОЛНЕН ПО ДОРАБОТАННОЙ ТЕХНОЛОГИИ \"УТЕПЛЕННАЯ ШВЕДСКАЯ ПЛИТА\" И ПРЕДСТАВЛЯЕТ СОБОЙ ФУНДАМЕНТНУЮ ПЛИТУ С ОСТРЫМ УСЕЧЕННЫМ РОСТВЕРКОМ (С РЕБРАМИ ЖЕСТКОСТИ). \rСТРОИТЕЛЬНАЯ КОМПАНИЯ ИСКОНИ РЕКОМЕНДУЕТ ПРИОРИТЕТНО ИСПОЛЬЗОВАТЬ ИМЕННО ТАКОЙ ТИП ФУНДАМЕНТА ДЛЯ ДОМОВ ИЗ КЛЕЕНОГО БРУСА, ТАК КАК ЭТО САМЫЙ ТЕПЛЫЙ, НАДЕЖНЫЙ И ДОЛГОВЕЧНЫЙ ТИП ФУНДАМЕНТА ДЛЯ ДЕРЕВЯННОГО ДОМА. ОСНОВНЫМИ ПРЕИМУЩЕСТВАМИ ТАКОГО ФУНДАМЕНТА ЯВЛЯЮТСЯ:\r1) УСЕЧЕННЫЙ РОСТВЕРК С ОСТРЫМИ КРАЯМИ СДЕЛАН ПО ПЕРИМЕТРУ ПЛИТЫ, ТО ЕСТЬ ПЛИТА С РОСТВЕРКОМ СОСТАВЛЯЮТ ЕДИНУЮ МОНОЛИТНУЮ КОНСТРУКЦИЮ;\r2) ОСТРЫЕ КРАЯ-НОЖКИ РОСТВЕРКА ПОЗВОЛЯЮТ ПЛИТЕ ВСЕЙ МАССОЙ ЖЕСТКО И ПЛОТНО \"САДИТЬСЯ\" НА ПЕСЧАНУЮ ПОДУШКУ, \"ДОГОНЯЯ\" ВОЗМОЖНУЮ УСАДКУ ПЕСКА;\r3) ЭФФЕКТИВНОЕ ИСПОЛЬЗОВАНИЕ МАТЕРИАЛОВ (ПЕСОК, АРМАТУРА, БЕТОН);\r4) ШАГ АРМАТУРЫ (30 СМ) УСТОЙЧИВ К ЛЮБЫМ ВИДАМ НАГРУЗКИ, КОСОЙ РОСТВЕРК И УЗЛЫ УСИЛИВАЮТСЯ ДОПОЛНИТЕЛЬНО;\r5) ПРИМЕНЕНИЕ КОМПОЗИТНОЙ АРМАТУРЫ ПОЗВОЛЯЕТ СДЕЛАТЬ ФУНДАМЕНТ ПРОЧНЕЕ, ДОЛГОВЕЧНЕЕ И ДЕШЕВЛЕ;\r6) САМЫЙ ТЕПЛЫЙ ТИП ФУНДАМЕНТА, НЕ НУЖНО УТЕПЛЯТЬ ПОЛ ПЕРВОГО ЭТАЖА;\r7) ЗА СЧЕТ ЭФФЕКТИВНОГО ИСПОЛЬЗОВАНИЕ МАТЕРИАЛОВ, ПРОСТОТЫ КОНСТРУКЦИИ, А ТАКЖЕ ЗА СЧЕТ ТОГО, ЧТО НЕ НУЖНО УТЕПЛЯТЬ ПОЛ ПЕРВОГО ЭТАЖА ДАННЫЙ ТИП ФУНДАМЕНТА ЧАСТО ДЕШЕВЛЕ, ЧЕМ ЛЕНТОЧНЫЙ ФУНДАМЕНТ И В РАЗЫ НАДЕЖНЕЕ, ЧЕМ СВАЙНЫЙ И ДРУГИЕ ТИПЫ ФУНДАМЕНТОВ.\rОДИН ИЗ ПРИНЦИПОВ НАШЕЙ КОМПАНИИ - ВНЕДРЕНИЕ ИННОВАЦИОННЫХ ТЕХНОЛОГИЙ В КЛАССИЧЕСКОЕ ДЕРЕВЯННОЕ ДОМОСТРОЕНИЕ. ПРИ МОНТАЖЕ ДАННОГО ФУНДАМЕНТЫ МЫ ИСПОЛЬЗОВАЛИ ИННОВАЦИОННУЮ КОМПОЗИТНУЮ АРМАТУРУ КОМПАНИИ HELP COMPOSITE, КОТОРАЯ ОБЛАДАЕТ РЯДОМ ПРЕИМУЩЕСТВ ПО СРАВНЕНИЮ С МЕТАЛЛИЧЕСКОЙ, А ИМЕННО:\r1) ВЫГОДНАЯ ЦЕНА, ДАННАЯ АРМАТУРА ДЕШЕВЛЕ;\r2) ПО ВСЕМ ХАРАКТЕРИСТИКАМ НЕ УСТУПАЕТ МЕТАЛЛИЧЕСКОЙ АРМАТУРЕ;\r3) ПО ПРОЧНОСТИ ПРЕВОСХОДИТ МЕТАЛЛИЧЕСКУЮ АРМАТУРУ;\r4) НЕ БОИТСЯ КОРРОЗИИ, БОЛЕЕ ДОЛГИЙ СРОК ЭКСПЛУАТАЦИИ;\r5) ЛЕГЧЕ И ПРОЩЕ РАБОТАТЬ;\r6) ЛУЧШИЕ ХАРАКТЕРИСТИКИ АДГЕЗИИ;\r7) МОЖЕТ ВЫЙТИ С ЗАВОДА ЛЮБОЙ ФОРМЫ.\r\n');
